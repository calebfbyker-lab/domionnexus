3) Auto-license & seal header injector

tools/atlas_license_bind.py

from __future__ import annotations
import pathlib, datetime, hashlib

HEADER = """\
# EUCELA Tri-License Â© 2025 Caleb Fedor Byker (Konev)
# Subject SHA256: 2948fbc4ba1c0d7341204908882b89134a999f3e8f77f4a6a00ce6b68770282a
# This file is bound, sealed, and attested under LICENSE.EUCELA.tri
"""

EXTS = {".py",".json",".yml",".yaml",".md",".cjs",".txt"}

def needs_header(text:str)->bool:
    return "EUCELA Tri-License" not in text.splitlines()[:5]

def seal(path: pathlib.Path)->str:
    return hashlib.sha256(path.read_bytes()).hexdigest()

def main():
    changed=0
    for p in pathlib.Path(".").rglob("*"):
        if p.is_file() and p.suffix in EXTS and "site/" not in str(p):
            t=p.read_text(encoding="utf-8", errors="ignore")
            if needs_header(t):
                if p.suffix in {".json"}:
                    # JSON comments aren't standard; keep file intact, write sidecar .seal
                    (p.with_suffix(p.suffix+".seal")).write_text(
                        f"EUCELA Tri-License Â· Subject SHA256 "
                        f"2948fbc4ba1c0d7341204908882b89134a999f3e8f77f4a6a00ce6b68770282a\n",
                        encoding="utf-8"
                    )
                else:
                    p.write_text(HEADER + t, encoding="utf-8")
                changed+=1
    # write run receipt
    stamp = pathlib.Path("chain/atlas_license_bind.receipt")
    stamp.parent.mkdir(parents=True, exist_ok=True)
    stamp.write_text(f"{datetime.datetime.utcnow().isoformat()}Z files={changed}\n", encoding="utf-8")
    print(f"âœ… license headers bound Â· files updated: {changed}")

if __name__=="__main__":
    main()


