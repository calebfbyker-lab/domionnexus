    "encrypted_seed": encrypted_seed.hex(),
    "nonce": nonce.hex(),
    "hmac_signature": hmac_signature.hex(),
    "digital_signature": signature.hex(),
    "public_key": ed25519_priv.public_key().public_bytes().hex(),
    "sacred_data_summary": sacred_data
}

