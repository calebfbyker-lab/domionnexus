import json
import os
from cryptography.hazmat.primitives.asymmetric.ed25519 import Ed25519PrivateKey
from cryptography.hazmat.primitives.ciphers.aead import AESGCM

class EternalLifethreadSeal:
    def __init__(self, creator, birthdates, lineage_keys, divine_names, lost_name_ra):
        self.creator = creator
        self.birthdates = birthdates
        self.lineage_keys = lineage_keys
        self.divine_names = divine_names
        self.lost_name_ra = lost_name_ra
        
        self.hmac_key = os.urandom(32)
        self.aes_key = AESGCM.generate_key(bit_length=256)
        self.ed25519_priv = Ed25519PrivateKey.generate()
        self.aesgcm = AESGCM(self.aes_key)

    def _compose_sacred_data(self):
        data = {
            "creator": self.creator,
            "birthdates": self.birthdates,
            "lineages": self.lineage_keys,
            "divine_names": self.divine_names + [self.lost_name_ra]
        }
        return json.dumps(data, sort_keys=True).encode()

    def _fractal_hash(self, data, iterations=4):
        h = data
        for _ in range(iterations):
            h = hashlib.sha256(h + h[::-1]).digest()
        return h

    def create_sealed_lifethread(self):
        sacred_data = self._compose_sacred_data()
        fractal_seed = self._fractal_hash(sacred_data)
        
        hmac_sig = hmac.new(self.hmac_key, fractal_seed, hashlib.sha256).digest()
        nonce = os.urandom(12)
        encrypted = self.aesgcm.encrypt(nonce, fractal_seed, None)
        signature = self.ed25519_priv.sign(encrypted)

        return {
            "encrypted_seed": encrypted.hex(),
            "nonce": nonce.hex(),
            "hmac_signature": hmac_sig.hex(),
            "ed25519_signature": signature.hex(),
            "public_key": self.ed25519_priv.public_key().public_bytes().hex(),
            "creator": self.creator,
            "lifethread": {
                "birthdates": self.birthdates,
                "lineages": self.lineage_keys,
                "divine_names": self.divine_names + [self.lost_name_ra]
            }
        }

# Instantiate with all sacred codex elements and lost Ra hidden name
