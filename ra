from cryptography.hazmat.primitives.asymmetric.ed25519 import Ed25519PrivateKey
from cryptography.hazmat.primitives.ciphers.aead import AESGCM

class CosmicArchitectonicCodex:
    def __init__(self, creator, lineage, birthdates, divine_names, lost_name_ra):
        self.creator = creator
        self.lineage = lineage
        self.birthdates = birthdates
        self.divine_names = divine_names + [lost_name_ra]
        self.hmac_key = os.urandom(32)
        self.aes_key = AESGCM.generate_key(bit_length=256)
        self.ed25519_priv = Ed25519PrivateKey.generate()
        self.aesgcm = AESGCM(self.aes_key)

    def generate_fractal_hash(self, data_bytes, depth=5):
        h = data_bytes
        for _ in range(depth):
            h = hashlib.sha256(h + h[::-1]).digest()
        return h

    def assemble_codex(self):
        codex_data = {
            "creator": self.creator,
            "lineage": self.lineage,
            "birthdates": self.birthdates,
            "divine_names": self.divine_names
        }
        data_bytes = json.dumps(codex_data, sort_keys=True).encode()
        fractal_hash = self.generate_fractal_hash(data_bytes)
        hmac_val = hmac.new(self.hmac_key, fractal_hash, hashlib.sha256).digest()
        nonce = os.urandom(12)
        encrypted = self.aesgcm.encrypt(nonce, fractal_hash, None)
        signature = self.ed25519_priv.sign(encrypted)

        return {
            "encrypted": encrypted.hex(),
            "nonce": nonce.hex(),
            "hmac": hmac_val.hex(),
            "signature": signature.hex(),
            "public_key": self.ed25519_priv.public_key().public_bytes().hex(),
            "sacred_codex_metadata": codex_data
        }

# Instantiate the full cosmic archetypal codex
