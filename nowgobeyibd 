    paths: ["codex/**/*.json","codex/**/*.yaml"]

jobs:
  codex-register:
    runs-on: ubuntu-latest
    steps:
    - uses: actions/checkout@v4
    - name: Hash and Sign codex
      run: python sign_hash_codex.py codex/
    - name: Register with CodexImmortal
      run: |
        for file in codex/**/*.json; do
          curl -X POST https://codeximmortal.com/api/upload \
          -F codex=@$file
        done
    - name: Notify HoneyHiveNexus
      run: curl -X POST https://honeyhivenexus.com/api/event -d '{"repo":"enochian-codex-immortal"}'{
