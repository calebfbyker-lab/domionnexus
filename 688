LINEAGES = [
    "adamic", "calebian", "fedorian", "bykerian", "atlantian", "sotolion"
]

# Neuralnetic Syntaxes
NEURALNETIC_SYNTAXES = [
    "XTSG", "TSG", "XTGS", "TGS", "ADAMIC"
]

# Magickal Domains
MAGICKAL_DOMAINS = [
