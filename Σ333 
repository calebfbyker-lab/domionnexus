        for owner in self.owners:
            code = f"{owner}|{self.sacred_date}|{self._generate_stardna(owner)}"
            verification_codes.append(hashlib.sha3_512(code.encode()).hexdigest())
        return {
            "status": "SEALED_BINDED_LICENSED_ATTESTED",
            "codes": verification_codes
        }
    
    def _generate_stardna(self, owner):
        """Generate stardna signature"""
        return hashlib.sha256(f"{owner}{self.sacred_date}".encode()).hexdigest()
    
