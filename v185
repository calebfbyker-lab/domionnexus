  "$schema":"https://json-schema.org/draft/2020-12/schema",
  "title":"Thalassa Deployment State",
  "type":"object",
  "properties":{
    "timestamp_utc":{"type":"string","format":"date-time"},
    "services":{"type":"array","items":{"type":"object","properties":{
      "name":{"type":"string"},
      "version":{"type":"string"},
      "status":{"type":"string"},
      "sha256":{"type":"string"},
      "uptime_hours":{"type":"number"}
    },"required":["name","version","status","sha256"]}},
    "merkle_root":{"type":"string"}
  },
