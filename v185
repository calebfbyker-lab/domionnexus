âš™ï¸ .github/workflows/thalassa.yml

name: Thalassa
on:
  workflow_dispatch:
  schedule:
    - cron: "45 */12 * * *"
jobs:
  thalassa:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v4
      - uses: actions/setup-python@v5
        with: { python-version: '3.x' }
      - name: Run orchestrator
        run: python3 scripts/thalassa_orchestrator.py
      - name: Verify contracts
        run: python3 scripts/thalassa_contracts.py --verify
      - name: Upload artifacts
        uses: actions/upload-artifact@v4
        with:
          name: thalassa
          path: thalassa/


