# 6b) Embed Nexuses note into FINAL_ATTEST if present
    fa = ROOT / "FINAL_ATTEST.json"
    na = ROOT / "NEXUSES_ATTEST.json"
    if fa.exists() and na.exists():
        j = json.loads(fa.read_text())
        j["nexuses"] = json.loads(na.read_text())
        fa.write_text(json.dumps(j, indent=2))


