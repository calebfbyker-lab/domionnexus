4) Builder (seals + bundle + attest)

nexuses/tools/nexuses_build.py

from __future__ import annotations
import json, pathlib, hashlib, datetime, shutil, zipfile

ROOT = pathlib.Path(".")
NROOT = ROOT/"nexuses"
MAN = json.loads((NROOT/"manifest.json").read_text())
CONF = json.loads((NROOT/"config.json").read_text())
SCHEMA_PATHS = [
    "FINAL_ATTEST.json",
    "codex/harmonia/harmonia_result.json",
    "examples/constraint_spell.final.json",
    "treasury_allocation.json",
    "chain/attestations.jsonl"
]

def sha(p: pathlib.Path) -> str:
    return hashlib.sha256(p.read_bytes()).hexdigest()

def mk_digest(paths:list[str]) -> str:
    h = hashlib.sha256()
    for rel in paths:
        p = ROOT/rel
        if p.exists(): h.update(p.read_bytes())
        else: h.update(rel.encode())
    return h.hexdigest()

def assemble(kind:str, inputs:list[str], emblems:list[str]) -> dict:
    composite = mk_digest(inputs)
    return {
        "id": f"nexus:{kind}:{composite[:12]}",
        "kind": kind,
        "inputs": inputs,
        "emblems": emblems,
        "digests": {"composite_sha256": composite},
        "notes": f"{kind} nexus sealed for CFBK 1998-10-27"
    }

def main():
    # Inputs (required but tolerant)
    inputs = [rel for rel in SCHEMA_PATHS if (ROOT/rel).exists()]
    if not inputs:
        raise SystemExit("No inputs available to build Nexuses.")

    # Build three nexus records
    rec_summum  = assemble("summum", inputs, CONF.get("summum",[]))
    rec_absumm  = assemble("absumm", inputs, CONF.get("absumm",[]))
    rec_aetern  = assemble("aeternum", inputs, CONF.get("aeternum",[]))

    nexus_records = [rec_summum, rec_absumm, rec_aetern]
    attest = {
        "codex": "nexuses",
        "timestamp": datetime.datetime.utcnow().isoformat()+"Z",
        "binding": MAN["binding"],
        "license": MAN["license"],
        "records": nexus_records
    }

    # Write attest
    attest_path = ROOT / MAN["artifacts"]["attest"]
    attest_path.write_text(json.dumps(attest, indent=2), encoding="utf-8")

    # Bundle (zip): manifest, schema, config, attest
    bundle_path = ROOT / MAN["artifacts"]["bundle"]
    if bundle_path.exists(): bundle_path.unlink()
    with zipfile.ZipFile(bundle_path, "w", zipfile.ZIP_DEFLATED) as z:
        for rel in ["nexuses/manifest.json","nexuses/schema.json","nexuses/config.json", MAN["artifacts"]["attest"]]:
            z.write(ROOT/rel, arcname=rel)

    print("âœ… NEXUSES built")
    print("  â€¢ attest:", attest_path)
    print("  â€¢ bundle:", bundle_path)

if __name__ == "__main__":
    main()


---

5) Verifier (quick integrity check)

nexuses/tools/nexuses_verify.py

from __future__ import annotations
import json, pathlib, hashlib, sys

ROOT = pathlib.Path(".")
MAN  = json.loads((ROOT/"nexuses/manifest.json").read_text())
ATTEST = ROOT / MAN["artifacts"]["attest"]

def ok_hex(s:str)->bool:
    try:
        int(s,16); return True
    except: return False

if __name__=="__main__":
    if not ATTEST.exists():
        print("âŒ missing attest"); sys.exit(2)
    a = json.loads(ATTEST.read_text())
    ok = True
    for r in a.get("records", []):
        h = r.get("digests",{}).get("composite_sha256","")
        if not (h and len(h)==64 and ok_hex(h)):
            print("âŒ bad digest for", r.get("id"))
            ok = False
    if not ok: sys.exit(2)
    print("âœ… NEXUSES verify ok")


