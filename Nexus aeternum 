{
  "$id": "codex.nexuses.schema",
  "type": "object",
  "required": ["id", "kind", "inputs", "emblems", "digests"],
  "properties": {
    "id": { "type": "string" },
    "kind": { "enum": ["summum", "absumm", "aeternum"] },
    "inputs": {
      "type": "array",
      "items": { "type": "string" },
      "minItems": 1
    },
    "emblems": {
      "type": "array",
      "items": { "type": "string" }
    },
    "digests": {
      "type": "object",
      "required": ["composite_sha256"],
      "properties": {
        "composite_sha256": { "type": "string" }
      }
    },
    "notes": { "type": "string" }
  }
}


