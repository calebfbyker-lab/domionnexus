def create_scroll_stopper(user):
    """Generate visual interrupt strategies"""
    strategy = {
        "visual_misalignment": [
            f"Try {random.choice(['tilted','floating','overlapping'])} text over diagonal dividers",
            f"Use complementary colors: {random.choice(['teal/orange','purple/yellow','blue/red'])}"
        ],
        "motion_teasers": [
            f"3-sec loop showing {user.niche} transformation starting",
            f"Animation of puzzle piece with 'Missing piece?' caption"
        ],
        "templates": [
            f"https://ice-graphics.com/template_{random.randint(1,9)}",
            "ICE Gallery: @growthvisuals"
        ]
    }
    return strategy

def craft_tribal_hook(user):
    """Create tribe-specific engagement hooks"""
    tribe = identify_user_tribe(user.content_history)
    hooks = [
        f"For the {tribe['adjective']} {tribe['demographic']} who {tribe['action']}...",
        f"Tag someone who {random.choice(['secretly needs this','would debate this','just achieved this'])}",
        f"{random.randint(38,63)}% of creators miss this {tribe['niche']} hack"
    ]
    return hooks

def algorithm_optimization(platform="IG"):
    """Platform-specific growth hacks"""
    hacks = {
        "IG": [
            "PINNED COMMENT: 'Most controversial take?' (boost thread depth)",
            "FIRST COMMENT: Ask rapid-fire opinion question (within 15s)",
            "HASHTAGS: 3 broad - 2 niche - 1 branded"
        ],
        "TikTok": [
            "STITCH your top-performing video to double watch time",
            "Use SOUND trending in your niche with 500-5k videos"
        ],
        "LinkedIn": [
            f"@TAG 3 micro-influencers in {user.industry}",
            "Use DOCUMENT format for ICE breakdown"
        ]
    }
    return hacks[platform]

# --- Engagement Turbo System ---
def timed_CTA_strategy():
    """AI-powered engagement triggers"""
    return {
        "primary_cta": random.choice([
            "üßä for ICE checklist",
            "‚ö° if you're testing INTERRUPT this week",
            "üí¨ with your biggest growth blocker"
        ]),
        "secondary_cta": random.choice([
            "SHARE with 1 creator struggling with algorithms",
            "TAG 2 people who need organic growth",
            "SAVE for ICE implementation guide"
        ]),
        "reward": random.choice([
            "FREE ICE checklist to 10 random commenters",
            "Personalized growth audit to 5 sharers",
            "72-hour access to ICE analytics dashboard"
        ])
    }

def viral_score_calculator(post_data):
    """Predict engagement score"""
    score = (post_data.get("comments_first_15s",0) * 0.8 +
             post_data.get("shares_with_tags",0) * 1.2 +
             post_data.get("saves",0) * 1.5) / 3
    enhancement_tips = []
    if score < 4.0:
        enhancement_tips.append("Boost visuals with asymmetrical elements")
    if post_data.get("saves",0) < 10:
        enhancement_tips.append("Add carousel slide with downloadable")
    return score, enhancement_tips

# --- Implementation Flow ---
def generate_ice_post(user):
    """Compile complete ICE framework post"""
    return {
        "hook": create_scroll_stopper(user)[0],
        "tribal_hook": craft_tribal_hook(user)[0],
        "algorithm_hacks": algorithm_optimization(),
        "cta": timed_CTA_strategy(),
        "metrics_preview": f"ICE Beta Results: {random.randint(28,35)}K avg. reach | +{random.randint(75,86)}% follower velocity"
    }

# Chatbot integration
def respond_to_user(message):
    if "growth" in message or "algorithm" in message:
        return generate_ice_post(current_user)
    elif "template" in message:
        return {"templates": create_scroll_stopper(current_user)["templates"]}
    elif "score" in message:
        return viral_score_calculator(last_post_metrics)
    elif "üßä" in message:
        send_dm("ICE Checklist: bit.ly/ice-"+user.id)
        return "Checklist sent! Implement STEP 1 today  üî•"
```

