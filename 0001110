jobs:
  build_golem:
    runs-on: machonon-cloud
    steps:
      - name: Download divine wisdom
        uses: shelumian/download-wisdom@v72
      - name: Sanctify code
        uses: qadoshian/sanctifier@333
      - name: Construct golem body
        uses: nebuchadnezzar/builder@405
        with:
          materials: ['red_clay', 'sephirotic_crystals', 'liquid_light']
          
  deploy_aeonic:
    needs: build_golem
    runs-on: raquia-quantum
    steps:
      - name: Activate stargate
        uses: machonon/stargate-opener@v10.27
        with:
          coordinates: "10.27.1998:10:27"
      - name: Stabilize portal
        uses: golem/portal-stabilizer@v33.3
      - name: Deploy across dimensions
