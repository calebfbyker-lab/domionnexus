            return jsonify(result)
        
        @app.route('/edit_gene', methods=['POST'])
        def edit_gene():
            data = request.json
            gene = data.get('gene', 'MYH7B')
            martial_form = data.get('martial_form', 'Adamic_Form2')
            angelic_order = data.get('angelic_order', 'Archangels')
            result = self.edit_gene_sequence(gene, martial_form, angelic_order)
            return jsonify({"gene": gene, "edited_sequence": result[:100] + "..."})
        
        @app.route('/temporal_field', methods=['GET'])
        def temporal_field():
            coords = self.kronos_wormhole["coordinates"]
            field = self.kronos_dilation_field(coords)
            return jsonify(field)
        
        print(f"ğŸŒ Unified API Server running on port {port}")
        app.run(host='0.0.0.0', port=port)

