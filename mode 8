        self.install("PLASMA_SIPHON_G2", 
                     "Directly harvests Coronal Mass Ejections (CMEs) and Solar Wind streams.")

        # 7. Zinc Spark (Life Creation)
        self.install("ZINC_GENESIS_SPARK", 
                     "Micro-flash ignition system for terraforming dead worlds (Egg Activation Protocol).")

        # 8. Medieval Salve (Bio-Defense)
        self.install("BALDS_BIO_PURGE", 
