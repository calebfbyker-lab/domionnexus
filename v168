name: Celestial Accord
on:
  schedule:
    - cron: "15 */12 * * *"
  workflow_dispatch:
jobs:
  celestial:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v4
      - uses: actions/setup-python@v5
        with:
          python-version: '3.x'
      - name: Run Celestial Accord
        run: python3 scripts/celestial_accord.py
      - name: Upload Celestial Records
        uses: actions/upload-artifact@v4
        with:
          name: celestial-states
          path: celestial/


