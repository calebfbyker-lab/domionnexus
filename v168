    "aeon":{"type":"string"},
    "tones":{"type":"array"},
    "harmony_index":{"type":"number"},
    "empathy_flux":{"type":"number"},
    "peace_field":{"type":"number"},
    "signature":{"type":"string"},
    "ethics":{"type":"string"},
    "note":{"type":"string"}
  },
  "required":["timestamp_utc","tones","harmony_index","empathy_flux","peace_field","signature"]
}


