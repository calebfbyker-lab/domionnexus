src/glyph/tsg_glyph_runtime.js
// src/glyph/tsg_glyph_runtime.js
const fs = require('fs');
const BRAID = JSON.parse(fs.readFileSync(__dirname + '/tsg_braid_palette.json','utf8'));
const PLANETS = new Map(BRAID.planets.map(p=>[p.emoji,p]));
const ZODIAC  = new Map(BRAID.zodiac_core.map(z=>[z.sign,z]));

const FLAGS = {
  'ğŸ”’strict':  { ethics:'strict' },
  'ğŸ’ audit':   { audit:true },
  'ğŸ›¡ï¸':        { protect:true },
  'â›ï¸mine':    { mining:true },
  'ğŸ§ council': { ai_council:true },
  'âœ¨':         { blessing:true }
};

function extractTheme(src){
  const planets = [...src].filter(ch=>PLANETS.has(ch)).map(ch=>PLANETS.get(ch).hex);
  const zodiac  = [...src].filter(ch=>ZODIAC.has(ch)).map(ch=>ZODIAC.get(ch).hex);
  const ribbon = planets.slice(0,3);
  const tint = zodiac[0] || '#9aa0b2';
  return { ribbon, tint };
}
function extractFlags(src){
  const out = {};
  Object.entries(FLAGS).forEach(([k,v])=>{ if (src.includes(k)) Object.assign(out,v); });
  const solomon = /Î£72/.test(src) ? Array.from({length:72},(_,i)=>i+1) : [];
  const codex   = (/Î£333/.test(src) || /\/333/.test(src)) ? Array.from({length:333},(_,i)=>i+1) : [];
  return { ...out, seals:{ solomon, codex } };
}
function noun(src){ const m = src.match(/âŸ¦([^âŸ§]+)âŸ§/); if(!m) throw new Error('Missing âŸ¦NOUNâŸ§'); return m[1].trim().toUpperCase(); }

function compile(glyph, subjectSha256){
  const theme = extractTheme(glyph);
  const flags = extractFlags(glyph);
  const N = noun(glyph);
  const intent = `solomon_custom_${N.toLowerCase()}_${subjectSha256.slice(0,8)}`;
  const policy = { noun:N, theme, flags, subject_sha256:subjectSha256, glyph, t:new Date().toISOString() };
  return { intent, policy };
}
module.exports = { compile };

