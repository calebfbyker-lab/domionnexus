        for i, file in enumerate(contents):
            angle = sum(ord(c) for c in file.content)/1000 * np.pi/2
            qc.rx(angle, i % 144)
            
        # Create optimization oracle
        qc.h(range(144))
        for i in range(143):
            qc.cx(i, i+1)
