class CorePulseDetector:
    def detect(self, strength, depth):
        """Capture Earth's magnetic pulse"""
        return {'amplitude': strength * depth / 1e3}

    def amplify(self, factor=1.618):
        """Golden ratio amplification"""
        return {'amplitude': self.amplitude * factor}
