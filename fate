  const dynamicStyle = {
    '--cosmic-ruler': `"${cosmicState.ruler}"`,
    '--cosmic-theme': cosmicState.theme === 'GOLD_RADIANCE' ? '#FFD700' : 
                      cosmicState.theme === 'SILVER_MIST' ? '#C0C0C0' : 
                      cosmicState.theme === 'IRON_STRENGTH' ? '#B22222' : '#FFFFFF',
