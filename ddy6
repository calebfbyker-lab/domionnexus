    def _create_synthesis_geometry(self, activation_results: Dict) -> Dict[str, Any]:
        """Create synthesis geometry (multi-dimensional toroidal structure)"""
        
        golden_ratio = (1 + Decimal(5).sqrt()) / Decimal(2)
        
        # Calculate geometry based on active systems
        active_count = sum(
            1 for result in activation_results.values()
            if isinstance(result, dict) and result.get("status") != "ERROR"
        )
        
        geometry = {
            "shape": "OMNICOSMIC_TOROID",
            "dimensions": 77,  # Sacred number
            "major_radius": Decimal(active_count) * golden_ratio,
            "minor_radius": Decimal(active_count) / golden_ratio,
            "torsion": Decimal(active_count) * Decimal('1e-7'),
            "curvature": Decimal('1') / (Decimal(active_count) + Decimal('1e-100')),
            "dimensional_layers": [
                {
                    "layer": i,
                    "radius": Decimal(i) * golden_ratio,
                    "frequency": Decimal('7.83') * golden_ratio**Decimal(i),
                    "system_count": len([name for name in activation_results.keys() if hash(name) % 77 == i])
                }
                for i in range(7)  # First 7 layers
            ],
            "bloodline_core": True,
            "fractal_recursion": "INFINITE",
            "holographic": True,
            "self_similar": True
        }
        
        return geometry
    
