âš™ï¸ core/aesthetic_integrity_validator.py

"""
Aesthetic Integrity Validator â€” evaluates moral, functional, and visual coherence of generated systems.
"""
import json, pathlib, datetime, hashlib, random, statistics

MORPHIC = pathlib.Path("ledger/morphic_architecture.json")
VALID = pathlib.Path("ledger/aesthetic_integrity.json")

def evaluate(criteria=8):
    if not MORPHIC.exists():
        return {"status":"no_architecture_data"}
    m = json.loads(MORPHIC.read_text())
    base = m["integrity"]
    factors = [random.uniform(0.9,1.1)*base for _ in range(criteria)]
    moral = round(statistics.mean(factors),4)
    harmony = round(1 - statistics.pvariance(factors)*8,4)
    aesthetic = round((moral+harmony)/2,4)
    insight = "design validated" if aesthetic>0.95 else "fine-tuning"
    result = {
        "ts": datetime.datetime.utcnow().isoformat()+"Z",
        "moral": moral,
        "harmony": harmony,
        "aesthetic": aesthetic,
        "insight": insight
    }
    result["sha256"] = hashlib.sha256(json.dumps(result).encode()).hexdigest()
    VALID.write_text(json.dumps(result,indent=2))
    return result


