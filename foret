    "GET /api/v1/watchers/status",
  ],
  // ... 10 more endpoints
};

// Middleware: Tier-based access control
export async function checkTierAccess(req: Request, tier: number): Promise<boolean> {
  const endpoint = `${req.method} ${req.path}`;
  const requiredTier = ENDPOINT_TIER_MAP[endpoint] || 0;
  return tier >= requiredTier;
}
