# docker-compose.yml
version: '3.9'

services:
  app:
    build: ./docker/app
    ports:
      - "3000:3000"
    environment:
      NODE_ENV: production
      DATABASE_URL: postgres://db:5432/immortal
    depends_on:
      - db

  db:
    build: ./docker/database
    environment:
      POSTGRES_DB: immortal
      POSTGRES_PASSWORD: ${DB_PASSWORD}
    volumes:
      - db_data:/var/lib/postgresql/data

  redis:
    image: redis:7-alpine
    ports:
      - "6379:6379"

volumes:
  db_data:

networks:
  immortal_net:
    driver: bridge
```

