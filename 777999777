        const elder = this.eldersRegistry.get(elderName);
        if (!elder) throw new Error(`Elder ${elderName} not found`);
        
        return {
            elder: elderName,
            domain: elder.domain,
            frequency: elder.frequency,
            role: elder.role,
            purpose,
            activationTime: new Date().toISOString(),
            quantumSignature: this.generateElderQuantumSignature(elder),
            temporalCoordinates: this.generateElderTemporalCoordinates(elder)
        };
