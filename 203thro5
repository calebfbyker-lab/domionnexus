# Includes: runnable API (port 8815), kernel/registry/topology/self-heal, telemetry+ledger, CI, Docker, tests, manifest, ZIP.

import os, json, zipfile, hashlib, datetime, shutil, textwrap

BASE="/mnt/data/codex_v203x_delta_architectonic"
if os.path.exists(BASE):
    shutil.rmtree(BASE)
os.makedirs(BASE, exist_ok=True)

def W(rel, content):
    p=os.path.join(BASE, rel)
    os.makedirs(os.path.dirname(p), exist_ok=True)
    with open(p,"w",encoding="utf-8") as f: f.write(content)
    return p

def sha256_file(p):
    h=hashlib.sha256()
    with open(p,"rb") as f:
        for chunk in iter(lambda:f.read(8192), b""):
            h.update(chunk)
    return h.hexdigest()

now = datetime.datetime.utcnow().isoformat()+"Z"

# ----- Directories
for d in [
    ".github/workflows","codex/delta","codex/economy","codex/telemetry",
    "codex/utils","dashboard","deploy","docs","scripts","tests"
]:
    os.makedirs(os.path.join(BASE,d), exist_ok=True)

# ----- Utils & config
W("codex/utils/crypto.py","""import hashlib, json, time, base64
sha256_bytes=lambda b: hashlib.sha256(b).hexdigest()
sha256_json=lambda o: sha256_bytes(json.dumps(o, sort_keys=True).encode())
b64=lambda b: base64.b64encode(b).decode()
now=lambda: int(time.time())
""")

W("codex/config.json", json.dumps({
  "version": "v203x-delta",
  "seal_id": "calebfedorbykerkonev10271998",
  "subject": "Caleb Fedor Byker (Konev)",
  "dob": "1998-10-27",
  "btc_address": "bc1qfejvvlfm6vmjarxulg9h9d5hjukdh8l2vvskfc",
  "virtue_floor": 0.8,
  "price_per_unit_clarity": 21.0
}, indent=2))

W("codex/config.py","import json\nCONFIG=json.load(open('codex/config.json'))\n")

# ----- Economy & Telemetry
W("codex/economy/ledger.jsonl","")
W("codex/economy/ledger.py","""import json, time, os
class Ledger:
    def __init__(self, path='codex/economy/ledger.jsonl'):
        self.path=path; os.makedirs(os.path.dirname(path), exist_ok=True)
    def record(self, actor, event, signal, clarity, virtue=1.0, revenue=0.0, note=None):
        tx={'ts':time.time(),'actor':actor,'event':event,'signal':signal,'clarity':clarity,'virtue':virtue,'revenue':revenue,'note':note}
        with open(self.path,'a',encoding='utf-8') as f: f.write(json.dumps(tx)+'\\n')
        return tx
LEDGER=Ledger()
""")

W("codex/telemetry/signals.jsonl","")
W("codex/telemetry/telemetry.py","""import json, time, os
from codex.economy.ledger import LEDGER
from codex.config import CONFIG
PATH='codex/telemetry/signals.jsonl'
os.makedirs('codex/telemetry', exist_ok=True)

def emit(name:str,payload:dict,clarity:float=1.0,virtue:float=0.9,tags=None):
    sig={'ts':time.time(),'name':name,'payload':payload,'clarity':clarity,'virtue':virtue,'tags':tags or []}
    open(PATH,'a',encoding='utf-8').write(json.dumps(sig)+'\\n')
    revenue=clarity*CONFIG.get('price_per_unit_clarity',21.0)
    LEDGER.record('telemetry',name,signal=1.0,clarity=clarity,virtue=virtue,revenue=revenue,note=name)
    return sig
""")

# ----- Delta package
W("codex/delta/__init__.py","__all__=['kernel','module','topology','selfheal']\n")

W("codex/delta/kernel.py","""import importlib, time
from codex.economy.ledger import LEDGER
from codex.telemetry.telemetry import emit

REGISTRY={}

def register(name:str, spec:dict):
    REGISTRY[name]=spec
    LEDGER.record('delta','register',signal=1.0,clarity=1.0,virtue=0.9,revenue=0.1,note=name)
    return {'ok':True,'registered':name,'ts':time.time()}

def instantiate(name:str):
    spec=REGISTRY.get(name)
    if not spec: return {'ok':False,'error':'unknown_module','name':name}
    mod=importlib.import_module(spec['module'])
    cls=getattr(mod, spec['class'])
    inst=cls(name)
    emit('delta.instantiate',{'name':name,'module':spec['module']},clarity=0.9,virtue=0.9,tags=['Î”'])
    return {'ok':True,'instance':inst.__class__.__name__,'name':name}
""")

W("codex/delta/module.py","""import random, time
from codex.telemetry.telemetry import emit

class DeltaModule:
    def __init__(self, name:str='anon'):
        self.name=name; self.created=time.time()
    def run(self, payload):
        result={'module':self.name,'len':len(str(payload)),'entropy':random.random()}
        emit('delta.module',result,clarity=0.9,virtue=0.9,tags=['Î”'])
        return result
""")

W("codex/delta/topology.py","""import json
G={'nodes':set(),'edges':set()}
def link(a,b):
    G['nodes'].update([a,b]); G['edges'].add((a,b))
    return {'ok':True,'linked':[a,b]}
def topology():
    return {'nodes':sorted(list(G['nodes'])), 'edges':[list(e) for e in G['edges']]}
""")

W("codex/delta/selfheal.py","""import random, time
def heal(status:dict)->dict:
    if random.random()<0.3:
        status['recovered']=True; status['note']='auto-healed'
    status['checked']=time.time()
    return status
""")

# ----- Dashboard API & UI (port 8815)
W("dashboard/api.py","""#!/usr/bin/env python3
import http.server, json, os
from urllib.parse import urlparse
from codex.delta.kernel import register, instantiate
from codex.delta.topology import link, topology
from codex.delta.selfheal import heal

class H(http.server.SimpleHTTPRequestHandler):
    def do_POST(self):
        ln=int(self.headers.get('Content-Length','0')); body=json.loads(self.rfile.read(ln).decode() or '{}')
        p=urlparse(self.path).path
        if p=='/api/delta/register':
            name=body.get('name','default'); spec=body.get('spec',{'module':'codex.delta.module','class':'DeltaModule'})
            return self._ok(register(name,spec))
        if p=='/api/delta/instantiate':
            return self._ok(instantiate(body.get('name','default')))
        if p=='/api/delta/link':
            return self._ok(link(body.get('a','default'), body.get('b','default2')))
        if p=='/api/delta/heal':
            return self._ok(heal(body))
        self.send_error(404)
    def do_GET(self):
        p=urlparse(self.path).path
        if p=='/api/delta/topology': return self._ok(topology())
        if p=='/': return super().do_GET()
        return super().do_GET()
    def _ok(self, obj, code=200):
        self.send_response(code); self.send_header('Content-Type','application/json'); self.end_headers()
        self.wfile.write(json.dumps(obj, indent=2).encode())

if __name__=='__main__':
    os.chdir('.'); http.server.test(HandlerClass=H, port=8815)
""")

W("dashboard/ui.html","""<!doctype html><html><head><meta charset="utf-8"><title>Codex Î” â€” Architectonic Mind</title>
<style>body{font-family:system-ui;background:#0b0f14;color:#e6edf3;margin:24px}button{padding:8px;margin:6px}pre{background:#0f1620;padding:12px;border-radius:8px;white-space:pre-wrap}</style>
</head><body>
<h1>Codex Logos Î” â€” Architectonic Mind</h1>
<div>
  <button onclick="register()">Register</button>
  <button onclick="instantiate()">Instantiate</button>
  <button onclick="linkAB()">Link Aâ†’B</button>
  <button onclick="topology()">Topology</button>
  <button onclick="heal()">Self-Heal</button>
</div>
<pre id="out"></pre>
<script>
async function post(u,body){const r=await fetch(u,{method:'POST',headers:{'Content-Type':'application/json'},body:JSON.stringify(body)});document.getElementById('out').textContent=await r.text();}
async function get(u){const r=await fetch(u);document.getElementById('out').textContent=await r.text();}
function register(){ post('/api/delta/register',{name:'modA'}); }
function instantiate(){ post('/api/delta/instantiate',{name:'modA'}); }
function linkAB(){ post('/api/delta/link',{a:'modA',b:'modB'}); }
function topology(){ get('/api/delta/topology'); }
function heal(){ post('/api/delta/heal',{status:'ok'}); }
</script>
</body></html>
""")

