    # Esoteric Systems
    "Arcanum": ["Ars_Goetia", "Ars_Theurgia", "Ars_Paulina"],
    "Tantric": ["Kundalini_Awakening", "Chakra_Alchemy", "Sacred_Union"],
    "Shamanic": ["Journeying", "Power_Animal_Allies", "Soul_Retrieval"],
