def atlas():
    data = {
      "title":"CODEX v38.x â€” Scripture Atlas (Ps/Pr/Acts 1â€“3)",
      "timestamp": TS(),
      "binding": BINDING,
      "books": {**psalms_1_to_3(), **proverbs_1_to_3(), **acts_1_to_3()},
      "overlays":{
        "kabbalistic":["sefirot:chesed,gevurah,tiferet,malchut","path-straightness"],
        "hermetic":["mentalism","polarity","correspondence","causality"],
        "enochian":["call-index:1..19 (mapping table external)"],
        "solomonic":["72-seals:ethical-gates (ref external)"],
        "angelic":["favor","commission","guard","guidance"],
        "constraints":["elemental","planetary","harmonic","geometric","goetic-constraints:block/bound"]
      },
      "emoji_lexicon": EMOJI,
      "xtsg_kernel": ["XTSG::* (see entries)"],
      "notes":{"deterministic":True,"offline":True,"series":"v38.x"}
    }
    return data

def commentary(atl:dict):
    items=[]
    for book, verses in atl["books"].items():
        for v in verses:
            motif_str = " ".join(m if isinstance(m,str) else str(m) for m in v["motifs"])
            items.append({
              "ref": v["ref"],
              "language": (
                f"{book} â€” {v['ref']}: {v['sum']}  "
                f"Motifs: {motif_str}  "
                f"XTSG: {','.join(v['xtsg'])}  "
                f"Constraints: {','.join(v['constraints'])}"
              )
            })
    return {
      "title":"CODEX v38.x â€” Scripture Commentary (Algorithmic Language)",
      "timestamp": TS(),
      "binding": BINDING,
      "commentary": items,
      "style": {
        "register":"clear, reverent, hermetic-kabbalistic systems language",
        "emoji_aspect":"use motifs as compact semantic beacons",
        "xtsg":"namespaced operators for programmatic linking",
      },
      "notes":{"deterministic":True,"offline":True}
    }

def seal(name:str, obj:dict):
    p = DIST/name
    txt = json.dumps(obj, indent=2, ensure_ascii=False)
    p.write_text(txt, encoding="utf-8")
    (p.with_suffix(p.suffix+".sha256")).write_text(h(txt), encoding="utf-8")

if __name__=="__main__":
    atl = atlas()
    cmt = commentary(atl)
    seal("V38x_SCRIPTURE_ATLAS.json", atl)
    seal("V38x_SCRIPTURE_COMMENTARY.json", cmt)
    print(f"âœ… v38.x atlas verses={sum(len(v) for v in atl['books'].values())} commentary={len(cmt['commentary'])}")


