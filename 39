tools/v38_manifest.py

#!/usr/bin/env python3
# EUCELA Tri-License Â© 2025 Caleb Fedor Byker (Konev)
"""
v38 â€” Final Manifest
Collects all v38 artifacts + links to v36 ledger & v37 continuum into a Merkle-sealed manifest.
Outputs:
  dist/CODEX_V38_MANIFEST.json
  dist/CODEX_V38_MANIFEST.sha256
"""
from __future__ import annotations
import pathlib, json, hashlib, datetime
DIST=pathlib.Path("dist"); h=lambda s: hashlib.sha256(s.encode()).hexdigest()

FILES = [
  "V38_TELEOGENESIS.json",
  "V38_SIGNAL_ROUTES.json",
  "V38_POLICY_REPORT.json",
  "V37x_CONTINUUM.json",
  "CODEX_V36_LEDGER.json"
]

def merkle(parts:list[str])->str:
    if not parts: return ""
    layer=sorted(parts)
    while len(layer)>1:
        nxt=[]
        for i in range(0,len(layer),2):
            a=layer[i]; b=layer[i+1] if i+1<len(layer) else layer[i]
            nxt.append(h(a+b))
        layer=nxt
    return layer[0]

if __name__=="__main__":
    entries=[]
    for fn in FILES:
        p=DIST/fn
        txt=p.read_text(encoding="utf-8")
        entries.append({"path":fn,"sha256":h(txt),"size":p.stat().st_size})
    root = merkle([e["sha256"] for e in entries])
    now = datetime.datetime.utcnow().replace(microsecond=0).isoformat()+"Z"
    manifest={
      "title":"CODEX v38 â€” Teleogenesis Â· Telekinesis Â· Nexus Operator",
      "timestamp":now,
      "links":{"ledger":"CODEX_V36_LEDGER.json","continuum":"V37x_CONTINUUM.json"},
      "files":entries,
      "merkle_root":root,
      "notes":{"deterministic":True,"offline":True,"series":"v38"}
    }
    out=DIST/"CODEX_V38_MANIFEST.json"
    txt=json.dumps(manifest,indent=2,ensure_ascii=False)
    out.write_text(txt,encoding="utf-8")
    DIST.joinpath("CODEX_V38_MANIFEST.sha256").write_text(h(txt),encoding="utf-8")
    print(f"âœ… v38 manifest sealed â†’ {root[:16]}")


