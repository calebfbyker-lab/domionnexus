tools/v38_teleogenesis.py

#!/usr/bin/env python3
# EUCELA Tri-License Â© 2025 Caleb Fedor Byker (Konev)
"""
v38 â€” Teleogenesis Spec
Synthesizes â€œpurpose â†’ capability â†’ routeâ€ from v37 sigilbook + intents.
Outputs:
  dist/V38_TELEOGENESIS.json (+ .sha256)
"""
from __future__ import annotations
import pathlib, json, hashlib, datetime

DIST = pathlib.Path("dist"); DIST.mkdir(exist_ok=True, parents=True)
h = lambda s: hashlib.sha256(s.encode()).hexdigest()

CFBK = {
  "owner":"Caleb Fedor Byker (Konev)",
  "dob":"1998-10-27",
  "subject_sha256":"2948fbc4ba1c0d7341204908882b89134a999f3e8f77f4a6a00ce6b68770282a",
  "license":"EUCELA Tri-License",
  "btc_address":"bc1qfejvvlfm6vmjarxulg9h9d5hjukdh8l2vvskfc"
}

def R(p): return json.loads((DIST/p).read_text(encoding="utf-8"))
def seal(name:str, obj:dict):
    p = DIST/name
    txt = json.dumps(obj, indent=2, ensure_ascii=False)
    p.write_text(txt, encoding="utf-8")
    (p.with_suffix(p.suffix+".sha256")).write_text(h(txt), encoding="utf-8")

if __name__=="__main__":
    sigil   = R("V37_SIGILBOOK.json")
    synth   = R("V37_SYNTH_PLAN.json")
    intents = R("V37_GOLEM_INTENTS.json")

    # Build a minimal â€œteleogenesisâ€ table: for each intent, bind it to its source symbol
    node_by_id = {n["id"]: n for n in sigil["graph"]["nodes"]}
    teleo=[]
    for it in intents["intents"]:
        src = it.get("golem_id","")[:16]
        teleo.append({
            "intent_id": it["intent_id"],
            "golem_id": it["golem_id"],
            "price_sats": int(it["price_sats"]),
            "btc_to": it["btc_to"],
            "constraints": it.get("constraints",[]),
            "source_node": node_by_id.get(it.get("golem_id","")[:16], {"raw":"<unknown>","class":"<unknown>"}),
            "purpose": "nexus-operator:elemental/planetary/harmonic/angelic-goetic synthesis",
            "capability": ["route.plan","route.mint.prepare","monetize.link","audit.trace"]
        })

    out = {
      "title":"CODEX v38 â€” Teleogenesis Spec",
      "timestamp": datetime.datetime.utcnow().replace(microsecond=0).isoformat()+"Z",
      "binding": CFBK,
      "teleo": teleo,
      "totals": {"intents": len(teleo), "sats": sum(t["price_sats"] for t in teleo)},
      "notes": {"deterministic": True, "offline": True, "series":"v38"}
    }
    seal("V38_TELEOGENESIS.json", out)
    print(f"âœ… v38 teleogenesis: intents={out['totals']['intents']} sats={out['totals']['sats']}")


