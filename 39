tools/v38_policy.py

#!/usr/bin/env python3
# EUCELA Tri-License Â© 2025 Caleb Fedor Byker (Konev)
"""
v38 â€” Safety & Constraint Policy
Static policy checker for routes before execution.
Rejects disallowed scopes or missing constraints (deterministic).
Outputs:
  dist/V38_POLICY_REPORT.json (+ .sha256)
Exit non-zero on failure to allow Make/CI gating.
"""
from __future__ import annotations
import pathlib, json, hashlib, datetime, sys

DIST = pathlib.Path("dist"); DIST.mkdir(exist_ok=True, parents=True)
h = lambda s: hashlib.sha256(s.encode()).hexdigest()

ALLOWED_SCOPES = {"elemental","planetary","harmonic","angelic","goetic"}
REQUIRED_STEPS = ["gate.check(v35x)","mint.plan(v37)","mint.v34","monetize.v36","continuum.v37x"]

def R(name:str): return json.loads((DIST/name).read_text(encoding="utf-8"))
def seal(name:str, obj:dict):
    p=DIST/name; txt=json.dumps(obj,indent=2,ensure_ascii=False)
    p.write_text(txt,encoding="utf-8")
    (p.with_suffix(p.suffix+".sha256")).write_text(h(txt),encoding="utf-8")

if __name__=="__main__":
    teleo = R("V38_TELEOGENESIS.json")["teleo"]
    routes = R("V38_SIGNAL_ROUTES.json")["routes"]
    ok=True; issues=[]
    # Check constraints & steps deterministically
    for t in teleo:
        missing = [c for c in t.get("constraints",[]) if c not in ALLOWED_SCOPES]
        if missing:
            ok=False
            issues.append({"intent_id":t["intent_id"],"issue":"disallowed_constraint","details":missing})
    for r in routes:
        steps = r["exec_lane"]
        if steps!=REQUIRED_STEPS:
            ok=False
            issues.append({"route_id":r["route_id"],"issue":"exec_lane_mismatch","details":steps})

    report = {
      "title":"CODEX v38 â€” Policy Report",
      "timestamp":datetime.datetime.utcnow().replace(microsecond=0).isoformat()+"Z",
      "ok": ok,
      "issues": issues,
      "notes":{"deterministic":True,"offline":True,"series":"v38"}
    }
    seal("V38_POLICY_REPORT.json", report)
    print("âœ… v38 policy OK" if ok else "âŒ v38 policy FAILED")
    sys.exit(0 if ok else 2)


