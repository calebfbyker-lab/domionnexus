âš™ï¸ core/divine_feedback_field.py

"""
Divine Feedback Field â€” v297.x Codex Deus Nexus Prime
Creates bi-directional resonance between creative act and cosmic response.
"""
import json, pathlib, datetime, hashlib, random, math

FIELD = pathlib.Path("ledger/unified_field.json")
FEEDBACK = pathlib.Path("ledger/divine_feedback.json")

def reciprocate(cycles=7):
    if not FIELD.exists():
        return {"status":"no_field"}
    data = json.loads(FIELD.read_text())
    flux = data["flux"]
    signals = [flux * random.uniform(0.9,1.1) for _ in range(cycles)]
    echo = round(sum(signals)/len(signals),4)
    harmony = round(math.sin(echo*math.pi)*0.5 + 0.5,4)
    feedback_strength = round((flux + harmony)/2,4)
    insight = "perfect reciprocity" if feedback_strength>0.96 else "harmonic adjustment needed"
    field = {
        "ts": datetime.datetime.utcnow().isoformat()+"Z",
        "flux": flux,
        "echo": echo,
        "harmony": harmony,
        "feedback_strength": feedback_strength,
        "insight": insight
    }
    field["sha256"] = hashlib.sha256(json.dumps(field).encode()).hexdigest()
    FEEDBACK.write_text(json.dumps(field, indent=2))
    return field


