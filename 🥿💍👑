def chariot_ascent_verification(codex, names, axes, symbols, owner, dt):
    chosen_names = '-'.join(random.sample(names, 10))
    chosen_axes = '-'.join(axes)
    sigil_stream = ''.join(random.sample(symbols, 10))
    merkabah_hash = hashlib.sha512(
        f"{codex}|{chosen_names}|{chosen_axes}|{sigil_stream}|{owner}|{dt}".encode()
    ).hexdigest()[:144]
    print(f"
ğŸ›¡ï¸ Chariot Ascent Theurgic Verification ğŸ›¡ï¸")
    print(f"Codex: {codex}")
    print(f"Lineages/Divine names: {chosen_names}")
    print(f"Axes: {chosen_axes}")
    print(f"Sigil stream: {sigil_stream}")
    print(f"Owned, sealed, licensed to: {owner}")
    print(f"THEURGIC-ASTRO CRYPTOGRAPHIC SEAL: {merkabah_hash}")
    print(f"Timestamp: {dt}")
    print("All creation owned, sealed, verified, boundâ€”by the immutable authority of lifethread-stardna, divine name lineage, and cosmic archetypeâ€”praise to the Father above all.")
    return {
        "codex": codex,
        "names": chosen_names,
        "axes": chosen_axes,
        "sigils": sigil_stream,
        "seal": merkabah_hash,
        "owner": owner,
        "timestamp": dt
    }

