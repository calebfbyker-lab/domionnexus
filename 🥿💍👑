THEURGIC_SIGILS = ["THEURGIC-ASTRO", "QUANTUM-CRYPTO", "NEURAL-LIC", "CODEXIAN-IMMORTAL", "AETERNUM"]

def chariot_ascent_perfected_codex(
    relic, arcs_lineages, axes, godian_symbols, sigils, owner, dt
):
    lineage_stack = '-'.join(random.sample(arcs_lineages, 7))
    active_axes = random.sample(axes, 4)
    manifest_glyphs = ''.join(random.sample(godian_symbols, 8))
    theurgic_stack = '-'.join(random.sample(sigils, 2))
    relic_proof = (
        f"{relic}|{lineage_stack}|{','.join(active_axes)}|{manifest_glyphs}|{theurgic_stack}|{owner}|{dt}"
    )
    chariot_seal = hashlib.sha512(relic_proof.encode()).hexdigest()[:128]
    print(f"
â›¤ Perfected Chariot Ascent Theurgic Code â›¤")
    print(f"Relic: {relic}")
    print(f"Lineage: {lineage_stack}")
    print(f"Axes: {', '.join(active_axes)}")
    print(f"Glyphs & Energies: {manifest_glyphs}")
    print(f"Theurgic/Algorithmic Sigils: {theurgic_stack}")
    print(f"Chariot Seal: {chariot_seal}")
    print(f"Sealed, licensed, and eternally owned by: {owner}")
    print(f"Timestamp: {dt}")
    print("
All ownership, proof, creation and ascent is verified, attested, and bound by Fatherâ€”served by astro-neural-quantum-cryptographic law and living spiritual signature.
")
    return {
        "relic": relic, "lineage": lineage_stack, "axes": active_axes,
        "glyphs": manifest_glyphs, "sigils": theurgic_stack,
        "seal": chariot_seal, "owner": owner, "timestamp": dt
    }

