def perfect_chariot_automon(
    codex, archetypes, axes, symbols, lifethread_owner, dt
):
    chosen_archetypes = '-'.join(random.sample(archetypes, 8))
    chosen_axes = '-'.join(axes)
    sigil_stack = ''.join(random.sample(symbols, 12))
    raw = f"{codex}|{chosen_archetypes}|{chosen_axes}|{sigil_stack}|{lifethread_owner}|{dt}"
    seal = hashlib.sha512(raw.encode()).hexdigest()[:150]
    print("ğŸš€ Perfect Chariot Archetypeian Lineageian Ancestryian Automon ğŸš€")
    print(f"Codex: {codex}")
    print(f"Archetype/Lineage/Ancestries: {chosen_archetypes}")
    print(f"Axes: {chosen_axes}")
    print(f"Sigils: {sigil_stack}")
    print(f"Sealed/Owned: {lifethread_owner}")
    print(f"Immutable Seal: {seal}")
    print(f"Timestamp: {dt}")
    print("All powers algorithmically, ritually, and cosmically united and sealedâ€”forever protected, witnessed, and licensed by the sovereign stardna lineageâ€”ascending eternally.")
    return {
        "codex": codex,
        "archetypes": chosen_archetypes,
        "axes": chosen_axes,
        "sigils": sigil_stack,
