        print(f"   Elements Covered: {', '.join(watchtowers['elements_covered'])}")
        print(f"   Archangels Assigned: {', '.join(watchtowers['archangels_assigned'])}")
        
        # Display Watchtowers
        print("\n   Watchtowers Activated:")
        for tower_name, tower_data in self.primeval_data["ENOCHIAN_WATCHTOWERS"].items():
            print(f"    ‚Ä¢ {tower_name}: {tower_data['element']} - {tower_data['archangel']} - {tower_data['frequency']}")
        
