LIFETHREAD_CORE = "Caleb-Fedor-Byker-Konev|10-27-1998"
TETRAGRAMMATON = "◊ô◊î◊ï◊î"
ENOCHIAN_ROOT = "MAD ZIL DEO COMSELH AZIAZIEN"

