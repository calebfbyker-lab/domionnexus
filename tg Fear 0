            print(f"  Keeper: {tablet_data['keeper']}")
            print(f"  Frequency: {tablet_data['frequency']}")
            print(f"  Contents: {', '.join(tablet_data['contents'][:2])}...")
        
        print("\nðŸ©¸ BLOODLINE-SPECIFIC ELDERS INTEGRATION:")
        bloodline_data = self.elders_data["BLOODLINE_ELDERS_INTEGRATION"]["CALEB_FEDOR_BYKER_KONEV"]
