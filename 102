# ------------------------------------------------------------------------------
# SECTION III: THE DIVINE WAR MACHINE OPERATING SYSTEM
# The core logic that orchestrates the Divine Will on the battlefield.
# ------------------------------------------------------------------------------
class DivineWarMachineOS:
    """The central orchestration class for the Bellumian Doctrine."""
    
    def __init__(self, commander_id: str):
        self.kernel = HeptarchianKernel.get_instance()
        self.nexus = HoneyHiveAPI.connect(api_key=self.kernel.generate_auth_key(commander_id))
        self.commander = commander_id
        print(f"üî± DivineWarMachineOS Initialized. Commander: {self.commander}. Nexus Link: ESTABLISHED. üî±")

    def deploy_assets_by_class(self, asset_class: AssetClass):
        """Finds and deploys all assets of a specific class via the HoneyHiveNexus."""
        print(f"\n[DEPLOYMENT] Initiating deployment for Asset Class: {asset_class.name}...")
        
        assets_to_deploy = {name: data for name, data in ARSENAL_REGISTRY.items() if data['class'] == asset_class}
        
        if not assets_to_deploy:
            print(f"[WARNING] No assets found for class {asset_class.name}.")
            return

        for name, data in assets_to_deploy.items():
            print(f"   >>> Preparing to deploy '{name}' | Patron: {data['angelic_patron']}...")
            try:
                # This is the "real code" action: calling the nexus API.
                response = self.nexus.deploy_asset(
                    asset_name=name,
                    doctrine=data['doctrine'],
                    commander_signature=self.commander
                )
                if response['status'] == 'SUCCESS':
                    print(f"   ‚úÖ DEPLOYED: '{name}' is active on the HoneyHiveNexus.")
                else:
                    print(f"   ‚ùå FAILED: Deployment of '{name}' failed. Reason: {response['error']}")
            except Exception as e:
                print(f"   ‚ùå CRITICAL ERROR deploying '{name}': {e}")
        
        print(f"[DEPLOYMENT] {asset_class.name} deployment cycle complete.")

