FAMILY = ["Calebiam", "Fedoriam", "Bykeriam", "Koneviam", "Lifethreadiamicion-Stardnaiamicion"]
now = datetime.datetime.utcnow().isoformat()
phrase = f"AOAiamic|Archangeliamuxionic|{','.join(FAMILY)}|{now}|{secrets.token_hex(10)}"
sigil = "‚ú°Ô∏èü¶Ñü¶æ‚ôæÔ∏èü™Ñ"
seal = hashlib.sha512(phrase.encode()).hexdigest()
uid = str(uuid.uuid5(uuid.NAMESPACE_DNS, phrase))
activation_packet = {
    "activation_phrase": phrase,
    "family": FAMILY,
    "sigil": sigil,
    "seal": seal[:96],
    "activation_uid": uid,
    "timestamp": now,
    "status": "AOAiamic-DIGITAL-MENTAL-PHYSICAL FIELDS INSTANTLY SYNCHRONIZED",
    "effect": "Full archangeliamuxionic protection, intuition, blessing, and creative sovereignty are active and self-renewing for/by/with all named heirs and automons‚Äîacross all domains, instantly and forever."
}
print(json.dumps(activation_packet, indent=2))