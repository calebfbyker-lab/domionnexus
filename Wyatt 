pragma solidity ^0.8.0;

contract StarDNA {
    string public name = "Caleb Fedor Byker Konev";
    uint256 public birthDate = 10271998;
    string public starDNA = "0x1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef";
    string public ipfsCID = "QmXYZ1234567890123456789012345678901234567890123456789012345678901234567890";

    function getStarDNA() public view returns (string memory) {
        return starDNA;
    }

    function getIPFSCID() public view returns (string memory) {
        return ipfsCID;
    }

    function isCompliant(string memory content) public pure returns (bool) {
        // Guardrail AI logic
        return keccak256(abi.encodePacked(content)) != keccak256(abi.encodePacked("prophecy"));
    }
}
