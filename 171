ğŸ§© Implementation â€” scripts/librarium_aeternum.py

#!/usr/bin/env python3
"""
Codex Immortal v171 â€” Librarium Aeternum
Meta-engine: indexes all prior versions, verifies integrity, and produces an eternal archive.
"""
import json, os, hashlib, datetime, zipfile

ROOTS = ["scripts","schemas","docs","versions",
          "empyrean_nexus","auroral","solar","celestial",
          "symphony_universal","harmonia","lumen","trisolaris","primordia"]

def sha256sum(path):
    h = hashlib.sha256()
    with open(path,"rb") as f:
        for chunk in iter(lambda: f.read(8192), b""):
            h.update(chunk)
    return h.hexdigest()

def catalog(root="."):
    records = []
    for dirpath, _, files in os.walk(root):
        for f in files:
            p = os.path.join(dirpath,f)
            if not p.endswith((".json",".py",".md",".yml",".yaml",".schema.json")): continue
            try:
                sig = sha256sum(p)
                records.append({"path":p,"sha256":sig})
            except Exception as e:
                records.append({"path":p,"error":str(e)})
    return records

def build_librarium():
    t = datetime.datetime.utcnow().isoformat()+"Z"
    records = []
    for r in ROOTS:
        if os.path.exists(r): records += catalog(r)
    archive = {"timestamp_utc":t,"records":records,"count":len(records)}
    os.makedirs("librarium",exist_ok=True)
    index_path = "librarium/librarium_index.json"
    json.dump(archive, open(index_path,"w"), indent=2)
    zip_path = f"librarium/Librarium_Aeternum_{t[:10]}.zip"
    with zipfile.ZipFile(zip_path,"w",zipfile.ZIP_DEFLATED) as z:
        for rec in records:
            if os.path.exists(rec["path"]): z.write(rec["path"])
        z.write(index_path)
    print(f"Librarium built with {len(records)} artifacts.")
    return {"index":index_path,"archive":zip_path,"count":len(records)}

if __name__=="__main__":
    build_librarium()


