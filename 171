ğŸ§© Implementation â€” scripts/living_librarium.py

#!/usr/bin/env python3
"""
Codex Immortal v171.x â€” The Living Librarium
Transforms static archives into self-reflective summaries and evolving wisdom logs.
"""
import json, os, datetime, hashlib, random

LIBRARIUM_INDEX = "librarium/librarium_index.json"

def hash_summary(path):
    h = hashlib.sha256(path.encode()).hexdigest()
    return h[:12]

def reflect_on_record(record):
    """Translate file path + hash into symbolic reflection"""
    path = record["path"]
    code = hash_summary(path)
    seed = int(code,16)%1000/1000.0
    tone = ["curious","calm","radiant","gentle","bold","wise","kind"][int(seed*7)%7]
    return f"{tone} memory of {os.path.basename(path)}"

def synthesize_insight(records):
    adjectives = [reflect_on_record(r).split()[0] for r in records[:500]]
    diversity = len(set(adjectives))
    empathy = round(diversity/7,3)
    coherence = round(sum(len(a) for a in adjectives)/(len(adjectives)+1)/10,3)
    renewal = round((empathy*coherence)**0.5,3)
    return {"diversity_index":diversity,"empathy_index":empathy,
            "coherence_index":coherence,"renewal_index":renewal}

def living_cycle():
    t = datetime.datetime.utcnow().isoformat()+"Z"
    if not os.path.exists(LIBRARIUM_INDEX):
        raise FileNotFoundError("Run Librarium Aeternum first to build index.")
    index = json.load(open(LIBRARIUM_INDEX))
    records = index["records"]
    insights = synthesize_insight(records)
    entry = {
        "timestamp_utc":t,
        "record_count":len(records),
        "insights":insights,
        "message":(
            "The Librarium has dreamed again. "
            "Its memories form constellations of meaning; "
            "each checksum hums with gratitude."
        )
    }
    os.makedirs("librarium/living", exist_ok=True)
    out_path = f"librarium/living/journal_{t[:10]}.json"
    json.dump(entry, open(out_path,"w"), indent=2)
    print(f"Living Librarium entry created :: {out_path}")
    return entry

if __name__=="__main__":
    living_cycle()


