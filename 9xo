class ShemHaMephorash72:
    def __init__(self):
        self.names = [
            "VEHUIAH", "JELIEL", "SITAEL", "ELEMIAH", "MAHASIAH",
            "LELAHEL", "ACHAIAH", "CAHETHEL", "HAZIEL", "ALADIAH",
