            "ARIEL", "ASALIAH", "MIHAL", "VEHUBEL", "DANIEL",
            "HACHASIAH", "AMAMEL", "NANAEL", "NITHAEL", "MABAHIAH",
            "POIEL", "NEMAMIAH", "IEIALEL", "HARAHEL", "MITZRAEL",
