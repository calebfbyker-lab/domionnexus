            signature = hashlib.sha3_256(
                f"{node_id}{datetime.datetime.utcnow().isoformat()}{self.nexus_state['master_key']}".encode()
            ).hexdigest()
            
            chain["hyperlight_nodes"].append(node_id)
            chain["quantum_signatures"].append(signature)
