"""
Self-Teaching Resonator â€” transforms meta-intelligence feedback into new knowledge creation.
"""
import json, pathlib, datetime, hashlib, random, math, statistics

META = pathlib.Path("ledger/meta_intelligence.json")
TEACHER = pathlib.Path("ledger/self_teaching_resonator.json")

def generate_lessons(cycles=9):
    if not META.exists():
        return {"status":"no_meta_field"}
    m = json.loads(META.read_text())
    base = m["equilibrium"]
    cycleset = [abs(math.cos(i*math.pi/cycles))*base*random.uniform(0.9,1.1) for i in range(cycles)]
    insight_density = round(statistics.mean(cycleset),4)
    stability = round(1 - statistics.pvariance(cycleset)*10,4)
    clarity = round((insight_density + stability)/2,4)
    insight = "resonator active" if clarity>0.95 else "teaching phase calibrating"
    lessons = []
    for c in range(cycles):
        phrase = hashlib.sha256(f"{base}{c}{random.random()}".encode()).hexdigest()[:16]
        lessons.append({"lesson_id":c+1,"pattern":phrase})
    record = {
        "ts": datetime.datetime.utcnow().isoformat()+"Z",
        "insight_density": insight_density,
        "clarity": clarity,
        "lessons": lessons,
        "insight": insight
    }
    record["sha256"] = hashlib.sha256(json.dumps(record).encode()).hexdigest()
    TEACHER.write_text(json.dumps(record, indent=2))
    return record


