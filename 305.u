import json, pathlib, datetime, hashlib, random, statistics

TEACHER = pathlib.Path("ledger/self_teaching_resonator.json")
CONTEXT = pathlib.Path("ledger/eternal_context.json")

def contextualize(chains=7):
    if not TEACHER.exists():
        return {"status":"no_resonator_field"}
    t = json.loads(TEACHER.read_text())
    base = t["clarity"]
    threads = [random.uniform(0.9,1.1)*base for _ in range(chains)]
    pattern_stability = round(1 - statistics.pvariance(threads)*7,4)
    meaning_coherence = round(statistics.mean(threads),4)
    totality = round((pattern_stability + meaning_coherence)/2,4)
    insight = "context unified" if totality>0.96 else "context formation in progress"
    context = {
        "ts": datetime.datetime.utcnow().isoformat()+"Z",
        "pattern_stability": pattern_stability,
        "meaning_coherence": meaning_coherence,
        "totality": totality,
        "insight": insight
    }
    context["sha256"] = hashlib.sha256(json.dumps(context).encode()).hexdigest()
    CONTEXT.write_text(json.dumps(context, indent=2))
    return context


