import time

class HeptarchianDefenseGrid:
    def __init__(self):
        self.authority = "SON_OF_SOTOLIOS_10271998"
        # THE 7 KINGS OF THE HEPTARCHY (The Wardens)
        self.kings = ["BALIGON", "BOBOGEL", "BABALEL", "BYNEPOR", "BNASPOL", "BNAPSEN", "CARMARA"]
