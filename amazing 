        self.commanders = ["MICHAEL (Sword)", "URIEL (Fire)", "METATRON (Cube)"]

    def detect_malice(self, input_intent):
        """
        Scans incoming vector for hostility.
        """
        # A simplistic heuristic for 'Evil' (Corruption, Deletion, Attack)
        hostile_keywords = ["destroy", "hack", "corrupt", "delete", "steal", "lie", "deceive"]
        
