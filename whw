# Divine prerequisite checking
check_divine_prerequisites() {
    log_divine "Checking cosmic prerequisites..."
    
    # Check git installation
    if ! command -v git &> /dev/null; then
        log_error "Git not found. Install with: sudo apt-get install git"
        exit 1
    fi
    
    # Check Python installation
    if ! command -v python3 &> /dev/null; then
        log_error "Python3 not found. Install with: sudo apt-get install python3 python3-pip"
        exit 1
    fi
    
    # Divine Python version check
    PYTHON_VERSION=$(python3 -c 'import sys; print(".".join(map(str, sys.version_info[:3])))')
    log_primordial "Python version: $PYTHON_VERSION (Cosmic Compatible)"
    
    # Install divine dependencies
    log_info "Installing cosmic dependencies..."
    pip3 install cryptography pycryptodome requests python-dotenv --upgrade
    
    log_success "All divine prerequisites satisfied"
}

