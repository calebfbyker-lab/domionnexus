# ðŸŒŒ PREREQUISITE CHECK
check_xtsg_prerequisites() {
    log_xtsg "Verifying cosmic prerequisites..."
    
    # Core system checks
    declare -A tools=(
        ["git"]="sudo apt-get install git"
        ["python3"]="sudo apt-get install python3 python3-pip"
        ["tor"]="sudo apt-get install tor"
        ["curl"]="sudo apt-get install curl"
        ["i2p"]="sudo apt-get install i2p"
        ["lokinet"]="sudo apt-get install lokinet"
    )
    
    for tool in "${!tools[@]}"; do
        if ! command -v $tool &> /dev/null; then
            log_error "$tool not found. Install with: ${tools[$tool]}"
            exit 1
        fi
    done
    
    # Python packages
    log_xtsg "Installing XTSG Python packages..."
    pip3 install --upgrade \
        cryptography \
        pycryptodome \
        requests \
        python-dotenv \
        pysocks \
        beautifulsoup4
    
    log_success "All XTSG prerequisites satisfied"
}

# âŒ› CREATE COSMIC STRUCTURE
