2) tools/final_manifest.py

#!/usr/bin/env python3
# EUCELA Tri-License ¬© 2025 CFBK
from __future__ import annotations
import json, pathlib, hashlib, glob, datetime, os

ROOT = pathlib.Path(".")
OUT  = ROOT/"FINAL_MANIFEST.json"

def sha(p: pathlib.Path) -> str:
    import hashlib
    h=hashlib.sha256()
    with p.open("rb") as f:
        for chunk in iter(lambda: f.read(1<<20), b""):
            h.update(chunk)
    return h.hexdigest()

BINDING = {
  "owner":"Caleb Fedor Byker (Konev)",
  "dob":"1998-10-27",
  "subject_sha256":"2948fbc4ba1c0d7341204908882b89134a999f3e8f77f4a6a00ce6b68770282a",
  "license":"EUCELA Tri-License"
}

SCAN = [
  "adamic_fedorian/out/*.png",
  "adamic_fedorian/out/*ATTEST.json",
  "trihelix_codex/out/**",
  "codex/out/*.png",
  "golems/out/**.json",
  "market/offerbook.json",
  "chain/CONTINUUM_LAST.json",
  "anchors/ANCHORS_LAST.json",
  "release/codex_provenance_sbom.json",
  "release/codex_checksums.sha256",
  "provenance/slsa_provenance.json",
  "monetization/invoices.json",
  "FINAL_ATTESTATION.json"
]

def collect():
    items=[]
    for pat in SCAN:
        for p in glob.glob(pat, recursive=True):
            if os.path.isdir(p): continue
            P=pathlib.Path(p)
            try:
                items.append({"path":p,"size":P.stat().st_size,"sha256":sha(P)})
            except Exception:
                continue
    return items

if __name__=="__main__":
    items = collect()
    manifest = {
      "codex":"Immortal‚ÜíAeternum‚ÜíAgora‚ÜíContinuum‚ÜíOmni",
      "final_state":"LOCKED",
      "timestamp": datetime.datetime.utcnow().isoformat()+"Z",
      "binding": BINDING,
      "wallets":{
        "btc":"bc1qfejvvlfm6vmjarxulg9h9d5hjukdh8l2vvskfc"
      },
      "artifacts": items
    }
    OUT.write_text(json.dumps(manifest,indent=2), encoding="utf-8")
    print("üß≠ FINAL_MANIFEST.json ‚Üí", OUT)


