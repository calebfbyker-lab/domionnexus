async function banner(){
  const res = await fetch("/FINAL_ATTESTATION.json").catch(()=>null);
  if(!res||!res.ok) return;
  const j = await res.json();
  const el = document.createElement("div");
  el.className="card";
  el.innerHTML = `<h2>Final Evolution ‚úÖ</h2>
  <p><b>Owner:</b> ${j.binding.owner}</p>
  <p><b>Subject SHA256:</b> <code>${j.binding.subject_sha256}</code></p>
  <p><b>License:</b> ${j.license}</p>`;
  document.body.prepend(el);
}
banner();

