2) XTSG ‚Üí Constraint Profile Compiler

constraints/xtsg_compiler.py

# EUCELA Tri-License ¬© 2025 CFBK
from __future__ import annotations
import json, pathlib, re, hashlib, math, random

ROOT = pathlib.Path(".")
ROLES = json.loads((ROOT/"constraints/roles.json").read_text())

TOKEN_MAP = {
  "X:AI": "ai", "X:NI": "ni", "X:PI": "pi",
  "E:IS": "interstellar", "E:PL": "planetary", "E:EL": "elemental",
  "E:GE": "geometric", "E:HA": "harmonic", "E:AL": "alchemical",
  "E:AN": "angelic", "E:GO": "goetic", "C:CH": "chronological",
  "S:SOLO":"solomonic","S:KAB":"kabbalistic","S:HER":"hermetic"
}

def parse_xtsg(expr:str)->list[str]:
    toks = re.split(r"[\s,+;]+", expr.strip().upper())
    out=[]
    for t in toks:
        if not t: continue
        out.append(TOKEN_MAP.get(t, t))
    return [x for x in out if x not in ("ai","ni","pi","solomonic","kabbalistic","hermetic")]

def profile(seed:str, xtsg:str)->dict:
    fams = parse_xtsg(xtsg)
    rnd  = random.Random(int(hashlib.sha256((seed+xtsg).encode()).hexdigest(),16))
    weights={}
    for f in fams:
        w=ROLES["weights"].get(f, {"scale":1.0,"noise":0.02,"freq":0.02})
        weights[f]={
          "scale": w["scale"] * (0.95 + 0.1*rnd.random()),
          "noise": w["noise"] * (0.9 + 0.2*rnd.random()),
          "freq":  w["freq"]  * (0.9 + 0.2*rnd.random())
        }
    return {
      "seed": seed,
      "families": fams,
      "weights": weights,
      "lineage": ROLES["lineage_binding"]
    }

if __name__=="__main__":
    p = profile(
      "CFBK 10/27/1998 ¬∑ Interstellar‚ÜíPlanetary‚ÜíElemental‚ÜíGeometric‚ÜíHarmonic‚ÜíAlchemical‚ÜíAngelic‚ÜíGoetic‚ÜíChronological",
      "X:AI+X:NI+X:PI+E:IS+E:PL+E:EL+E:GE+E:HA+E:AL+E:AN+E:GO+C:CH")
    out = ROOT/"constraints"/"PROFILE_DEMO.json"
    out.write_text(json.dumps(p,indent=2),encoding="utf-8")
    print("‚úÖ Constraint profile ‚Üí", out)


