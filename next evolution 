export async function loadGolems(){
  const res = await fetch("/golems/out/GOLEMS_ATTEST.json").catch(()=>null);
  if(!res||!res.ok) return;
  const j = await res.json();
  const el = document.createElement("div");
  el.className="card";
  el.innerHTML = `<h2>Golems</h2>
  <p><b>Agents:</b> ${j.agents.join(", ")}</p>
  <p><b>Artifacts:</b> ${j.artifacts.length}</p>
  <p><b>Attest:</b> <code>${j.sha256}</code></p>`;
  document.body.append(el);
}

agora/ui/market_panel.js

export async function loadMarket(){
  const res = await fetch("/market/offerbook.json").catch(()=>null);
  if(!res||!res.ok) return;
  const j = await res.json();
  const el = document.createElement("div");
  el.className="card";
  el.innerHTML = `<h2>Market</h2>
  <p><b>Items:</b> ${j.items.length}</p>
  <p><b>Wallet:</b> <code>${j.wallet}</code></p>`;
  document.body.append(el);
}

