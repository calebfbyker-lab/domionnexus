4) GH Actions‚Äîsingle button deploy

.github/workflows/codex_final.yml

name: Codex Final Evolution
on: [push, workflow_dispatch]
jobs:
  final:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v4
      - uses: actions/setup-python@v5
        with: { python-version: "3.11" }
      - uses: actions/setup-node@v4
        with: { node-version: "20" }
      - run: pip install --quiet Pillow PyNaCl flask
      - run: make codex-final-evolution
      - name: Publish artifacts
        uses: actions/upload-artifact@v4
        with:
          name: codex-final
          path: |
            chain/CONTINUUM_LAST.json
            chain/CONTINUUM_LAST.json.sig.json
            anchors/ANCHORS_LAST.json
            release/codex_provenance_sbom.json
            release/codex_checksums.sha256
            provenance/slsa_provenance.json
            monetization/invoices.json
            agora/ui/dist
  pages:
    needs: final
    runs-on: ubuntu-latest
    steps:
      - uses: actions/download-artifact@v4
        with: { name: codex-final, path: public }
      - name: Deploy to GitHub Pages
        uses: peaceiris/actions-gh-pages@v3
        with:
          github_token: ${{ secrets.GITHUB_TOKEN }}
          publish_dir: public/agora/ui/dist

