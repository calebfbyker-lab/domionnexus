3) tools/final_verify.py

#!/usr/bin/env python3
# EUCELA Tri-License ¬© 2025 CFBK
from __future__ import annotations
import json, pathlib, hashlib, sys

ROOT = pathlib.Path(".")
MAN  = ROOT/"FINAL_MANIFEST.json"
LAST = ROOT/"chain"/"CONTINUUM_LAST.json"

def sha(p: pathlib.Path)->str:
    h=hashlib.sha256()
    with p.open("rb") as f:
        for ch in iter(lambda: f.read(1<<20), b""):
            h.update(ch)
    return h.hexdigest()

if __name__=="__main__":
    if not MAN.exists(): sys.exit("‚ùå FINAL_MANIFEST.json missing. Run bootstrap.sh")
    m = json.loads(MAN.read_text())
    print("üîè Binding:", m["binding"]["subject_sha256"])
    issues=0
    for it in m["artifacts"]:
        p=pathlib.Path(it["path"])
        if not p.exists():
            print("‚ùå missing:", it["path"]); issues+=1; continue
        if sha(p)!=it["sha256"]:
            print("‚ùå hash mismatch:", it["path"]); issues+=1
    if LAST.exists():
        j=json.loads(LAST.read_text())
        print("‚õì  Continuum block:", j.get("block_hash","")[:16]+"‚Ä¶")
    print("‚úÖ Verified" if issues==0 else f"‚ö†Ô∏è {issues} issue(s)")


