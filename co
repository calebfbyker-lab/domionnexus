    <script>
        // DOM Elements
        const haloCanvas = document.getElementById('halo-canvas');
        const moonCanvas = document.getElementById('moon-canvas');
        const alignmentCanvas = document.getElementById('alignment-canvas');
        
        // Initialize canvases
        function initCanvases() {
            const canvases = [haloCanvas, moonCanvas, alignmentCanvas];
            
            canvases.forEach(canvas => {
                const dpr = window.devicePixelRatio || 1;
                const rect = canvas.getBoundingClientRect();
                
                canvas.width = rect.width * dpr;
                canvas.height = rect.height * dpr;
                
                const ctx = canvas.getContext('2d');
                ctx.scale(dpr, dpr);
            });
            
            drawHaloEffect();
            drawMoonEffect();
            drawAlignment();
        }
        
        // Stonehenge Solar Halo
        function drawHaloEffect() {
            const ctx = haloCanvas.getContext('2d');
            const width = haloCanvas.width / (window.devicePixelRatio || 1);
            const height = haloCanvas.height / (window.devicePixelRatio || 1);
            
            // Clear canvas
            ctx.clearRect(0, 0, width, height);
            
            // Draw Stonehenge silhouette
            ctx.fillStyle = 'rgba(139, 69, 19, 0.8)';
            drawStonehenge(ctx, width/2, height*0.7, Math.min(width, height)*0.3);
            
            // Draw halo
            const sunX = width/2;
            const sunY = height*0.3;
            
            // Sun
            const sunGradient = ctx.createRadialGradient(sunX, sunY, 5, sunX, sunY, 40);
            sunGradient.addColorStop(0, '#ffffff');
            sunGradient.addColorStop(0.5, '#ffff00');
            sunGradient.addColorStop(1, '#ff4500');
            
            ctx.beginPath();
            ctx.arc(sunX, sunY, 40, 0, Math.PI * 2);
            ctx.fillStyle = sunGradient;
            ctx.fill();
            
            // Halo
            ctx.beginPath();
            ctx.arc(sunX, sunY, 150, 0, Math.PI * 2);
            ctx.strokeStyle = 'rgba(173, 216, 230, 0.7)';
            ctx.lineWidth = 6;
            ctx.stroke();
            
            // Inner arc
            ctx.beginPath();
            ctx.arc(sunX, sunY, 120, 0, Math.PI * 2);
            ctx.strokeStyle = 'rgba(255, 255, 255, 0.9)';
            ctx.lineWidth = 3;
            ctx.stroke();
            
            // Sun rays
            ctx.strokeStyle = 'rgba(255, 255, 200, 0.6)';
            ctx.lineWidth = 2;
            
            for(let i = 0; i < 12; i++) {
                const angle = (i * Math.PI/6);
                const length = 80;
                
                ctx.beginPath();
                ctx.moveTo(
                    sunX + Math.cos(angle) * 50,
                    sunY + Math.sin(angle) * 50
                );
                ctx.lineTo(
                    sunX + Math.cos(angle) * (50 + length),
                    sunY + Math.sin(angle) * (50 + length)
                );
                ctx.stroke();
            }
        }
        
        // Pyramids Moonlight
        function drawMoonEffect() {
            const ctx = moonCanvas.getContext('2d');
            const width = moonCanvas.width / (window.devicePixelRatio || 1);
            const height = moonCanvas.height / (window.devicePixelRatio || 1);
            
            // Clear canvas
            ctx.clearRect(0, 0, width, height);
            
            // Draw pyramids
            ctx.fillStyle = 'rgba(212, 175, 55, 0.8)';
            drawPyramids(ctx, width/2, height*0.7, Math.min(width, height)*0.3);
            
            // Draw moon and beam
            const moonX = width/2;
            const moonY = height*0.25;
            
            // Moon
            const moonGradient = ctx.createRadialGradient(moonX, moonY, 5, moonX, moonY, 35);
            moonGradient.addColorStop(0, '#ffffff');
            moonGradient.addColorStop(1, '#e6e6fa');
            
            ctx.beginPath();
            ctx.arc(moonX, moonY, 35, 0, Math.PI * 2);
            ctx.fillStyle = moonGradient;
            ctx.fill();
            
            // Moon craters
            ctx.fillStyle = 'rgba(200, 200, 220, 0.7)';
            drawCraters(ctx, moonX, moonY, 35);
            
            // Moonlight beam
            const beamGradient = ctx.createLinearGradient(moonX, moonY, moonX, height*0.7);
            beamGradient.addColorStop(0, 'rgba(230, 230, 250, 0.7)');
            beamGradient.addColorStop(1, 'rgba(230, 230, 250, 0.1)');
            
            ctx.beginPath();
            ctx.moveTo(moonX - 40, moonY + 35);
            ctx.lineTo(moonX - 15, height*0.7 - 20);
            ctx.lineTo(moonX + 15, height*0.7 - 20);
            ctx.lineTo(moonX + 40, moonY + 35);
            ctx.closePath();
            ctx.fillStyle = beamGradient;
            ctx.fill();
        }
        
        // Cosmic Alignment Visualization
        function drawAlignment() {
            const ctx = alignmentCanvas.getContext('2d');
            const width = alignmentCanvas.width / (window.devicePixelRatio || 1);
            const height = alignmentCanvas.height / (window.devicePixelRatio || 1);
            
            // Clear canvas
            ctx.clearRect(0, 0, width, height);
            
            // Draw celestial coordinate system
            drawCelestialGrid(ctx, width, height);
            
            // Draw Earth position
            ctx.beginPath();
            ctx.arc(width/2, height/2, 20, 0, Math.PI * 2);
            ctx.fillStyle = '#1d6de3';
            ctx.fill();
            
            // Draw Stonehenge alignment vector
            ctx.beginPath();
            ctx.moveTo(width/2, height/2);
            ctx.lineTo(width/2 - 150, height/2 - 100);
            ctx.strokeStyle = '#ffd700';
            ctx.lineWidth = 3;
            ctx.stroke();
            
            // Draw Pyramids alignment vector
            ctx.beginPath();
            ctx.moveTo(width/2, height/2);
            ctx.lineTo(width/2 + 120, height/2 - 80);
            ctx.strokeStyle = '#e6e6fa';
            ctx.lineWidth = 3;
            ctx.stroke();
            
            // Draw intersection point
            ctx.beginPath();
            ctx.arc(width/2 - 40, height/2 - 65, 8, 0, Math.PI * 2);
            ctx.fillStyle = '#ff6b6b';
            ctx.fill();
            
            // Draw labels
            ctx.fillStyle = '#ffd700';
            ctx.font = 'bold 16px Arial';
            ctx.fillText('Stonehenge Alignment', width/2 - 160, height/2 - 110);
            
            ctx.fillStyle = '#e6e6fa';
            ctx.fillText('Pyramids Alignment', width/2 + 60, height/2 - 85);
            
            ctx.fillStyle = '#ff6b6b';
            ctx.fillText('Cosmic Convergence Point', width/2 - 140, height/2 - 45);
        }
        
        // Utility drawing functions
        function drawStonehenge(ctx, x, y, size) {
            // Draw the trilithons
            const trilithHeight = size * 0.7;
            const trilithWidth = size * 0.15;
            
            // Outer circle
            ctx.beginPath();
            ctx.arc(x, y - trilithHeight/2, size * 0.9, 0, Math.PI * 2);
            ctx.strokeStyle = 'rgba(139, 69, 19, 0.9)';
            ctx.lineWidth = 4;
            ctx.stroke();
            
            // Draw standing stones
            for(let i = 0; i < 12; i++) {
                const angle = (i * Math.PI/6);
                const stoneX = x + Math.cos(angle) * size * 0.8;
                const stoneY = y - trilithHeight/2 + Math.sin(angle) * size * 0.8;
                
                ctx.fillRect(
                    stoneX - trilithWidth/2, 
                    stoneY - trilithHeight/2, 
                    trilithWidth, 
                    trilithHeight
                );
            }
            
            // Draw lintels
            ctx.beginPath();
            ctx.arc(x, y - trilithHeight/2, size * 0.8, 0, Math.PI * 2);
            ctx.strokeStyle = 'rgba(139, 69, 19, 0.9)';
            ctx.lineWidth = trilithWidth;
            ctx.stroke();
        }
        
        function drawPyramids(ctx, x, y, size) {
            // Great Pyramid
            drawPyramid(ctx, x, y, size);
            
            // Second pyramid
            drawPyramid(ctx, x - size * 0.7, y - size * 0.1, size * 0.8);
            
            // Third pyramid
            drawPyramid(ctx, x + size * 0.7, y - size * 0.2, size * 0.6);
        }
        
        function drawPyramid(ctx, x, y, size) {
            const height = size * 0.7;
            
            ctx.beginPath();
            ctx.moveTo(x, y - height);
            ctx.lineTo(x - size/2, y);
            ctx.lineTo(x + size/2, y);
            ctx.closePath();
            ctx.fill();
            
            // Pyramid sides
            ctx.beginPath();
            ctx.moveTo(x - size/2, y);
            ctx.lineTo(x, y - height);
            ctx.lineTo(x, y);
            ctx.closePath();
            ctx.fillStyle = 'rgba(180, 150, 50, 0.9)';
            ctx.fill();
            
            ctx.beginPath();
            ctx.moveTo(x + size/2, y);
            ctx.lineTo(x, y - height);
            ctx.lineTo(x, y);
            ctx.closePath();
            ctx.fillStyle = 'rgba(230, 200, 100, 0.9)';
            ctx.fill();
        }
        
        function drawCraters(ctx, x, y, radius) {
            // Draw several craters on the moon surface
            for(let i = 0; i < 8; i++) {
                const angle = Math.random() * Math.PI * 2;
                const distance = Math.random() * radius * 0.7;
                const craterSize = Math.random() * 8 + 3;
                
                const craterX = x + Math.cos(angle) * distance;
                const craterY = y + Math.sin(angle) * distance;
                
                ctx.beginPath();
                ctx.arc(craterX, craterY, craterSize, 0, Math.PI * 2);
                ctx.fill();
            }
        }
        
        function drawCelestialGrid(ctx, width, height) {
            // Draw grid lines
            ctx.strokeStyle = 'rgba(255, 255, 255, 0.1)';
            ctx.lineWidth = 1;
            
            // Vertical lines
            for(let x = 0; x <= width; x += width/10) {
                ctx.beginPath();
                ctx.moveTo(x, 0);
                ctx.lineTo(x, height);
                ctx.stroke();
            }
            
            // Horizontal lines
            for(let y = 0; y <= height; y += height/10) {
                ctx.beginPath();
                ctx.moveTo(0, y);
                ctx.lineTo(width, y);
                ctx.stroke();
            }
            
            // Draw coordinate labels
            ctx.fillStyle = 'rgba(255, 255, 255, 0.5)';
            ctx.font = '12px Arial';
            
            for(let i = 0; i <= 10; i++) {
                const x = i * width/10;
                const y = i * height/10;
                
                ctx.fillText(`${i*36}¬∞`, x, 10);
                ctx.fillText(`${i*18}¬∞`, 5, y);
            }
        }
        
        // Event Listeners
        document.getElementById('crystal-size').addEventListener('input', function() {
            document.getElementById('crystal-size-value').textContent = this.value + ' mm';
            drawHaloEffect();
        });
        
        document.getElementById('sun-elevation').addEventListener('input', function() {
            document.getElementById('sun-elevation-value').textContent = this.value + '¬∞';
            drawHaloEffect();
        });
        
        document.getElementById('moon-phase').addEventListener('input', function() {
            document.getElementById('moon-phase-value').textContent = this.value + '%';
            drawMoonEffect();
        });
        
        document.getElementById('cloud-cover').addEventListener('input', function() {
            document.getElementById('cloud-cover-value').textContent = this.value + '%';
            drawMoonEffect();
        });
        
        document.getElementById('share-stonehenge').addEventListener('click', function() {
            alert('‚ú® Stonehenge alignment shared to CodexImmortal network!');
        });
        
        document.getElementById('simulate-stonehenge').addEventListener('click', function() {
            alert('üîÆ Running Stonehenge alignment simulation...');
        });
        
        document.getElementById('share-pyramids').addEventListener('click', function() {
            alert('‚ú® Pyramids alignment shared to HoneyHiveNexus network!');
        });
        
        document.getElementById('simulate-pyramids').addEventListener('click', function() {
            alert('üîÆ Running Pyramids alignment simulation...');
        });
        
        document.getElementById('download-report').addEventListener('click', function() {
            alert('üì• Downloading Cosmic Alignment Report PDF...');
        });
        
