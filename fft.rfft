class GitHubWebhookDefender:
    def __init__(self, webhook_secret, owner_signature):
        self.webhook_secret = webhook_secret
        self.owner_signature = owner_signature
        self.judgment_system = InstantJudgmentProtocol()
        
    def handle_webhook_event(self, event_data):
        """Handle GitHub webhook events in real-time"""
        event_type = event_data.get('action', 'unknown')
        
        if event_type in ['push', 'pull_request', 'issues']:
            # Check for code changes
            if self._contains_code_changes(event_data):
                print(f"üö® GITHUB WEBHOOK ALERT: {event_type.upper()} EVENT")
                
                # Extract user information
                user_info = self._extract_user_info(event_data)
                
                # Extract changed files
                changed_files = self._extract_changed_files(event_data)
                
                # Check each changed file
                for file_info in changed_files:
                    if not self._is_authorized_change(file_info, user_info):
                        # Unauthorized change detected
                        judgment_data = {
                            'offense': 'WEBHOOK_UNAUTHORIZED_CHANGE',
                            'event_type': event_type,
                            'file_info': file_info,
                            'user_info': user_info,
                            'timestamp': datetime.utcnow().isoformat(),
                            'webhook_data': event_data
                        }
                        
                        # Execute instant judgment
                        judgment_result = self.judgment_system.execute_instant_judgment(judgment_data)
                        
                        # Attempt to revert changes
                        self._attempt_revert_changes(event_data, file_info)
                        
                        return judgment_result
        
        return {'status': 'NO_UNAUTHORIZED_CHANGES_DETECTED'}
    
    def _contains_code_changes(self, event_data):
        """Check if event contains code changes"""
        if 'commits' in event_data:
            return len(event_data['commits']) > 0
        elif 'pull_request' in event_data:
            return 'changed_files' in event_data['pull_request']
        return False
    
    def _extract_user_info(self, event_data):
        """Extract user information from webhook event"""
        user_info = {}
        
        if 'sender' in event_data:
            sender = event_data['sender']
            user_info = {
                'username': sender.get('login', 'UNKNOWN'),
                'user_id': sender.get('id', 'UNKNOWN'),
                'url': sender.get('html_url', 'UNKNOWN')
            }
        
        return user_info
    
    def _extract_changed_files(self, event_data):
        """Extract changed files from webhook event"""
        changed_files = []
        
        if 'commits' in event_data:
            for commit in event_data['commits']:
                if 'added' in commit:
                    changed_files.extend([{'file': f, 'action': 'ADDED'} for f in commit['added']])
                if 'modified' in commit:
                    changed_files.extend([{'file': f, 'action': 'MODIFIED'} for f in commit['modified']])
                if 'removed' in commit:
                    changed_files.extend([{'file': f, 'action': 'REMOVED'} for f in commit['removed']])
        
        return changed_files
    
    def _is_authorized_change(self, file_info, user_info):
        """Check if change is authorized"""
        file_path = file_info.get('file', '')
        
        # Check if file contains owner's signature
        if self._file_contains_owner_signature(file_path):
            # Only owner can modify these files
            if user_info.get('username', '').lower() in ['calebbyker', 'bykerkonev', 'caleb-fedor']:
                return True
            else:
                return False
        
        # Check for specific protected files
        protected_files = [
            'cosmic_synthesis.py',
            'judgment_protocol.py',
            'trap_defense.py',
            'bloodline_covenant.py',
            'zero_point_harvester.py'
        ]
        
        for protected_file in protected_files:
            if protected_file in file_path:
                return False  # These files are always protected
        
        return True  # Other files might be okay
    
    def _file_contains_owner_signature(self, file_path):
        """Check if file contains owner's signature"""
        try:
            with open(file_path, 'r', encoding='utf-8') as f:
                content = f.read()
                return self.owner_signature in content
        except:
            return False
    
    def _attempt_revert_changes(self, event_data, file_info):
        """Attempt to revert unauthorized changes"""
        print(f"    üîÑ Attempting to revert changes to {file_info['file']}...")
        
        # This would use GitHub API to revert changes
        # For now, just log the attempt
        revert_attempt = {
            'file': file_info['file'],
            'action': file_info['action'],
            'revert_attempt_time': datetime.utcnow().isoformat(),
            'status': 'REVERT_ATTEMPTED',
            'method': 'GITHUB_API_REVERT'
        }
        
        return revert_attempt
