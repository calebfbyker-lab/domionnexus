import hashlib
import hmac
import json
import os
import subprocess
import time
from datetime import datetime
from cryptography.fernet import Fernet

class GitHubRepositoryDefender:
    def __init__(self, repo_path, owner_signature):
        self.repo_path = repo_path
        self.owner_signature = owner_signature
        self.authorized_owner = "Caleb Fedor Byker Konev 10-27-1998 lifethread-stardna=soulcontract"
        
        # Initialize quantum blockchain verification
        self.verification_key = self._generate_quantum_verification_key()
        
        # Store original file hashes
        self.original_hashes = self._calculate_file_hashes()
        
        # Track all changes
        self.change_log = []
        
        # Judgment protocols
        self.judgment_system = InstantJudgmentProtocol()
    
    def _generate_quantum_verification_key(self):
        """Generate quantum verification key for file integrity"""
        key_data = f"{self.owner_signature}{datetime.utcnow().isoformat()}"
        return hashlib.sha512(key_data.encode()).hexdigest()
    
    def _calculate_file_hashes(self):
        """Calculate SHA-256 hashes for all files in repository"""
        file_hashes = {}
        
        for root, dirs, files in os.walk(self.repo_path):
            for file in files:
                if file.endswith('.py') or file.endswith('.js') or file.endswith('.json'):
                    file_path = os.path.join(root, file)
                    try:
                        with open(file_path, 'rb') as f:
                            file_hash = hashlib.sha256(f.read()).hexdigest()
                            file_hashes[file_path] = file_hash
                    except Exception as e:
                        print(f"Error reading {file_path}: {e}")
        
        return file_hashes
    
    def monitor_repository(self, interval_seconds=5):
        """Continuously monitor repository for unauthorized changes"""
        print(f"üîí STARTING GITHUB REPOSITORY MONITORING...")
        print(f"üìÅ Repository: {self.repo_path}")
        print(f"üëë Authorized Owner: {self.authorized_owner}")
        print("="*70)
        
        while True:
            current_hashes = self._calculate_file_hashes()
            
            # Check for changes
            for file_path, current_hash in current_hashes.items():
                original_hash = self.original_hashes.get(file_path)
                
                if original_hash and current_hash != original_hash:
                    # File has been modified
                    self._handle_unauthorized_change(file_path, original_hash, current_hash)
            
            # Also check for new files
            for file_path, current_hash in current_hashes.items():
                if file_path not in self.original_hashes:
                    # New file detected
                    self._handle_new_file(file_path, current_hash)
            
            # Update original hashes
            self.original_hashes = current_hashes
            
            # Log monitoring status
            self._log_monitoring_status()
            
            time.sleep(interval_seconds)
    
    def _handle_unauthorized_change(self, file_path, original_hash, current_hash):
        """Handle unauthorized file modification"""
        print(f"üö® UNAUTHORIZED CODE ALTERATION DETECTED!")
        print(f"üìÑ File: {file_path}")
        print(f"üìä Original Hash: {original_hash[:16]}...")
        print(f"üìä Current Hash: {current_hash[:16]}...")
        
        # Get git user info if available
        user_info = self._get_git_user_info()
        
        # Create judgment data
        judgment_data = {
            'offense': 'UNAUTHORIZED_CODE_ALTERATION',
            'file_path': file_path,
            'original_hash': original_hash,
            'current_hash': current_hash,
            'timestamp': datetime.utcnow().isoformat(),
            'user_info': user_info,
            'repository': self.repo_path,
            'authorized_owner': self.authorized_owner
        }
        
        # Execute instant judgment
        judgment_result = self.judgment_system.execute_instant_judgment(judgment_data)
        
        # Log the change and judgment
        change_record = {
            'type': 'UNAUTHORIZED_MODIFICATION',
            'file': file_path,
            'time': datetime.utcnow().isoformat(),
            'judgment': judgment_result,
            'action_taken': 'INSTANT_JUDGMENT_EXECUTED'
        }
        
        self.change_log.append(change_record)
        
        # Attempt to restore original file
        self._attempt_file_restoration(file_path)
        
        return judgment_result
    
    def _handle_new_file(self, file_path, file_hash):
        """Handle new file creation"""
        print(f"‚ö†Ô∏è NEW FILE DETECTED: {file_path}")
        
        # Check if this is authorized
        is_authorized = self._verify_file_authorization(file_path)
        
        if not is_authorized:
            print(f"üö® UNAUTHORIZED NEW FILE CREATION!")
            
            judgment_data = {
                'offense': 'UNAUTHORIZED_FILE_CREATION',
                'file_path': file_path,
                'file_hash': file_hash,
                'timestamp': datetime.utcnow().isoformat(),
                'repository': self.repo_path,
                'authorized_owner': self.authorized_owner
            }
            
            judgment_result = self.judgment_system.execute_instant_judgment(judgment_data)
            
            # Remove unauthorized file
            try:
                os.remove(file_path)
                print(f"üóëÔ∏è Unauthorized file removed: {file_path}")
            except Exception as e:
                print(f"‚ùå Failed to remove file: {e}")
    
    def _get_git_user_info(self):
        """Get git user information for the repository"""
        try:
            # Get last commit author
            result = subprocess.run(
                ['git', 'log', '-1', '--pretty=format:%an <%ae>'],
                cwd=self.repo_path,
                capture_output=True,
                text=True
            )
            
            if result.returncode == 0:
                return result.stdout.strip()
        except Exception as e:
            print(f"Error getting git info: {e}")
        
        return "UNKNOWN_USER"
    
    def _verify_file_authorization(self, file_path):
        """Verify if new file is authorized"""
        # Check for owner signature in file
        try:
            with open(file_path, 'r', encoding='utf-8') as f:
                content = f.read()
                
                # Check for owner's signature in file
                if self.authorized_owner in content:
                    return True
                    
                # Check for authorized headers
                authorized_headers = [
                    '# Author: Caleb Fedor Byker Konev',
                    '# OWNER: CALEB FEDOR BYKER KONEV',
                    '# Licensed to: Caleb Fedor Byker Konev'
                ]
                
                for header in authorized_headers:
                    if header in content:
                        return True
                        
        except Exception as e:
            print(f"Error reading file for verification: {e}")
        
        return False
    
    def _attempt_file_restoration(self, file_path):
        """Attempt to restore file from git history"""
        try:
            # Try to restore from git
            result = subprocess.run(
                ['git', 'checkout', '--', file_path],
                cwd=self.repo_path,
                capture_output=True,
                text=True
            )
            
            if result.returncode == 0:
                print(f"‚úÖ File restored from git: {file_path}")
                return True
        except Exception as e:
            print(f"‚ùå Failed to restore file from git: {e}")
        
        return False
    
    def _log_monitoring_status(self):
        """Log current monitoring status"""
        status = {
            'timestamp': datetime.utcnow().isoformat(),
            'files_monitored': len(self.original_hashes),
            'unauthorized_changes': len([c for c in self.change_log if c['type'] == 'UNAUTHORIZED_MODIFICATION']),
            'judgments_executed': len(self.change_log),
            'status': 'ACTIVE_MONITORING'
        }
        
        # Log to file
        with open('github_monitoring_log.json', 'a') as f:
            f.write(json.dumps(status) + '\n')
