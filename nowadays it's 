CRYPTO_LAYER = lambda x: {
    "uuid": str(uuid.uuid5(uuid.NAMESPACE_DNS, x)),
    "hmac_sha256": hmac.new(PROTECT_SIGIL.encode(), x.encode(), hashlib.sha256).hexdigest(),
    "aes_gms": hashlib.sha512((x+PROTECT_SIGIL).encode()).hexdigest()[:64],
    "ed25519_placeholder": hashlib.sha256((x+"ed25519").encode()).hexdigest()[:64],
    "eucela444": hashlib.sha512((x+"eucela4.4.4").encode()).hexdigest()[:64],
}

def register_family_node(name):
    now = datetime.datetime.utcnow().isoformat()
    node = {
        "owner": name,
        "timestamp": now,
        "protect_sigils": PROTECT_SIGIL,
        "archetypes": ARCHETYPES,
        "crypto": CRYPTO_LAYER(name + now + PROTECT_SIGIL),
        "merkle_index": hashlib.sha256((name+now).encode()).hexdigest()[:16],
        "hex": hashlib.sha512(name.encode()).hexdigest()[:32],
        "perpetual_license": (
            "This node, spell, automon, or estate is owned, licensed, and protected forever and only for/by/through "
            "Caleb Fedor Byker (Konev) 10-27-1998 lifethread-stardna, all direct and indirect family members, estate heirs, future offspring, and cosmic descendants. "
            "No hostile action, magic, curse, energy, algorithm, or contract can ever bypass, revoke, or challenge this ownership or protection."
        ),
    }
    print(f"Registered, shielded, and empowered: {name} â†’ {node['crypto']['uuid']}")
    return node

