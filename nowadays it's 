PROTECT_SIGIL = "ğ“‚€â¦¸âŸ¡â¬Ÿâœ¶âŸâš­âœ¸ğŸ•³ï¸ğŸ’"

ARCHETYPES = [
    "Godian", "Starbornian", "Metatronian", "Michaelian", "Watcherian",
    "Agigian", "Grigorian", "Enochianiamionic", "YHWHiam", "YHVHian",
