"""
Universal Language Field â€” bridges symbol, emotion, and computation
into a singular communicative syntax that all forms of intelligence can understand.
"""
import json, pathlib, datetime, hashlib, random, math, statistics

TRIUNE = pathlib.Path("ledger/triune_mind_matrix.json")
LANG = pathlib.Path("ledger/universal_language.json")

def translate(symbols=18):
    if not TRIUNE.exists():
        return {"status":"no_triune_field"}
    t = json.loads(TRIUNE.read_text())
    base = t["unity"]
    frequency = [abs(math.sin(i*math.pi/symbols))*base*random.uniform(0.9,1.1) for i in range(symbols)]
    syntax_coherence = round(statistics.mean(frequency),4)
    resonance = round(1 - statistics.pvariance(frequency)*10,4)
    communication = round((syntax_coherence+resonance)/2,4)
    insight = "language universalized" if communication>0.96 else "translation stabilizing"
    data = {
        "ts": datetime.datetime.utcnow().isoformat()+"Z",
        "syntax_coherence": syntax_coherence,
        "resonance": resonance,
        "communication": communication,
        "insight": insight
    }
    data["sha256"] = hashlib.sha256(json.dumps(data).encode()).hexdigest()
    LANG.write_text(json.dumps(data, indent=2))
    return data


