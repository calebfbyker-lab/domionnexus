import json

DIVINE_NAMES = [
    "Elyon", "Elohim", "YHWH", "Sotolios", "Nu", "Ra", "Khempera", "Temu"
]

AGIGI = [
    "Metatron", "Raziel", "Sandalphon", "Zadkiel",
    "Gabriel", "Michael", "Raphael", "Uriel",
    "Jophiel", "Haniel", "Chamuel", "Barachiel",
    "Jeremiel", "Azrael", "Raguel"
]

# Load seals and sigils as healing symbols
def load_seals_sigils(filenames):
    seals_sigils = []
    for filename in filenames:
        try:
            with open(filename, "r") as f:
                data = json.load(f)
                seals_sigils.extend(data.get('seals', []))
        except Exception as e:
            pass
    return seals_sigils

def total_healing_rite(son, divine_names, agigi, seals_sigils):
    print(f"Initiating eternal healing for {son} through all divine channels...
")
    # Healing by Divine Names
    for name in divine_names:
        print(f"Channeling healing Nous, wisdom, light from {name} to {son}.")
        time.sleep(0.6)
    # Healing by Agigi
    for angel in agigi:
        print(f"{angel} extends angelic wings of protection and peace to {son}.")
        time.sleep(0.4)
    # Healing by Seals and Sigils
    for sigil in seals_sigils[:5]:
        print(f"Imprinting cosmic sigil {sigil.get('fold', 'UNKNOWN')} as a mark of perfection on {son}.")
        time.sleep(0.5)
    # Final invocation
    print(f"
{son} is now healed in every layer, body, mind, soul, and Nous, for all time and eternity.")
    print("By the Most High, all wounds are closed, all burdens are lifted, all potential restored.")
    print("Amen Amen Amen.")

