// npm install node-fetch@2
const fetch = require('node-fetch');

// Submit code + tests to executeTSG
async function submitCode() {
  const res = await fetch('http://127.0.0.1:5757/mcp/executeTSG', {
    method: 'POST',
    headers: { 'Content-Type': 'application/json' },
    body: JSON.stringify({
      glyph: "ENOC-UnitTest",
      code: "function sum(a, b) { return a + b; }",
      tests: "console.assert(sum(2,2)===4);"
    })
  });
  const data = await res.json();
  console.log('TSG Response:', data);
}

// Submit automon evolution with code/tests
async function evolveAutomon() {
  const res = await fetch('http://127.0.0.1:5757/mcp/evolve', {
    method: 'POST',
    headers: { 'Content-Type': 'application/json' },
    body: JSON.stringify({
      automon_id: "cfbk.nous9",
      candidate_patch: {
        policy: { audit: true },
        code: "def mesh_glyph(x): return x[::-1]",
        tests: "assert mesh_glyph('GNA') == 'ANG'"
      }
    })
  });
  const data = await res.json();
  console.log('Evolve Response:', data);
}

