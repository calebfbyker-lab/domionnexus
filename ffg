}

# Pharmaceutical Database
PHARMACEUTICAL_FORMULATIONS = {
    "SUBOXONE": {
        "standard": ["Buprenorphine/Naloxone", "Suboxone Film", "Suboxone Tablet"],
        "extended_release": ["Buprenorphine XR", "Sublocade", "Brixadi"],
        "metabolites": ["Norbuprenorphine", "Buprenorphine-glucuronide"],
        "receptor_sites": ["Œº-opioid", "Œ∫-opioid", "Œ¥-opioid", "ORL-1"],
