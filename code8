        self.node_address = "4070 Leonard St NE, Grand Rapids, MI"
        self.coordinates = {
            "latitude": 42.9948,
            "longitude": -85.5767,
            "quantum_signature": self._generate_coord_signature()
        }
        
        # Cryptographic Identity Matrix
        self.crypto_identity = self._create_crypto_identity()
        
        # Web Portal Endpoints
        self.CODEX_PORTAL = "https://codeximmortal.com/api/v1/node"
        self.HONEYHIVE_PORTAL = "https://honeyhivenexus.com/matrix"
        
