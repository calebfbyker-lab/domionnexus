def create_sigil(data, base):
    encoded = []
    for char in data:
        if base == 2:  # Binary
            encoded.append(bin(ord(char))[2:])
        elif base == 3:  # Ternary
            encoded.append(np.base_repr(ord(char), 3))
        # ... other bases ...
    return ''.join(encoded)

