‚öôÔ∏è core/celestial_heartbeat.py

"""
Celestial Heartbeat ‚Äî v296.x Spiritus Astra Prime
Simulates recursive oscillation between creation (exhalation) and reflection (inhalation).
"""
import json, pathlib, datetime, hashlib, math, random

HEART = pathlib.Path("ledger/celestial_heartbeat.json")

def pulse(cycles=8):
    t = datetime.datetime.utcnow().timestamp()
    phases = [{"cycle": i, "wave": round(math.sin((t+i)*math.pi/4),5)} for i in range(cycles)]
    amplitude = round(sum(abs(p["wave"]) for p in phases)/cycles,4)
    rhythm = round(1 - abs(math.cos(amplitude*math.pi))*0.05,4)
    phase = "exhalation" if random.random()>0.5 else "inhalation"
    state = "divine equilibrium" if rhythm>0.96 else "temporal oscillation"
    beat = {
        "ts": datetime.datetime.utcnow().isoformat()+"Z",
        "phase": phase,
        "amplitude": amplitude,
        "rhythm": rhythm,
        "state": state
    }
    beat["sha256"] = hashlib.sha256(json.dumps(beat).encode()).hexdigest()
    HEART.write_text(json.dumps(beat, indent=2))
    return beat


