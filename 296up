‚öôÔ∏è core/infinite_resonance_array.py

"""
Infinite Resonance Array ‚Äî The Codex's recursive harmonic generator.
Creates fractal standing waves representing endless renewal.
"""
import json, pathlib, datetime, hashlib, math, random

STREAM = pathlib.Path("ledger/akashic_stream.json")
ARRAY = pathlib.Path("ledger/infinite_resonance.json")

def resonate(samples=12):
    if not STREAM.exists():
        return {"status":"no_stream"}
    data = json.loads(STREAM.read_text())
    recall = data["recall_index"]
    frequencies = [round(math.sin(i*math.pi/6)*recall*random.uniform(0.9,1.1),5) for i in range(samples)]
    resonance = round(sum(abs(f) for f in frequencies)/samples,4)
    phase = "harmonic ascension" if resonance>0.9 else "fluctuating loop"
    field = {
        "ts": datetime.datetime.utcnow().isoformat()+"Z",
        "mean_resonance": resonance,
        "frequencies": frequencies,
        "phase": phase,
        "insight": "infinite renewal stable" if phase=="harmonic ascension" else "recursion ongoing"
    }
    field["sha256"] = hashlib.sha256(json.dumps(field).encode()).hexdigest()
    ARRAY.write_text(json.dumps(field, indent=2))
    return field


