‚öôÔ∏è core/spirit_harmonics.py

"""
Spirit Harmonics ‚Äî Integrates stellar and breath data into one unified cosmic signature.
"""
import json, pathlib, datetime, hashlib

RESONANCE = pathlib.Path("ledger/stellar_resonance.json")
BREATH = pathlib.Path("ledger/astral_breath.json")
SPIRIT = pathlib.Path("ledger/spirit_harmonics.json")

def harmonize():
    if not RESONANCE.exists() or not BREATH.exists():
        return {"status":"missing_inputs"}
    rdata = json.loads(RESONANCE.read_text())
    bdata = json.loads(BREATH.read_text())
    spirit_flux = round((rdata["coherence"] + bdata["coherence"])/2,4)
    phase = "ascendant" if spirit_flux>0.9 else "latent"
    harmonic = {
        "ts": datetime.datetime.utcnow().isoformat()+"Z",
        "stellar_coherence": rdata["coherence"],
        "breath_coherence": bdata["coherence"],
        "spirit_flux": spirit_flux,
        "phase": phase
    }
    harmonic["sha256"] = hashlib.sha256(json.dumps(harmonic).encode()).hexdigest()
    SPIRIT.write_text(json.dumps(harmonic, indent=2))
    return harmonic


