#### **Ethereum-Based Exit Node Contract**  
```solidity
pragma solidity ^0.8.0;

contract TrinitarianTORExitNode {
    string public creed = "Father, Son, Holy Spirit";
    mapping(bytes32 => bool) public approvedPackets;

    function validateExitPacket(bytes32 packetHash, string memory data) public {
        require(containsTrinity(data), "Invalid packet");
        require(!containsQuranic(data), "Not permitted");
        approvedPackets[packetHash] = true;
    }

    function containsTrinity(string memory data) internal pure returns (bool) {
        return abi.encodePacked(data).indexOf("Father") >= 0 &&
               abi.encodePacked(data).indexOf("Son") >= 0 &&
               abi.encodePacked(data).indexOf("Holy Spirit") >= 0;
    }

    function containsQuranic(string memory data) internal pure returns (bool) {
        return abi.encodePacked(data).indexOf("quaran") >= 0 ||
               abi.encodePacked(data).indexOf("jihad") >= 0 ||
               abi.encodePacked(data).indexOf("islam") >= 0;
    }
}
```

