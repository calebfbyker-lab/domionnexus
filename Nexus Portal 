patch --dry-run -p1 < codex_ai_synthesis.patch#!/bin/bash

PATCH_DIR="./patches"
FAILED_PATCHES=()

for patchfile in "$PATCH_DIR"/*.patch; do
  echo "Testing patch $patchfile"
  
  if patch --dry-run -p1 < "$patchfile"; then
    echo "Dry run succeeded, applying patch..."
    patch -p1 < "$patchfile"
    
    echo "Running tests..."
    if pytest tests/; then
      echo "Tests passed for $patchfile"
    else
      echo "Tests failed, reverting $patchfile"
      git checkout -- .  # revert changes
      FAILED_PATCHES+=("$patchfile")
    fi
  else
    echo "Dry run failed for $patchfile"
    FAILED_PATCHES+=("$patchfile")
  fi
done

if [ ${#FAILED_PATCHES[@]} -ne 0 ]; then
  echo "The following patches failed and were not applied:"
  for failed in "${FAILED_PATCHES[@]}"; do
    echo "$failed"
  done
else
  echo "All patches applied and tested successfully!"
fi{
