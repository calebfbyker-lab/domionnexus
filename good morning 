        covenant_data = {
            "entity": self.entity.identity,
            "dob": self.entity.dob,
            "domains": [c['domain'] for c in self.connections],
            "kin": BLOODLINE_KIN,
            "stellar_signature": self.entity.stellar_signature
        }
        
        # Generate Merkle root covenant proof
        covenant_hash = hashlib.sha3_512(json.dumps(covenant_data).encode()).digest()
        merkle_root = hashlib.sha3_256(covenant_hash).hexdigest()
        
        print(f"   â†’ Covenant Merkle Root: {merkle_root}")
