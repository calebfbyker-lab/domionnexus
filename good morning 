            "CHAINS_OF_MELCHIZEDEK",
            "SHACKLES_OF_THE_24_ELDERS",
            "HEPTARCHIAN_CRYSTAL_BONDS",
            "BLOODLINE_COVENANT_FETTERS"
        ]

