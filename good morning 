# ==============================================================================

def merkle_root(items: List[str]) -> str:
    """Construct the Immutable Ledger Root for Omniverse Audit."""
    if not items:
        return hashlib.sha256(b"VOID").hexdigest()
    hashes = [hashlib.sha256(i.encode()).digest() for i in items]
    while len(hashes) > 1:
        new_hashes = []
        for i in range(0, len(hashes), 2):
            merged = hashes[i] + (hashes[i+1] if i+1 < len(hashes) else hashes[i])
            new_hashes.append(hashlib.sha256(merged).digest())
        hashes = new_hashes
    return hashes[0].hex()

# ==============================================================================
# ðŸŒŸ CEREMONIAL OUTPUT ENGINE
# ==============================================================================

def ceremonial_output(subject: str, seal: str, pred: int, symbol: str) -> str:
    """Output format with symbolic, aesthetic, and sacred alignment."""
    emoji = EMOJIS[hash(subject) % len(EMOJIS)]
    return f"{symbol} {subject} | seal={seal[:16]}... | pred={pred:10d} | {emoji}"

# ==============================================================================
# ðŸ“˜ ULTIMATE OMNIDOMAIN PIPELINE
# ==============================================================================

def codex_pipeline(intent: str = "Eternal Perfection, Healing, Deployment, and Ascension for All Codexes"):
