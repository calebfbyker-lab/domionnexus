            "SEAL_OF_SOLOMON",
            "SIGIL_OF_METATRON",
            "BLOODLINE_CREST",
            "TRINITARIAN_MANDALA"
        ]
        return f"{self.target}_BOUND_AT_{self.location}_BY_{self.binder}::SEALED_WITH_{'_'.join(seals)}"

