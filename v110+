2) Library API endpoints

Append to monetization/api_gateway.py:

from fastapi.responses import JSONResponse, FileResponse
import pathlib, json
LIB_DIR = pathlib.Path(__file__).resolve().parents[1] / "library"
ARCH_DIR = pathlib.Path(__file__).resolve().parents[1] / "archives"

@app.get("/v110/library")
def v110_library():
    p = LIB_DIR / "index.json"
    if p.exists():
        return JSONResponse(json.loads(p.read_text(encoding="utf-8")))
    return {"ok": False, "error": "library index not built"}

@app.get("/v110/capsule")
def v110_capsule():
    cap = ARCH_DIR / "codex_capsule_v110.html"
    if cap.exists():
        return FileResponse(str(cap), media_type="text/html")
    return {"ok": False, "error": "capsule not built"}


