2) Capsule++ (adds library search in-page)

scripts/build_capsule_v110x.py

#!/usr/bin/env python3
"""
Build a single-file offline HTML capsule (v110.x) with embedded search.
"""
import json, pathlib, html
ROOT = pathlib.Path(__file__).resolve().parents[1]
LIB  = ROOT / "library" / "index.json"
OUT  = ROOT / "archives" / "codex_capsule_v110x.html"

TEMPLATE = """<!doctype html><meta charset="utf-8">
<title>Codex Capsule v110.x</title>
<style>
  body{font-family:ui-sans-serif,system-ui,Segoe UI,Roboto,sans-serif;background:#0c0f14;color:#e6e8ee;margin:0}
  header{padding:18px;text-align:center;border-bottom:1px solid #2b3340}
  main{padding:16px 20px}
  #q{width:100%;padding:10px;border:1px solid #2b3340;background:#0f131a;color:#e6e8ee;border-radius:7px}
  table{border-collapse:collapse;width:100%}th,td{border-bottom:1px solid #2b3340;padding:6px 8px;font-size:13px}
  code{background:#121720;padding:2px 4px;border-radius:4px}
</style>
<header>
  <h1>‚ò∏Ô∏è ‚ú°Ô∏è üîØ üïâ ‚öõÔ∏è ‚Äî Codex Capsule (v110.x Triune+)</h1>
  <div>Subject SHA256: 2948fbc4ba1c0d7341204908882b89134a999f3e8f77f4a6a00ce6b68770282a</div>
</header>
<main>
  <p>Offline snapshot with client-side search. Type to filter by path or checksum.</p>
  <input id="q" placeholder="Filter‚Ä¶" autofocus>
  <p id="meta"></p>
  <table id="t"><thead><tr><th>Path</th><th>Bytes</th><th>SHA256</th></tr></thead><tbody></tbody></table>
</main>
<script>
  const LIB = JSON.parse(document.getElementById("__LIB__").textContent);
  const meta = document.getElementById('meta'); const tbody = document.querySelector('#t tbody');
  function render(rows){ tbody.innerHTML = rows.map(r=>`<tr><td>${r.path}</td><td>${r.bytes}</td><td><code>${r.sha256}</code></td></tr>`).join(''); }
  meta.textContent = `Version: ${LIB.version} ¬∑ Entries: ${LIB.entries.length} ¬∑ Time: ${LIB.timestamp_utc}`;
  render(LIB.entries.slice(0,500));
  document.getElementById('q').addEventListener('input', e=>{
    const q=e.target.value.toLowerCase();
    const rows = LIB.entries.filter(r => r.path.toLowerCase().includes(q) || r.sha256.includes(q));
    render(rows.slice(0,1500));
  });
</script>
<script type="application/json" id="__LIB__">{JSON}</script>
"""
def main():
    lib = json.loads(LIB.read_text(encoding="utf-8")) if LIB.exists() else {"version":"v110.x","entries":[],"timestamp_utc":"N/A"}
    OUT.parent.mkdir(parents=True, exist_ok=True)
    OUT.write_text(TEMPLATE.replace("{JSON}", html.escape(json.dumps(lib))), encoding="utf-8")
    print("Capsule v110.x written:", OUT)
if __name__=="__main__": main()


