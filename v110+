1) Library indexer (technical library)

modules/library/indexer.py

# modules/library/indexer.py ‚Äî v110 Triune: builds a canonical library index
import json, hashlib, pathlib, time

ROOT = pathlib.Path(__file__).resolve().parents[2]
LIB  = ROOT / "library"
GLOBS = [
    "manifest.json",
    "provenance/*.json",
    "provenance/*.sha256",
    "provenance/*.log",
    "archives/*.zip",
    "archives/*.json",
    "output/*.json",
    "output/*.svg",
    "output/*.graphml",
    "docs/*.md",
    "assets/*.json",
    "site/*.html",
    "site/*.js",
    "site/*.css"
]

def sha256_file(p: pathlib.Path) -> str:
    h = hashlib.sha256()
    with open(p, "rb") as f:
        for chunk in iter(lambda: f.read(8192), b""): h.update(chunk)
    return h.hexdigest()

def build_index():
    LIB.mkdir(parents=True, exist_ok=True)
    entries = []
    for pattern in GLOBS:
        for p in ROOT.glob(pattern):
            if p.is_file():
                entries.append({
                    "path": str(p.relative_to(ROOT)).replace("\\","/"),
                    "bytes": p.stat().st_size,
                    "sha256": sha256_file(p)
                })
    index = {
        "title": "Codex Continuum Library Index",
        "version": "v110",
        "timestamp_utc": time.strftime("%Y-%m-%dT%H:%M:%SZ", time.gmtime()),
        "subject_sha256": "2948fbc4ba1c0d7341204908882b89134a999f3e8f77f4a6a00ce6b68770282a",
        "entries": sorted(entries, key=lambda e: e["path"])
    }
    (LIB / "index.json").write_text(json.dumps(index, indent=2), encoding="utf-8")
    return index

if __name__ == "__main__":
    out = build_index()
    print("Library entries:", len(out["entries"]))


