1) Library search + sitemap + feed

modules/library/search.py

# v110.x Triune+ ‚Äî tiny in-memory search over library/index.json
import json, pathlib, re

ROOT = pathlib.Path(__file__).resolve().parents[2]
LIB  = ROOT / "library" / "index.json"

def _load():
    return json.loads(LIB.read_text(encoding="utf-8")) if LIB.exists() else {"entries":[]}

def search(q:str, limit:int=200):
    idx = _load()
    pat = re.compile(re.escape(q), re.I)
    hits = [e for e in idx["entries"] if pat.search(e["path"]) or pat.search(e["sha256"])]
    return hits[:limit]

API endpoints (add to monetization/api_gateway.py)

from modules.library.search import search as libsearch
from fastapi.responses import PlainTextResponse
import pathlib, json, datetime

@app.get("/v110.x/search")
def v110x_search(q: str = "", limit: int = 200):
    return {"ok": True, "q": q, "results": libsearch(q, limit)}

@app.get("/v110.x/sitemap.txt", response_class=PlainTextResponse)
def v110x_sitemap_txt():
    root = pathlib.Path(__file__).resolve().parents[1]
    files = [
        "site/frontispiece.html",
        "archives/codex_capsule_v110.html",
        "library/index.json",
    ]
    lines = [f"/{p}" for p in files if (root / p).exists()]
    return "\n".join(lines)

@app.get("/v110.x/feed.json")
def v110x_feed():
    lib = json.loads((pathlib.Path(__file__).resolve().parents[1]/"library"/"index.json").read_text(encoding="utf-8"))
    now = datetime.datetime.utcnow().strftime("%Y-%m-%dT%H:%M:%SZ")
    latest = sorted(lib.get("entries",[]), key=lambda e: e["path"])[-10:] if lib.get("entries") else []
    return {"version":"https://jsonfeed.org/version/1",
            "title":"Codex Continuum v110.x feed",
            "home_page_url":"/site/frontispiece.html",
            "items":[{"id":e["sha256"],"url":f"/{e['path']}", "title":e["path"], "date_modified": now} for e in latest]}


