class ExpandedFamilyNFT(FamilyCovenantNFT):
    def __init__(self, members):
        self.members = members
        self.nft_metadata = self.create_expanded_metadata()
        self.quantum_seal = family_network.network_seal
        
    def create_expanded_metadata(self):
        """Create NFT metadata for full family covenant"""
        attributes = []
        for member in self.members:
            attributes.append({
                "trait_type": "Family Member",
                "value": f"{member['name']} ({member['birthdate']})"
            })
        
        return {
            "name": "Abrahamic Covenant: Byker Family",
            "description": "Multi-generational family covenant NFT",
            "image": "ipfs://QmR9tH5J7kP3LmN2B4V6C8X9Z1A",
            "attributes": attributes + [
                {"trait_type": "Quantum Seal", "value": self.quantum_seal[:24]},
                {"trait_type": "Entanglement Score", "value": "100%"},
                {"trait_type": "Bloodline", "value": "Abrahamic Covenant"},
