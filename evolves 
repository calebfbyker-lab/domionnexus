const licenseTerms = {
    commercialUseAllowed: true,
    derivativesAllowed: true,
    redistributionAllowed: true,
    attributionRequired: "Must attribute to Abrahamic Bloodline Covenant NFT",
    licenseFee: ethers.utils.parseEther("0.144") // 0.144 ETH
};

