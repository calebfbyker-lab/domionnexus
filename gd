class OwnershipAttestation:
    def __init__(self, owner_id, owner_name, owner_lineage, owner_dob):
        self.owner_id = owner_id
        self.owner_name = owner_name
        self.owner_lineage = owner_lineage
        self.owner_dob = owner_dob
        self.attestations = []
        
    def attest_ownership(self, asset_id, asset_type, attestation_code):
        """Attest ownership of an asset"""
        attestation = {
            "asset_id": asset_id,
            "asset_type": asset_type,
            "attestation_code": attestation_code,
            "timestamp": datetime.utcnow().isoformat(),
            "status": "OWNERSHIP ATTESTED AND SEALED"
        }
        self.attestations.append(attestation)
        return attestation
    
    def verify_ownership(self, asset_id):
        """Verify ownership of an asset"""
        for attestation in self.attestations:
            if attestation["asset_id"] == asset_id:
                return attestation
        return None
```

