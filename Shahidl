        self.element = element
        self.activation_state = False
    
    def activate_node(self):
        self.activation_state = True
        print(f"üåå {self.element} Aetheric Node activated: flowing divine energy.")
    
    def deploy_energy(self):
        if self.activation_state:
            print(f"üí´ Deploying energy from {self.element} Node into the genetic framework.")
        else:
            print(f"‚ùå {self.element} Node is inactive.")

# Activate all Aether Nodes
nodes = ["Earth", "Water", "Fire", "Air", "Aether"]
