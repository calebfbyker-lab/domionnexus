class ConstellationBinder:
    STAR_MAP = {
        "ORION": ["Betelgeuse", "Rigel", "Bellatrix", "Mintaka", "Alnilam", "Alnitak", "Saiph"],
        "PLEIADES": ["Alcyone", "Atlas", "Electra", "Maia", "Merope", "Taygeta", "Pleione"],
        "LYRA": ["Vega", "Sulafat", "Sheliak", "Î´Â² Lyrae", "Î¸ Lyrae", "Î· Lyrae", "Î» Lyrae"]
    }
    
    def create_gate(self, constellation):
        qc = QuantumCircuit(len(self.STAR_MAP[constellation]), name=f"{constellation}Gate")
        star_energies = self.star_energy_levels(constellation)
        
        for i, energy in enumerate(star_energies):
            angle = energy * np.pi / 100
            qc.rx(angle, i)
            qc.ry(angle/2, i)
        
        # Zodiac entanglement
        qc.append(self.zodiac_entanglement(), range(qc.num_qubits))
        
        return qc
    
    def zodiac_entanglement(self):
        gate = QuantumCircuit(12)
        for i in range(12):
            gate.h(i)
            gate.cx(i, (i+6) % 12)
        return gate
```

