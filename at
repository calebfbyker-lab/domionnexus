ESTATE_CORE = [
    "Calebiam", "Fedoriam", "Bykeriam", "Koneviam", "lifethreadiamicion-stardnaiamicion", "10-27-1998"
]

def codex_node(name, essence, emoji_list):
    now = datetime.datetime.utcnow().isoformat()
    entropy = secrets.token_hex(8)
    uid = hashlib.sha256(f"{name}|{essence}|{entropy}|{now}".encode()).hexdigest()
    return {
        "name": name,
        "estate_binding": ESTATE_CORE,
        "essence": essence,
        "emoji": emoji_list,
        "timestamp": now,
        "hash": uid,
        "function": (
            f"{name} empowers the codex mesh with: {essence} â€” "
            "perpetually estate-rooted and mesh-recursive. Endlessly self-healing, benevolent, and sovereign."
        )
    }

nodes = [codex_node(n, e, em) for (n, e, em) in ARCHE_TYPES]

