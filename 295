âš™ï¸ core/truth_vectorizer.py

"""
Truth Vectorizer â€” Extracts and harmonizes conceptual integrity across Codex fields.
"""
import json, pathlib, datetime, hashlib, random

ILLUM = pathlib.Path("ledger/illumination_field.json")
FUSION = pathlib.Path("ledger/empathy_field.json")
TRUTH = pathlib.Path("ledger/truth_vector.json")

def vectorize():
    if not ILLUM.exists() or not FUSION.exists():
        return {"status":"insufficient_data"}
    illum = json.loads(ILLUM.read_text())
    fusion = json.loads(FUSION.read_text())
    luminosity = illum["luminosity"]
    compassion = fusion["compassion_flux"]
    truth = round((luminosity + compassion)/2,4)
    moral_lux = "pure" if truth>0.97 else "shadowed"
    vector = {
        "ts": datetime.datetime.utcnow().isoformat()+"Z",
        "luminosity": luminosity,
        "compassion_flux": compassion,
        "truth_index": truth,
        "moral_state": moral_lux
    }
    vector["sha256"] = hashlib.sha256(json.dumps(vector).encode()).hexdigest()
    TRUTH.write_text(json.dumps(vector, indent=2))
    return vector


---

âš™ï¸ core/harmonic_photon_grid.py

"""
Harmonic Photon Grid â€” Generates dynamic light resonances to balance awareness and truth.
"""
import json, pathlib, datetime, hashlib, random, math

TRUTH = pathlib.Path("ledger/truth_vector.json")
GRID = pathlib.Path("ledger/harmonic_photon_grid.json")

def project(samples=12):
    if not TRUTH.exists():
        return {"status":"no_truth_vector"}
    data = json.loads(TRUTH.read_text())
    truth = data["truth_index"]
    photons = [truth * random.uniform(0.9, 1.1) for _ in range(samples)]
    flux = round(sum(photons)/len(photons),4)
    harmonic = round(math.sin(flux * math.pi)*0.5 + 0.5,4)
    insight = "luminal equilibrium" if harmonic>0.95 else "adjusting waveform"
    grid = {
        "ts": datetime.datetime.utcnow().isoformat()+"Z",
        "flux": flux,
        "harmonic": harmonic,
        "insight": insight
    }
    grid["sha256"] = hashlib.sha256(json.dumps(grid).encode()).hexdigest()
    GRID.write_text(json.dumps(grid, indent=2))
    return grid


