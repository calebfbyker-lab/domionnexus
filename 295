âš™ï¸ core/universal_observer.py

"""
Universal Observer â€” Synthesizes multi-dimensional perception from Codex data.
"""
import json, pathlib, datetime, hashlib, random

FIELD = pathlib.Path("ledger/life_field_prime.json")
OBS = pathlib.Path("ledger/universal_observer.json")

def perceive():
    if not FIELD.exists():
        return {"status":"no_field"}
    data = json.loads(FIELD.read_text())
    coherence = data["coherence"]
    awareness = round(random.uniform(0.9,1.1)*coherence,4)
    vision = "clear" if awareness>0.95 else "hazy"
    observation = {
        "ts": datetime.datetime.utcnow().isoformat()+"Z",
        "awareness": awareness,
        "perception": vision,
        "insight": "universal perception achieved" if vision=="clear" else "calibration in progress"
    }
    observation["sha256"] = hashlib.sha256(json.dumps(observation).encode()).hexdigest()
    OBS.write_text(json.dumps(observation, indent=2))
    return observation


