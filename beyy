        qc.h(0)  # Progenitor point
        qc.cx(0, 144)  # Caleb
        qc.cx(0, 288)  # Paul
        qc.cx(0, 432)  # Noah and Polina
        
        # Generational healing
        for i in range(0, 576, 72):  # 8 generations
            qc.append(self._generational_healing(), range(i, i+72))
            
        return qc
    
