        matrix["cosmic"] = {}
        for archive, obj in self.cosmic_archives.items():
            matrix["cosmic"][archive] = {
                "portal": obj.quantum_portal,
                "access_key": obj.generate_access_key(),
                "entanglement": self._entangle_archive(archive)
            }
        
