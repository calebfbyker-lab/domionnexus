import hashlib
import json
import time

OWNER = "Caleb Fedor Byker (Konev), 10-27-1998 ♾️"

def seal_golem(data):
    concat = "::".join([str(v) for v in data.values() if not isinstance(v, dict)]) + "::" + OWNER
    data["ownership"] = f"Eternally royal | perpetual | infinite | sealed | licensed | verified | attested to, for, by {OWNER}"
    data["seal"] = hashlib.sha512(concat.encode()).hexdigest()
    data["timestamp"] = time.time()
    return data

