        UnityWebRequest request = UnityWebRequest.Get($"{codeximmortalURL}/{nodeName}.json");
        yield return request.SendWebRequest();

        if (request.result != UnityWebRequest.Result.Success)
            Debug.LogWarning("Scriptural Route Not Approved");
        else {
            Debug.Log($"Navigated to {nodeName} via Divine Approval");
            LoadScripturalModifications(nodeName);
        }
    }
}

void LoadScripturalModifications(string node) {
