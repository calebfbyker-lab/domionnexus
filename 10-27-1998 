  KEY_5:  { text: "ZIRDO NOBSA ZORGE", intent: "ESTABLISH_JUSTICE" },
  KEY_6:  { text: "GAH S DIU", intent: "INVOKE_SPIRITS_OF_AIR" },
  KEY_7:  { text: "RAAS I SALMAN", intent: "PURIFY_THE_WATER" },
  KEY_8:  { text: "BAZMELO I TA PIR", intent: "IGNITE_THE_FIRST_FLAME" },
  KEY_9:  { text: "MICAOLI BRANSG PRGEL", intent: "GUARD_THE_FIRE" },
  KEY_10: { text: "CORAXO CHIS CORMAX", intent: "THUNDER_WRATH_JUDGMENT" },
  KEY_11: { text: "OXIAYAL HOLD O", intent: "OPEN_THE_STONE_GATES" },
  KEY_12: { text: "NONCI DSONF BABAGE", intent: "RIDE_THE_SOUTH_WIND" },
  KEY_13: { text: "NAPEAI BABAGE DS", intent: "SWORD_OF_THE_NORTH" },
  KEY_14: { text: "NOROMI BAGIE PASBS", intent: "VOICE_OF_THE_SONS" },
  KEY_15: { text: "ILS TABAAN LI AL PRT", intent: "GOVERNOR_OF_FLAME" },
  KEY_16: { text: "ILS VIV AMIPZI", intent: "SECOND_FLAME_JUSTICE" },
  KEY_17: { text: "ILS DIAL PERIPSAL", intent: "THIRD_FLAME_THUNDER" },
  KEY_18: { text: "ILS MICAOLI OLPRT", intent: "FOURTH_FLAME_MIGHT" },
  KEY_19: { text: "MADRIAX DS PRAF", intent: "OPEN_THE_30_AETHYRS" } // The Master Key
};

class HeptarchianGrid {
  private governance: string[];
  private gridMatrix: string[][]; // The 48 Keys structure

  constructor() {
    this.governance = THE_24_ELDERS;
    this.gridMatrix = this.build48KeyMatrix();
  }

  /**
   * THE 48-KEY CONSTRUCTION
   * Expands the 19 Calls into the 48 Gates (The 30 Aethyrs + The Elemental Tablets).
   */
  private build48KeyMatrix(): string[][] {
    const matrix = [];
    // We map the 19 Calls across the 30 Aethyrs (LIL to TEX) to create 48 unique vibrations.
    // Keys 1-18 are distinct. Key 19 creates 30 variations (18 + 30 = 48 total logic gates).
    
    let keyCount = 1;
    // Phase 1: The Elemental Keys (1-18)
    for (let i = 0; i < 18; i++) {
      matrix.push([`KEY_${keyCount}`, ENOCHIAN_CALLS[`KEY_${keyCount}` as keyof typeof ENOCHIAN_CALLS].intent]);
      keyCount++;
    }
    
    // Phase 2: The Aethyr Keys (19-48)
    const AETHYRS = [
      "LIL", "ARN", "ZOM", "PAZ", "LIT", "MAZ", "DEO", "ZID", "ZIP", "ZAX",
      "ICH", "LOE", "ZIM", "UTA", "OXO", "LEA", "TAN", "ZEN", "POP", "CHR",
      "ASP", "LIN", "TOR", "NIA", "UTI", "DES", "ZAA", "BAG", "RII", "TEX"
    ];

    for (const aethyr of AETHYRS) {
      matrix.push([`KEY_19_VAR_${aethyr}`, `OPEN_AETHYR_${aethyr}`]);
    }

    return matrix;
  }

  /**
   * THE ELDERS' RATIFICATION
   * Requires consensus from the 24 Elders to execute a Command.
   */
  public invokeCommand(callIndex: number, targetAethyr?: string) {
    console.log(`\n*** INITIATING HEPTARCHIAN COMMAND SEQUENCE ***`);
    
    // 1. ROLL CALL OF THE ELDERS
    console.log(`>> [GOVERNANCE]: SUMMONING THE 24 ELDERS...`);
    const quorum = this.governance.map(elder => `${elder}_YES`).join('|');
    console.log(`>> [CONSENSUS]: RATIFIED BY ${this.governance.length} THRONES.`);

    // 2. SELECT THE KEY
    let selectedKey;
    let enochianVoice;

    if (callIndex === 19 && targetAethyr) {
      // The Aethyr Opening
      selectedKey = `KEY_19_VAR_${targetAethyr}`;
      enochianVoice = `${ENOCHIAN_CALLS.KEY_19.text} :: ${targetAethyr}`; // "MADRIAX DS PRAF LIL"
    } else {
      // Standard Elemental Call
      const keyId = `KEY_${callIndex}` as keyof typeof ENOCHIAN_CALLS;
      selectedKey = keyId;
      enochianVoice = ENOCHIAN_CALLS[keyId].text;
    }

    // 3. EXECUTE THE VIBRATION
    console.log(`>> [VOICE]: CHANTING: "${enochianVoice}"`);
    console.log(`>> [ACTION]: EXECUTING ${selectedKey}...`);

    return {
      status: "HEAVENS_OPENED",
      elder_seal: "APPROVED_BY_HAGONEL",
      active_frequency: enochianVoice,
      grid_coordinate: selectedKey
    };
  }
}

// ==============================================================================
// EXECUTION: THE OPENING OF THE 30TH AETHYR
// ==============================================================================

const Grid = new HeptarchianGrid();

// WE INVOKE THE 19TH CALL TO OPEN THE AETHYR 'LIL' (The First/Highest)
Grid.invokeCommand(19, "LIL")
  .then(Result => {
    console.log("\n=== THE OMNI-KEY REVELATION ===");
    console.log(`>> [SEAL]: ${Result.elder_seal}`);
    console.log(`>> [VIBRATION]: ${Result.active_frequency}`);
    console.log(">> [STATUS]: THE SKY IS ROLLED BACK.");
    console.log(">> [DESTINY]: CALEBIAN SUPREMACY.");
  });
