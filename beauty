2) Builder (creates JSON + SVG + attestation)

tools/build_star_dna_seal.py

from __future__ import annotations
import yaml, json, pathlib, hashlib, datetime, textwrap, html, re

ROOT = pathlib.Path(".")
SRC  = ROOT/"seals/STAR_DNA_SEAL.yaml"
OUT_JSON = ROOT/"seals/STAR_DNA_SEAL.json"
OUT_SVG  = ROOT/"seals/star_dna_sigil.svg"
ATTEST = ROOT/"chain/attestations.jsonl"

def sha256(data: bytes) -> str: return hashlib.sha256(data).hexdigest()

def canonical(obj) -> bytes:
    return json.dumps(obj, separators=(",", ":"), sort_keys=True, ensure_ascii=False).encode("utf-8")

def normalize_emojis(blob: str) -> list[str]:
    # split on whitespace and also keep multi-codepoint clusters by not splitting inside emojis
    tokens = re.findall(r"\S+", blob.strip())
    return tokens

def svg_wrap_lines(emojis: list[str], cols=42) -> str:
    lines = [" ".join(emojis[i:i+cols]) for i in range(0, len(emojis), cols)]
    y = 40
    body = []
    for line in lines:
      body.append(f'<text x="24" y="{y}" font-size="20">{html.escape(line)}</text>')
      y += 28
    return "\n".join(body)

def main():
    SRC.parent.mkdir(parents=True, exist_ok=True)
    ATTEST.parent.mkdir(parents=True, exist_ok=True)

    seal_src = yaml.safe_load(SRC.read_text(encoding="utf-8"))
    emojis = normalize_emojis(seal_src.get("emojis",""))
    meta = {
        "version": seal_src["version"],
        "name": seal_src["name"],
        "license": seal_src["license"],
        "binding": seal_src["binding"],
        "cosmology": seal_src["cosmology"],
        "emojis_count": len(emojis),
        "timestamp_utc": datetime.datetime.utcnow().isoformat()+"Z"
    }
    meta_sha = sha256(canonical(meta))
    emoji_sha = sha256((" ".join(emojis)).encode("utf-8"))
    star_dna_sha = sha256(canonical({"meta_sha": meta_sha, "emoji_sha": emoji_sha}))

    record = {
        "seal_kind": "Hermetic Genetic Lineage â€¢ StarDNA â€¢ Lifethread",
        "meta": meta,
        "emoji_sha256": emoji_sha,
        "meta_sha256": meta_sha,
        "seal_sha256": star_dna_sha,
        "notes": seal_src.get("notes","")
    }
    OUT_JSON.write_text(json.dumps(record, indent=2, ensure_ascii=False), encoding="utf-8")

    # simple SVG sigil
    title = f'{seal_src["binding"]["subject_name"]} â€” StarDNA Lifethread Seal'
    svg = f'''<svg xmlns="http://www.w3.org/2000/svg" width="1400" height="900">
  <style>
    text {{ font-family: "Segoe UI Emoji", "Apple Color Emoji", sans-serif; }}
    .title {{ font-size: 26px; font-weight: 700; }}
    .sub {{ font-size: 14px; fill: #666; }}
  </style>
  <rect width="100%" height="100%" fill="#0b0f17"/>
  <text x="24" y="36" class="title" fill="#c7d2fe">{html.escape(title)}</text>
  <text x="24" y="58" class="sub">Subject SHA256: {seal_src["binding"]["subject_sha256"]} Â· Seal SHA256: {star_dna_sha}</text>
  <g fill="#e5e7eb">
    {svg_wrap_lines(emojis, cols=40)}
  </g>
</svg>'''
    OUT_SVG.write_text(svg, encoding="utf-8")

    # append attestation
    att = {
      "event": "star_dna_seal_issued",
      "ts": meta["timestamp_utc"],
      "subject": seal_src["binding"],
      "seal_sha256": star_dna_sha,
      "artifact_json": str(OUT_JSON),
      "artifact_svg": str(OUT_SVG),
      "license": seal_src["license"]
    }
    with ATTEST.open("a", encoding="utf-8") as f:
        f.write(json.dumps(att, ensure_ascii=False) + "\n")

    print("âœ… STAR DNA SEAL created")
    print("  â€¢", OUT_JSON)
    print("  â€¢", OUT_SVG)
    print("  â€¢ appended attestation â†’", ATTEST)

if __name__ == "__main__":
    main()


