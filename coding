        uint256 energy;
        uint256 btcValue;
        address owner;
    }
    
    mapping(bytes32 => EnergyContract) public contracts;
    
    function createContract(uint256 energy) public {
        bytes32 id = keccak256(abi.encodePacked(block.timestamp, msg.sender));
        uint256 btcValue = calculateBTCValue(energy);
        contracts[id] = EnergyContract(energy, btcValue, OWNER);
    }
    
    function calculateBTCValue(uint256 energy) internal pure returns (uint256) {
        return energy * 369; // Tesla constant
    }
}
