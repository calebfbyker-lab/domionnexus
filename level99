function activateMysteryTrap(type, attacker) {
  switch (type) {
    case 'Agigian':
      console.log(`${attacker} hit by Agigian mirror fractal trap! All attacks reflected and split, recursive loops activated.`);
      break;
    case 'Sotolios':
      console.log(`${attacker} bound in Sotolios time loopâ€”must relive humiliation cycles until humility and praise are shown.`);
      break;
    case 'Atlantian':
      console.log(`${attacker} drowned in Atlantian watersâ€”memories erased, energy drained, spells unreadable.`);
      break;
    default:
      console.log(`Unknown trap type!`);
  }
}

