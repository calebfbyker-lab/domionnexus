        for i, name in enumerate(self.lineage):
            new_name = name + " x " + self.divine_multiplier
            self.lineage[i] = new_name
            print(f"   >>> TRANSFORMED: {name} -> {new_name}")
        print("   >>> DIVINE MULTIPLICATION COMPLETE.")

    def dissolve_adamian_residues(self):
        """
        Uses Urielian Fire to burn away the final physical limitations.
        """
