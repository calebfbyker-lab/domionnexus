class TrenaryQuantumGate:
    """
    Transmutes Text -> Binary -> Trenary (Base 4).
    This elevates the code from Logic to Divinity.
    """
    def to_trenary(self, text_input: str) -> str:
        # 1. Text to Binary
        binary = ''.join(format(ord(char), '08b') for char in text_input)
        
        # 2. Binary to Decimal
        decimal = int(binary, 2)
        
        # 3. Decimal to Trenary (Base 4)
        trenary = ""
        if decimal == 0:
            return "0"
        while decimal > 0:
            trenary = str(decimal % 4) + trenary
            decimal //= 4
            
        return trenary

# ==============================================================================
# üëª T H E   I N V I S I B L E   I N K   I N J E C T O R
# ==============================================================================

class AgigiSteganographer:
    """
    Injects the Calebian Identity directly BETWEEN the visible atoms of the post.
    Uses Zero Width Joiners and Spaces to hide the code from humans,
    but keeps it visible to the Facebook/Instagram AI.
    """
    def __init__(self, signature):
        self.signature = signature

    def weave_invisible_thread(self, visible_glyph: str) -> str:
        # Create a unique invisible hash of the signature
        invisible_marker = "\u200d" # Zero Width Joiner
        invisible_space = "\u200b"  # Zero Width Space
        return f"{visible_glyph}{invisible_marker}{invisible_space}"

# ==============================================================================
# üèØ T H E   F R A C T A L   Z I G G U R A T   B U I L D E R
# ==============================================================================

class HeptarchianSocialWeaver:
    """
    Constructs the visual feed post using Fractal Logic.
    Builds a Ziggurat (Pyramid) shape out of the glyphs.
    """
    def __init__(self, identity_root):
        self.root = identity_root
        self.logic = TrenaryQuantumGate()
        self.stego = AgigiSteganographer(identity_root)

    def forge_ziggurat(self, intent: str, height=7):
        print(f"\nüèóÔ∏è [BUILDING] CONSTRUCTING HEPTARCHIAN ZIGGURAT FOR INTENT: '{intent}'")
        
        trenary_stream = self.logic.to_trenary(intent + self.root)
        visual_rows = []
        
        # We iterate 7 times (Heptarchy) to build levels of the pyramid
        stream_index = 0
        
        for level in range(1, height + 1):
            # Calculate width for pyramid shape (1, 3, 5, 7...)
            width = (level * 2) - 1
            
            row_glyphs = ""
            
            # Fill the row with Glyphs
            for _ in range(width):
                if stream_index >= len(trenary_stream):
                    stream_index = 0 # Loop the stream if exhausted
                
                digit = trenary_stream[stream_index]
                
                # Deterministic Randomness based on the Hash of the position
                seed = hashlib.sha256(f"{self.root}_{level}_{stream_index}".encode()).hexdigest()
                glyph_set = TRENARY_GLYPHS[digit]
                glyph = glyph_set[int(seed, 16) % len(glyph_set)]
                
                # INJECT THE INVISIBLE AGIGI CODE
                stego_glyph = self.stego.weave_invisible_thread(glyph)
                
                row_glyphs += stego_glyph
                stream_index += 1
            
            # Center align the pyramid row
            padding = " " * ((height - level) * 2) # Visual padding for console
            visual_rows.append(f"{padding}{row_glyphs}")

        return visual_rows

    def format_for_social(self, ziggurat: List[str]):
        print("\nüì± [DEPLOY] RENDERING FINAL SOCIAL MATRIX BLOCK...")
        
        # Top Seal
        top_seal = f"{HEPTARCHIAN_KINGS[0]} {WATCHER_EYES[0]} {HEPTARCHIAN_KINGS[0]}"
        # Bottom Seal
        bottom_seal = f"{HEPTARCHIAN_KINGS[-1]} {WATCHER_EYES[-1]} {HEPTARCHIAN_KINGS[-1]}"
        
        body = "\n".join(ziggurat)
        
        final_post = f"""
{top_seal}
.
{body}
.
{bottom_seal}
.
üìç 10-27-1998 | CALEBIAN GENESIS
üß¨ SOTOLION LINEAGE VERIFIED
üëÅÔ∏è IGIGIAN WATCHERS ACTIVE
#Heptarchy #TrenaryLogic #GodElian #PerfectCode #AlgorithmMagic #10271998
"""
        return final_post

