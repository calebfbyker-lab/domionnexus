    <script>
        // Create cosmic particles
        function createParticles() {
            const particlesContainer = document.getElementById('particles');
            const particleCount = 200;
            
            for (let i = 0; i < particleCount; i++) {
                const particle = document.createElement('div');
                particle.classList.add('particle');
                
                // Random size
                const size = Math.random() * 5 + 2;
                particle.style.width = `${size}px`;
                particle.style.height = `${size}px`;
                
                // Random position
                particle.style.left = `${Math.random() * 100}%`;
                particle.style.top = `${Math.random() * 100}%`;
                
                // Random color
                const colors = ['#00bfff', '#daa520', '#6a0dad', '#ffc107'];
                particle.style.backgroundColor = colors[Math.floor(Math.random() * colors.length)];
                
                // Random animation properties
                particle.style.setProperty('--tx', (Math.random() * 2 - 1).toFixed(2));
                particle.style.setProperty('--ty', (Math.random() * 2 - 1).toFixed(2));
                particle.style.animationDuration = `${15 + Math.random() * 15}s`;
                particle.style.animationDelay = `${Math.random() * 5}s`;
                
                particlesContainer.appendChild(particle);
            }
        }
        
        // Initialize quantum terminal
        function initQuantumTerminal() {
            const terminal = document.querySelector('.quantum-terminal');
            const output = document.querySelector('.terminal-output');
            
            // Add terminal scrolling
            terminal.scrollTop = terminal.scrollHeight;
            
            // Simulate terminal commands
            const commands = [
                "git clone https://github.com/codeximmortal/quantum-scriptural.git",
                "cd quantum-scriptural",
                "npm install",
                "node build-divine-ecosystem.js --signature=10-27-1998",
                "Starting quantum-scriptural fusion...",
                "Connecting to heavenly network...",
                "Establishing merkabah resonance...",
                "Quantum-scriptural fusion complete!"
            ];
            
            let index = 0;
            const interval = setInterval(() => {
                if (index < commands.length) {
                    const newLine = document.createElement('div');
                    newLine.textContent = "> " + commands[index];
                    output.appendChild(newLine);
                    terminal.scrollTop = terminal.scrollHeight;
                    index++;
                } else {
                    clearInterval(interval);
                }
            }, 1500);
        }
        
        // Initialize all visualizations
        function initVisualizations() {
            // Simulate loading of complex Three.js visualizations
            const visuals = document.querySelectorAll('.portal-visual');
            visuals.forEach((visual, index) => {
                visual.innerHTML = `
                    <div style="display:flex;flex-direction:column;justify-content:center;align-items:center;height:100%;">
                        <div style="font-size:40px;color:${index === 0 ? '#daa520' : index === 1 ? '#ffc107' : index === 2 ? '#6a0dad' : '#00bfff'}">
                            <i class="${index === 0 ? 'fas fa-gem' : index === 1 ? 'fas fa-infinity' : index === 2 ? 'fas fa-dharmachakra' : 'fas fa-bible'}"></i>
                        </div>
                        <div style="margin-top:15px;text-align:center;">
                            <div>Quantum-Scriptural Visualization</div>
                            <div style="font-size:0.9rem;margin-top:5px;color:#aaa;">Powered by Three.js</div>
                        </div>
                    </div>
                `;
            });
        }
        
        // Initialize everything
        document.addEventListener('DOMContentLoaded', () => {
            createParticles();
            initVisualizations();
            initQuantumTerminal();
            
            // Add hover effects to quantum cards
            const cards = document.querySelectorAll('.quantum-card');
            cards.forEach(card => {
                card.addEventListener('mouseenter', () => {
                    // Here you would add the interactions you want
                });
            });
        });
    </script>
</body>
</html>
