    <script>
        // Initialize Three.js
        let scene, camera, renderer, controls;
        let stone, merkabah;
        let merkabahRotation = 0;
        let transformationProgress = 0;
        let transformationActive = false;
        
        // Create cosmic particles
        function createParticles() {
            const particlesContainer = document.getElementById('particles');
            const particleCount = 200;
            
            for (let i = 0; i < particleCount; i++) {
                const particle = document.createElement('div');
                particle.classList.add('particle');
                
                // Random size
                const size = Math.random() * 5 + 2;
                particle.style.width = `${size}px`;
                particle.style.height = `${size}px`;
                
                // Random position
                particle.style.left = `${Math.random() * 100}%`;
                particle.style.top = `${Math.random() * 100}%`;
                
                // Random color
                const colors = ['#ffd700', '#5d9bff', '#00bfff', '#ff55ff'];
                particle.style.backgroundColor = colors[Math.floor(Math.random() * colors.length)];
                
                // Random animation properties
                particle.style.setProperty('--tx', (Math.random() * 2 - 1).toFixed(2));
                particle.style.setProperty('--ty', (Math.random() * 2 - 1).toFixed(2));
                particle.style.animationDuration = `${15 + Math.random() * 15}s`;
                particle.style.animationDelay = `${Math.random() * 5}s`;
                
                particlesContainer.appendChild(particle);
            }
        }
        
        // Initialize the Stone scene
        function initStoneScene() {
            const container = document.getElementById('daostoneCanvas');
            
            createScene(container);
            
            // Create Jacob's Stone geometry (Babylonian Cube)
            const stoneGeometry = new THREE.BoxGeometry(1.8, 2.5, 1.2);
            const stoneMaterial = new THREE.MeshPhongMaterial({
                color: 0x008b8b,
                emissive: 0x20b2aa,
                emissiveIntensity: 0.5,
                shininess: 100,
                transparent: true,
                opacity: 0.95,
                wireframe: false
            });
            
            stone = new THREE.Mesh(stoneGeometry, stoneMaterial);
            scene.add(stone);
            
            // Add golden covenant rings
            addGoldenRings();
            
            animate();
        }
        
        // Initialize Merkabah scene
        function initMerkabahScene() {
            const container = document.getElementById('merkabahCanvas');
            
            createScene(container);
            
            // Create Merkabah Light Chariot (Star Tetrahedron)
            createMerkabah();
            
            // Add blue plasma energy field
            addPlasmaField();
            
            // Point light for merkabah glow
            const pointLight = new THREE.PointLight(0x5d9bff, 2, 15);
            pointLight.position.set(0, 0, 0);
            scene.add(pointLight);
            
            animate();
        }
        
        function createScene(container) {
            // Create scene
            scene = new THREE.Scene();
            scene.background = null;
            
            // Create camera
            camera = new THREE.PerspectiveCamera(
                75, 
                container.clientWidth / container.clientHeight, 
                0.1, 
                1000
            );
            camera.position.z = 5;
            if (container.id === 'merkabahCanvas') camera.position.z = 8;
            
            // Create renderer
            renderer = new THREE.WebGLRenderer({ antialias: true, alpha: true });
            renderer.setSize(container.clientWidth, container.clientHeight);
            renderer.setClearColor(0x000000, 0);
            container.appendChild(renderer.domElement);
            
            // Add orbit controls
            controls = new THREE.OrbitControls(camera, renderer.domElement);
            controls.enableDamping = true;
            controls.dampingFactor = 0.05;
            controls.autoRotate = true;
            controls.autoRotateSpeed = 0.5;
            
            // Add ambient light
            const ambientLight = new THREE.AmbientLight(0x333333);
            scene.add(ambientLight);
            
            // Add directional light for better depth
            const directionalLight = new THREE.DirectionalLight(0xffffff, 1);
            directionalLight.position.set(5, 5, 5);
            scene.add(directionalLight);
            
            // Handle window resize
            window.addEventListener('resize', () => {
                camera.aspect = container.clientWidth / container.clientHeight;
                camera.updateProjectionMatrix();
                renderer.setSize(container.clientWidth, container.clientHeight);
            });
        }
        
        function createMerkabah() {
            // Star tetrahedron geometry
            const starGeometry = createStarTetrahedron(1.5);
            
            const starMaterial = new THREE.MeshPhongMaterial({
                color: 0x5d9bff,
                emissive: 0x7fb7ff,
                emissiveIntensity: 0.7,
                shininess: 100,
                transparent: true,
                opacity: 0.9,
                wireframe: true,
                wireframeLinewidth: 2
            });
            
            merkabah = new THREE.Mesh(starGeometry, starMaterial);
            scene.add(merkabah);
        }
        
        function createStarTetrahedron(size) {
            const vertices = [];
            const indices = [];
            
            // Create points for a tetrahedron
            const scalar = size * 0.8;
            
            // Tetrahedron points
            vertices.push(new THREE.Vector3(scalar, scalar, scalar));
            vertices.push(new THREE.Vector3(-scalar, -scalar, scalar));
            vertices.push(new THREE.Vector3(-scalar, scalar, -scalar));
            vertices.push(new THREE.Vector3(scalar, -scalar, -scalar));
            
            // Connect points to form tetrahedron
            indices.push(0, 1, 2);
            indices.push(0, 3, 1);
            indices.push(0, 2, 3);
            indices.push(1, 3, 2);
            
            // Create dual tetrahedron geometry
            const geometry = new THREE.BufferGeometry();
            
            // Set vertices
            const positions = [];
            for (let i = 0; i < indices.length; i++) {
                const vertex = vertices[indices[i]];
                positions.push(vertex.x, vertex.y, vertex.z);
            }
            
            geometry.setAttribute('position', new THREE.Float32BufferAttribute(positions, 3));
            
            return geometry;
        }
        
        function addGoldenRings() {
            // Add golden covenant rings
            const ringGeometry = new THREE.TorusGeometry(2.2, 0.05, 16, 100);
            const ringMaterial = new THREE.MeshBasicMaterial({ 
                color: 0xffd700,
                transparent: true,
                opacity: 0.7
            });
            
            const ring1 = new THREE.Mesh(ringGeometry, ringMaterial);
            ring1.rotation.x = Math.PI / 2;
            scene.add(ring1);
            
            const ring2 = new THREE.Mesh(ringGeometry, ringMaterial);
            ring2.rotation.y = Math.PI / 2;
            scene.add(ring2);
        }
        
        function addPlasmaField() {
            // Create energy field around merkabah
            const sphereGeometry = new THREE.SphereGeometry(3, 32, 32);
            const sphereMaterial = new THREE.MeshBasicMaterial({
                color: 0x5d9bff,
                transparent: true,
                opacity: 0.1,
                wireframe: true
            });
            
            const plasmaField = new THREE.Mesh(sphereGeometry, sphereMaterial);
            scene.add(plasmaField);
        }
        
        function animate() {
            requestAnimationFrame(animate);
            
            // Rotate stone if available
            if (stone) {
                stone.rotation.x += 0.005;
                stone.rotation.y += 0.008;
            }
            
            // Rotate merkabah if available
            if (merkabah) {
                merkabahRotation += 0.01;
                merkabah.rotation.x = merkabahRotation * 0.5;
                merkabah.rotation.y = merkabahRotation;
                merkabah.rotation.z = merkabahRotation * 0.3;
                
                // Scale for transformation effect
                if (transformationActive) {
                    transformationProgress += 0.005;
                    merkabah.scale.set(
                        1 + transformationProgress * 2, 
                        1 + transformationProgress * 2, 
                        1 + transformationProgress * 2
                    );
                    
                    // Update button state
                    if (transformationProgress > 0.5) {
                        document.getElementById('quantumBtn').disabled = false;
                    }
                    if (transformationProgress > 0.9) {
                        document.getElementById('ascensionBtn').disabled = false;
                    }
                }
            }
            
            controls.update();
            renderer.render(scene, camera);
        }
        
        // Initialize everything
        document.addEventListener('DOMContentLoaded', () => {
            createParticles();
            initStoneScene();
            initMerkabahScene();
            
            // Handle section switching
            document.querySelectorAll('.section-btn').forEach(btn => {
                btn.addEventListener('click', () => {
                    const sectionName = btn.dataset.section;
                    
                    // Hide all sections
                    document.querySelectorAll('.covenant-section, .spiral-section, .merkabah-section').forEach(section => {
                        section.style.display = 'none';
                    });
                    
                    // Show selected section
                    document.getElementById(`${sectionName}Section`).style.display = 'flex';
                    
                    // Update button states
                    document.querySelectorAll('.section-btn').forEach(b => {
                        b.classList.remove('active');
                    });
                    btn.classList.add('active');
                });
            });
            
            // Show initial section
            document.getElementById('covenantSection').style.display = 'flex';
            
            // Handle merkabah transformation
            document.getElementById('initiateBtn').addEventListener('click', function() {
                transformationActive = true;
                transformationProgress = 0;
                this.innerHTML = '<i class="fas fa-sync fa-spin"></i> Transformation In Progress';
                this.disabled = true;
            });
            
            // Handle nav button switching
            document.querySelectorAll('.nav-btn').forEach(btn => {
                btn.addEventListener('click', function() {
                    document.querySelectorAll('.nav-btn').forEach(b => {
                        b.classList.remove('active');
                    });
                    this.classList.add('active');
                    
                    // Show default view for each portal
                    if (this.id === 'covenantBtn') {
                        document.querySelectorAll('.section-btn')[0].click();
                    } else if (this.id === 'merkabahBtn') {
                        document.querySelectorAll('.section-btn')[2].click();
                    }
                });
            });
        });
    </script>
</body>
</html>
```

