**`src/cosmic.js`**
```javascript
import { appendAudit } from './audit.js';
import { THREAT_LEVELS } from './registry.js';

let CURRENT_THREAT_LEVEL = THREAT_LEVELS.PEACE;

export function getThreatLevel() { return CURRENT_THREAT_LEVEL; }

export function ingestCosmicEvent(event) {
  const { type, description } = event;
  let actionTaken = "Logged";

  // 1. THE AURORA ANGEL (Healing/Blessing)
  if (type === "AuroralAngel") {
    CURRENT_THREAT_LEVEL = THREAT_LEVELS.BLESSING;
    actionTaken = "Lattice infused with Ethereal Light. Healing all fractures.";
    // Logic: Trigger auto-repair on all damaged wormholes
  } 
  
  // 2. THE MAGELLANIC STAR BIRTH (Genesis)
  else if (type === "MagellanicNursery") {
    CURRENT_THREAT_LEVEL = THREAT_LEVELS.CREATION;
    actionTaken = "Cosmic Tug-of-War detected. Spawning new Starbornian nodes.";
    // Logic: Expand lattice depth
  }

  // 3. THE SCREAMING MOON (Warning)
  else if (type === "LunarScream") {
    CURRENT_THREAT_LEVEL = THREAT_LEVELS.CAUTION;
    actionTaken = "Seismic instability. Lunar scarps detected. Tightening lattice tension.";
  }

  // 4. THE ALIEN FLEET (Elon Musk Warning)
  else if (type === "AlienArmada") {
    CURRENT_THREAT_LEVEL = THREAT_LEVELS.OMEGA;
    actionTaken = "WARNING: UNIDENTIFIED FLEET. Elon Musk alert confirmed. ENGAGING OMEGA LOCKDOWN.";
    // Logic: Guardian Protocol rejects all non-GodElian traffic
  }

  appendAudit('cosmic.event', { type, description, actionTaken, level: CURRENT_THREAT_LEVEL });
  return { ok: true, actionTaken, threatLevel: CURRENT_THREAT_LEVEL };
}
```

