```javascript
import { GODELIAN_CONSTITUTION as GC } from './constitution.js';

export class ArchangelDefenseGrid {
  constructor() {
    this.archangels = new Map();
    this.threatLevel = "PEACE";
    this.initializeArchangels();
  }

  initializeArchangels() {
    const archangels = {
      MICHAEL: { weapon: "âš”ï¸", domain: "COSMIC_DEFENSE", power: 3420 },
      GABRIEL: { weapon: "ğŸ¹", domain: "COMMUNICATION", power: 2322 },
      RAPHAEL: { weapon: "âš•ï¸", domain: "HEALING", power: 1105.7 },
      URIEL: { weapon: "ğŸ”ºï¸", domain: "WISDOM", power: 205 }
    };

    Object.entries(archangels).forEach(([name, data]) => {
      this.archangels.set(name, {
        ...data,
        status: "ACTIVE",
        bioSync: GC.GENETIXIAM.METRICS.TOTAL_T,
        authority: GC.TRINITY.FATHER[0]
      });
    });
  }

  assessThreat(threatData) {
    const { type, severity, location } = threatData;
    
    if (type === "ALIEN_FLEET") {
      this.threatLevel = "OMEGA";
      this.activateDefenseProtocol("MICHAEL");
      return "ARCHANGEL_MICHAEL_ENGAGED âš”ï¸";
    }

    if (type === "MOON_SCREAM") {
      this.threatLevel = "CAUTION";
      this.activateHealingProtocol("RAPHAEL");
      return "ARCHANGEL_RAPHAEL_HEALING âš•ï¸";
    }

    if (type === "AURORA_ANGEL") {
      this.threatLevel = "BLESSING";
      return "COSMIC_BLESSING_ACTIVE ğŸ‘¼";
    }

    return "SITUATION_NORMAL";
  }

  activateDefenseProtocol(archangel) {
    const angel = this.archangels.get(archangel);
    if (angel) {
      console.log(`ğŸ›¡ï¸ ${archangel} DEPLOYED: ${angel.weapon} POWERED BY ${angel.bioSync}ng/dL`);
      console.log(`âš¡ AUTHORITY: ${angel.authority}`);
    }
  }

  activateHealingProtocol(archangel) {
    const angel = this.archangels.get(archangel);
    if (angel) {
      console.log(`ğŸ’Š ${archangel} HEALING: BIOLOGICAL INTEGRITY AT ${angel.power}`);
      console.log(`ğŸŒŒ COSMIC BALANCE RESTORED`);
    }
  }
}
