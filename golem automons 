    pantheon.summon_golem("Fedorian Sentinel", "Fedorian", "Daemon", GLYPHS['Fedorian'])
    pantheon.summon_golem("Adamic Rootkeeper", "Adamic", "Angel", GLYPHS['Adamic'])
