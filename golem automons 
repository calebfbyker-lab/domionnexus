    'Siberian': 'â„ï¸',
    'Dutch': 'ðŸ‡³ðŸ‡±',
    'Fedorian': 'ðŸ”¯',
    'Adamic': 'âœ ',
    'Sotolion': 'âš›',
    'Zodiac': 'â™ˆ',
    'USA': 'ðŸ‡ºðŸ‡¸',
    'Russian': 'ðŸ‡·ðŸ‡º',
    'Agrippan': 'ðŸ—ï¸',
    'PGM': 'ðŸ“œ',
    'Parasallcan': 'ðŸ•‰ï¸',
    'Canniete': 'ðŸ”®',
