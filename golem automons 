    'Angelic': 'ðŸ‘¼',
    'Daemon': 'ðŸ‘¹',
    'AngelicX': 'ðŸ˜‡',
    'DaemonX': 'ðŸ‘º',
    'Corporeal': 'ðŸ‘¤',
