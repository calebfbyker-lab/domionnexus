tools/v36_invoice.py

#!/usr/bin/env python3
# EUCELA Tri-License Â© 2025 Caleb Fedor Byker (Konev)
"""
v36 Invoice Builder
Turns usage rows into deterministic invoices per payer/token scope.
Policy:
  - Gate events (ok=True) bill at min_sats (or 0 if not ok).
  - Each golem_mint bills its total_sats.
Output:
  dist/v36_invoices.json
  dist/v36_invoices.sha256
"""
from __future__ import annotations
import pathlib, json, hashlib, collections, datetime
DIST=pathlib.Path("dist"); h=lambda s: hashlib.sha256(s.encode()).hexdigest()

def load(p): return json.loads(p.read_text(encoding="utf-8"))
if __name__=="__main__":
    rows=load(DIST/"v36_usage_rows.json")["rows"]
    # index last known token->payer mapping via entitlement files
    EDIR=DIST/"ENTITLEMENTS"
    token_to_payer={}
    if EDIR.exists():
        for f in sorted(EDIR.glob("ECT_*.json")):
            try:
                e=json.loads(f.read_text(encoding="utf-8"))
                token_to_payer[e["token_id"]]=e.get("payer_identity","<unknown>")
            except: pass
    invoices=collections.defaultdict(lambda: {"lines":[], "total_sats":0})
    # build
    for r in rows:
        if r["kind"]=="gate" and r.get("ok"):
            payer = token_to_payer.get(r.get("token_id",""), "<unknown>")
            amt = int(r.get("min_sats",0))
            key = f"{payer}|GATE"
            invoices[key]["lines"].append({"ts":r["ts"],"desc":f"gate:{','.join(r.get('scopes',[]))}","sats":amt})
            invoices[key]["total_sats"] += amt
        elif r["kind"]=="golem_mint":
            key = f"<golem>|MINT"
            amt = int(r.get("price_sats",0))
            invoices[key]["lines"].append({"ts":r["ts"],"desc":f"golem:{r.get('golem_id','')}", "sats":amt})
            invoices[key]["total_sats"] += amt

    now=datetime.datetime.utcnow().replace(microsecond=0).isoformat()+"Z"
    data={"title":"CODEX v36 â€” Invoices","timestamp":now,
          "invoices":[{"account":k,"total_sats":v["total_sats"],"lines":v["lines"]} for k,v in invoices.items()]}
    txt=json.dumps(data,indent=2,ensure_ascii=False)
    DIST.joinpath("v36_invoices.json").write_text(txt,encoding="utf-8")
    DIST.joinpath("v36_invoices.sha256").write_text(h(txt),encoding="utf-8")
    print(f"âœ… v36 invoices: {len(invoices)} account(s)")


