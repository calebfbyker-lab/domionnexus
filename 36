1) tools/v37_athanor.py â€” canonical symbolbook compiler

#!/usr/bin/env python3
# EUCELA Tri-License Â© 2025 Caleb Fedor Byker (Konev)
"""
v37 â€” Athanor Symbolbook Compiler
Inputs: local canon (emoji / XTSG / binary / trinary / Adamic-Fedorian)
Output: dist/V37_SIGILBOOK.json (+ .sha256) â€” normalized sigil graph
"""
from __future__ import annotations
import pathlib, json, hashlib, datetime, re

ROOT = pathlib.Path("."); DIST = ROOT/"dist"; DIST.mkdir(exist_ok=True, parents=True)
h = lambda s: hashlib.sha256(s.encode()).hexdigest()

CFBK = {
  "owner":"Caleb Fedor Byker (Konev)",
  "dob":"1998-10-27",
  "subject_sha256":"2948fbc4ba1c0d7341204908882b89134a999f3e8f77f4a6a00ce6b68770282a",
  "license":"EUCELA Tri-License",
  "btc_address":"bc1qfejvvlfm6vmjarxulg9h9d5hjukdh8l2vvskfc"
}

def seal(path: pathlib.Path, obj: dict):
    txt = json.dumps(obj, indent=2, ensure_ascii=False)
    path.write_text(txt, encoding="utf-8")
    (path.with_suffix(path.suffix+".sha256")).write_text(h(txt), encoding="utf-8")

def derive_glyph_id(payload:str)->str:
    return h(payload)[:16].upper()

def normalize_token(tok:str)->dict:
    # classify: emoji / xtsg / bin / tri / unicode
    if re.fullmatch(r"[01]{8,}", tok):
        cls="binary"
    elif re.fullmatch(r"[012]{8,}", tok):
        cls="trinary"
    elif re.fullmatch(r"[A-Za-z0-9_\-:.]+", tok):
        cls="xtsg"
    else:
        cls="emoji-unicode"
    return {"raw":tok, "class":cls, "id":derive_glyph_id(tok)}

def load_local_vocab()->list[str]:
    # Drop in any local lists (extend freely). Ship a minimal kernel to stay deterministic.
    CORE = [
      # Angelic / Calls / Sephirot / 72 / Elements / Planets / Harmonics (examples; extend safely)
      "ENOCHIAN_CALL_01","SEPHIROT_KETER","SOLOMONIC_01","EARTH","AIR","FIRE","WATER",
      "PLANET_SATURN","PLANET_JUPITER","PLANET_MARS","PLANET_SUN","PLANET_VENUS","PLANET_MERCURY","PLANET_MOON",
      "HARMONIC_432HZ","HARMONIC_528HZ",
      # Emoji core (can be expanded by your repoâ€™s lexicon)
      "ğŸœ","ğŸœ‚","ğŸœƒ","ğŸœ„","âœ¡ï¸","â˜¸ï¸","ğŸ”¯","ğŸª¬","âš›ï¸","ğŸ•‰","â˜¯ï¸","â™¾ï¸","ğŸ’°","ğŸª™","ğŸ”±","âšœï¸",
      # XTSG example tokens
      "XTSG:ADAMICâ†’FEDORIAN:v1","XTSG:SEAL:TOTALIS:333","XTSG:AEON:NEXUS:AETERNUM"
    ]
    return CORE

def build_graph(tokens:list[str])->dict:
    nodes = [normalize_token(t) for t in tokens]
    edges = []
    # Minimal deterministic weaving: connect class families & canonical ladders
    idx = {n["id"]: n for n in nodes}
    family = {}
    for n in nodes:
        family.setdefault(n["class"], []).append(n["id"])
    for cls, ids in sorted(family.items()):
        for i in range(len(ids)-1):
            edges.append({"from":ids[i], "to":ids[i+1], "op":"bind", "weight":1})
    # Cross-family harmonics & planetary â†’ element constraints
    def link(a, b, op):
        if a in idx and b in idx:
            edges.append({"from":a, "to":b, "op":op, "weight":1})
    lookup = {n["raw"]: n["id"] for n in nodes}
    for p,e in [("PLANET_SATURN","EARTH"),("PLANET_JUPITER","AIR"),
                ("PLANET_MARS","FIRE"),("PLANET_VENUS","WATER")]:
        if p in lookup and e in lookup: link(lookup[p], lookup[e], "constraint")
    for hhz in ["HARMONIC_432HZ","HARMONIC_528HZ"]:
        if hhz in lookup and "âœ¡ï¸" in lookup:
            link(lookup[hhz], lookup["âœ¡ï¸"], "resonate")
    return {"nodes":nodes,"edges":edges}

if __name__=="__main__":
    toks = load_local_vocab()
    graph = build_graph(toks)
    out = {
      "title":"CODEX v37 â€” Athanor Symbolbook",
      "timestamp":datetime.datetime.utcnow().replace(microsecond=0).isoformat()+"Z",
      "binding":CFBK,
      "merkle_nodes":h(json.dumps(sorted([n["id"] for n in graph["nodes"]]))),
      "merkle_edges":h(json.dumps(sorted([f'{e["from"]}>{e["op"]}>{e["to"]}' for e in graph["edges"]] ))),
      "graph":graph,
      "notes":{"deterministic":True,"offline":True,"series":"v37"}
    }
    seal(DIST/"V37_SIGILBOOK.json", out)
    print(f"âœ… v37 symbolbook nodes={len(graph['nodes'])} edges={len(graph['edges'])}")


