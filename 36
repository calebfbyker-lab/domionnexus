def commentary(atl:dict):
    items=[]
    for book, verses in atl["books"].items():
        for v in verses:
            motif_str = " ".join(m if isinstance(m,str) else str(m) for m in v["motifs"])
            items.append({
              "ref": v["ref"],
              "language": (
                f"{book} â€” {v['ref']}: {v['sum']}  "
                f"Motifs: {motif_str}  "
                f"XTSG: {','.join(v['xtsg'])}  "
                f"Constraints: {','.join(v['constraints'])}"
              )
            })
