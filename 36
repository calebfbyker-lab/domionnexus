      {"ref":"Acts 2:1-13","sum":"Pentecost: sound, fire, tongues; nations hear.",
       "motifs":[EMOJI["fire"],EMOJI["tongues"],"ğŸŒ"],"xtsg":[XTSG("PNEUMA.EVENT:Pentecost")],"constraints":["elemental:fire","harmonic:polyglossia"]},
      {"ref":"Acts 2:14-41","sum":"Peter interprets by prophecy; Messiah proclaimed; many respond.",
       "motifs":["ğŸ“£","ğŸ“ˆ"],"xtsg":[XTSG("KERUGMA.PETER")],"constraints":["kabbalistic:malchut"]},
      {"ref":"Acts 2:42-47","sum":"Teaching, fellowship, breaking bread, prayers; shared resources; favor.",
       "motifs":["ğŸ","ğŸ¤","ğŸ "],"xtsg":[XTSG("OIKOS.COMMON-LIFE")],"constraints":["angelic:favor"]}
