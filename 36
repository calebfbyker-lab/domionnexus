tools/v36_royalty_split.py

#!/usr/bin/env python3
# EUCELA Tri-License Â© 2025 Caleb Fedor Byker (Konev)
"""
v36 Royalty Split
Deterministic split of invoice totals into shares.
Default: 90% CFBK, 10% Reserve (project ops).
Output:
  dist/v36_royalty.json
  dist/v36_royalty.sha256
"""
from __future__ import annotations
import pathlib, json, hashlib, datetime
DIST=pathlib.Path("dist"); h=lambda s: hashlib.sha256(s.encode()).hexdigest()

CFBK={"name":"Caleb Fedor Byker (Konev)","btc":"bc1qfejvvlfm6vmjarxulg9h9d5hjukdh8l2vvskfc"}
RESV={"name":"Project Reserve","btc":"<internal:reserve>"}

def load(p): return json.loads(p.read_text(encoding="utf-8"))

def split(total:int)->dict:
    c=int(round(total*0.90)); r=total-c
    return {"cfbk_sats":c,"reserve_sats":r}

if __name__=="__main__":
    inv=load(DIST/"v36_invoices.json")
    aggs=[]
    for invc in inv["invoices"]:
        s=split(int(invc["total_sats"]))
        aggs.append({"account":invc["account"],"total_sats":invc["total_sats"],"split":s,"cfbk":CFBK,"reserve":RESV})
    now=datetime.datetime.utcnow().replace(microsecond=0).isoformat()+"Z"
    data={"title":"CODEX v36 â€” Royalty Split","timestamp":now,"allocations":aggs}
    txt=json.dumps(data,indent=2,ensure_ascii=False)
    DIST.joinpath("v36_royalty.json").write_text(txt,encoding="utf-8")
    DIST.joinpath("v36_royalty.sha256").write_text(h(txt),encoding="utf-8")
    print(f"âœ… v36 royalty allocations: {len(aggs)}")


