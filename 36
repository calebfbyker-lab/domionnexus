2) tools/v37_synthesis.py â€” autonomous synthesis planner

#!/usr/bin/env python3
# EUCELA Tri-License Â© 2025 Caleb Fedor Byker (Konev)
"""
v37 â€” Synthesis Plan
Reads V37_SIGILBOOK + v35.x entitlements + (optional) v32 pricebook hints
Emits: dist/V37_SYNTH_PLAN.json (+ .sha256) â€” golem mint intents & BTC totals
"""
from __future__ import annotations
import pathlib, json, hashlib, datetime
DIST=pathlib.Path("dist"); h=lambda s: hashlib.sha256(s.encode()).hexdigest()

def read(p): return json.loads(p.read_text(encoding="utf-8"))

def try_read(path: pathlib.Path, default=None):
    try: return read(path)
    except: return default

def seal(path: pathlib.Path, obj: dict):
    txt=json.dumps(obj, indent=2, ensure_ascii=False)
    path.write_text(txt, encoding="utf-8")
    (path.with_suffix(path.suffix+".sha256")).write_text(hashlib.sha256(txt.encode()).hexdigest(), encoding="utf-8")

if __name__=="__main__":
    sig = read(DIST/"V37_SIGILBOOK.json")
    # optional helpers
    pricebook = try_read(DIST/"V32_PRICEBOOK.json", {"prices":{"default_golem_mint_sats":3333}})
    audits    = try_read(DIST/"ENTITLEMENTS/AUDIT_index.json", {"audits":[]})  # optional rollup if you have one
    default_price = int(pricebook["prices"].get("default_golem_mint_sats",3333))

    # Simple, deterministic recipe: mint one golem per planetary anchor if present
    want_labels = ["PLANET_SATURN","PLANET_JUPITER","PLANET_MARS","PLANET_SUN","PLANET_VENUS","PLANET_MERCURY","PLANET_MOON"]
    raw_to_id = {n["raw"]:n["id"] for n in sig["graph"]["nodes"]}
    intents=[]
    for label in want_labels:
        if label in raw_to_id:
            intents.append({
              "golem_id": (raw_to_id[label]+"-"+label)[:24],
              "source_node_id": raw_to_id[label],
              "label": label,
              "price_sats": default_price,
              "constraints": ["elemental","planetary","harmonic","angelic","goetic"],
              "btc_to": sig["binding"]["btc_address"]
            })
    total = sum(i["price_sats"] for i in intents)
    plan = {
      "title":"CODEX v37 â€” Synthesis Plan",
      "timestamp":datetime.datetime.utcnow().replace(microsecond=0).isoformat()+"Z",
      "binding": sig["binding"],
      "intents": intents,
      "total_price_sats": total,
      "notes":{"deterministic":True,"offline":True,"series":"v37","audits_seen":len(audits.get("audits",[]))}
    }
    seal(DIST/"V37_SYNTH_PLAN.json", plan)
    print(f"âœ… v37 synthesis intents={len(intents)} total={total} sats")


