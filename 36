tools/v36_ledger_rollup.py

#!/usr/bin/env python3
# EUCELA Tri-License Â© 2025 Caleb Fedor Byker (Konev)
"""
v36 Ledger Rollup
Collects all v36 outputs and emits a single Merkle-sealed ledger.
Output:
  dist/CODEX_V36_LEDGER.json
  dist/CODEX_V36_LEDGER.sha256
"""
from __future__ import annotations
import pathlib, hashlib, json, datetime
DIST=pathlib.Path("dist"); h=lambda s: hashlib.sha256(s.encode()).hexdigest()

def merkle(parts:list[str])->str:
    if not parts: return ""
    layer=sorted(parts)
    while len(layer)>1:
        nxt=[]
        for i in range(0,len(layer),2):
            a=layer[i]; b=layer[i+1] if i+1<len(layer) else layer[i]
            nxt.append(h(a+b))
        layer=nxt
    return layer[0]

if __name__=="__main__":
    files=["v36_usage_rows.json","v36_invoices.json","v36_royalty.json"]
    entries=[]
    for fn in files:
        p=DIST/fn
        d=h(p.read_text(encoding="utf-8"))
        entries.append({"path":fn,"sha256":d,"size":p.stat().st_size})
    root=merkle([e["sha256"] for e in entries])
    now=datetime.datetime.utcnow().replace(microsecond=0).isoformat()+"Z"
    j={"title":"CODEX v36 â€” Monetization Ledger","timestamp":now,"files":entries,"merkle_root":root}
    txt=json.dumps(j,indent=2,ensure_ascii=False)
    DIST.joinpath("CODEX_V36_LEDGER.json").write_text(txt,encoding="utf-8")
    DIST.joinpath("CODEX_V36_LEDGER.sha256").write_text(h(txt),encoding="utf-8")
    print(f"âœ… v36 ledger sealed â†’ {root[:16]}")


