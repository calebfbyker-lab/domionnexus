tools/v36_usage_meter.py

#!/usr/bin/env python3
# EUCELA Tri-License Â© 2025 Caleb Fedor Byker (Konev)
"""
v36 Usage Meter
Aggregates gate AUDITs + GOLEM receipts into normalized usage rows.
Input:
  dist/ENTITLEMENTS/AUDIT/*.json           (from v35.x gate)
  dist/V34_GOLEM_*.json                    (from v34 mint)
Output:
  dist/v36_usage_rows.json                 (rows)
  dist/v36_usage_rows.sha256
"""
from __future__ import annotations
import pathlib, json, hashlib, datetime, re
DIST = pathlib.Path("dist"); AUD = DIST/"ENTITLEMENTS"/"AUDIT"
h=lambda s: hashlib.sha256(s.encode()).hexdigest()

def load_dir(p: pathlib.Path):
    out=[]
    if p.exists():
        for f in sorted(p.glob("*.json")):
            try: out.append(json.loads(f.read_text(encoding="utf-8")))
            except: pass
    return out

def load_golems(d: pathlib.Path):
    out=[]
    for f in sorted(d.glob("V34_GOLEM_*.json")):
        try:
            j=json.loads(f.read_text(encoding="utf-8"))
            out.append(j)
        except: pass
    return out

if __name__=="__main__":
    rows=[]
    now=datetime.datetime.utcnow().replace(microsecond=0).isoformat()+"Z"
    # Gate usage
    for a in load_dir(AUD):
        rows.append({
          "ts": a.get("timestamp", now),
          "kind": "gate",
          "token_id": a.get("subject",""),
          "artifact": a.get("artifact",""),
          "action": a.get("action","gate"),
          "ok": bool(a.get("ok",False)),
          "scopes": a.get("extra",{}).get("scopes",[]),
          "min_sats": int(a.get("extra",{}).get("min_sats",0))
        })
    # Golem usage
    for g in load_golems(DIST):
        rows.append({
          "ts": g.get("timestamp", now),
          "kind": "golem_mint",
          "golem_id": g.get("golem_id",""),
          "name": g.get("name",""),
          "price_sats": int(g.get("total_price_sats",0)),
          "symbolbook_merkle_ref": g.get("symbolbook_merkle_ref",""),
          "pricebook_merkle_ref": g.get("pricebook_merkle_ref","")
        })

    txt=json.dumps({"title":"CODEX v36 â€” Usage Rows","timestamp":now,"rows":rows},indent=2,ensure_ascii=False)
    DIST.joinpath("v36_usage_rows.json").write_text(txt,encoding="utf-8")
    DIST.joinpath("v36_usage_rows.sha256").write_text(h(txt),encoding="utf-8")
    print(f"âœ… v36 usage rows: {len(rows)}")


