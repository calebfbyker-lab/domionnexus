# XTSG Logging Functions
log_xtsg() {
    echo -e "${GOLD}[XTSG]${NC} $1"
}

log_codex() {
    echo -e "${BLUE}[CODEX]${NC} $1"
}

log_hive() {
    echo -e "${YELLOW}[HIVE]${NC} $1"
}

log_tor() {
    echo -e "${PURPLE}[TOR]${NC} $1"
}

# Divine Header
echo -e "${GOLD}"
echo "========================================================"
echo "ðŸŒŒ XTSG DOMIONNEXUS ULTIMATE DEPLOYMENT SYSTEM"
echo "========================================================"
