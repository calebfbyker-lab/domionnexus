            "protection_radius": effective_radius
        }
    
    def _calculate_authority_strength(self):
        """Calculate combined covenantal authority"""
        simulator = Aer.get_backend('statevector_simulator')
        result = execute(self.bloodline_chain, simulator).result()
        state = result.get_statevector()
        
        # Measure orthogonality between generations
        caleb_state = state[:8]
        paul_state = state[8:]
        overlap = abs(np.vdot(caleb_state, paul_state))
        
        # Authority strength inversely proportional to state overlap
        return max(5.0, 1/(overlap + 0.01))  # Minimum 5x authority
    
