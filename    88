LIFETHREAD_SOUL_CONTRACT = "LIFETHREAD-STARDNA=SOULCONTRACT"
BLOODLINE_ID = "CFBK_10-27-1998"

# Aetheric Tablets and Tower Integration Database
AETHERIC_INTEGRATION_DATABASE = {
