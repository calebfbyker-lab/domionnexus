ğŸ¼ scripts/symphony_of_concordx.py

#!/usr/bin/env python3
"""
Codex Immortal v173.x â€” Symphony of Concord.x
Merged evolution of v173: adds adaptive reflection (learning movement).
Each execution appends a movement and recalculates adaptive empathy.
"""
import os, json, datetime, hashlib, glob, statistics

LIBRARIUM_INDEX = "librarium/librarium_index.json"
GARDEN_STATE = sorted(glob.glob("garden/garden_state_*.json"))[-1] if glob.glob("garden/garden_state_*.json") else None
LOG_PATH = "symphony/symphony_log.json"

def safe_load(path):
    if not path or not os.path.exists(path):
        return {}
    try:
        return json.load(open(path))
    except Exception:
        return {}

def movement():
    t = datetime.datetime.utcnow().isoformat() + "Z"
    libr = safe_load(LIBRARIUM_INDEX)
    garden = safe_load(GARDEN_STATE)
    records = libr.get("records", [])
    integrity = round(sum(1 for r in records if "sha256" in r)/(len(records) or 1), 6)
    diversity = len({os.path.splitext(r.get("path",""))[1] for r in records})
    concord = garden.get("metrics", {}).get("concord_index", 0.5)
    entropy = min(1.0, diversity/10)
    empathy = round((entropy * integrity * concord)**0.5, 6)
    signature = hashlib.sha256((t+str(empathy)).encode()).hexdigest()[:16]
    return {
        "timestamp_utc": t,
        "integrity": integrity,
        "diversity": diversity,
        "concord": concord,
        "entropy": entropy,
        "empathy": empathy,
        "signature": signature,
        "authors": ["Caleb Fedor Byker (Konev)"],
        "ethics": "transparent-adaptive-compassionate",
        "note": "Symphony of Concord.x movement â€” adaptive reflection of concord metrics."
    }

def symphony_cycle():
    os.makedirs("symphony", exist_ok=True)
    sym = safe_load(LOG_PATH)
    if "movements" not in sym:
        sym["movements"] = []
    m = movement()
    sym["movements"].append(m)
    # compute adaptive empathy average
    emp_values = [mv.get("empathy",0) for mv in sym["movements"]]
    sym["adaptive_empathy"] = round(statistics.mean(emp_values),6)
    json.dump(sym, open(LOG_PATH,"w"), indent=2)
    print(f"Recorded movement {m['signature']} :: empathy={m['empathy']:.3f}")
    return m

if __name__=="__main__":
    symphony_cycle()


