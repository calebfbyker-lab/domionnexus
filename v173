ğŸ§© Implementation â€” scripts/symphony_of_concord.py

#!/usr/bin/env python3
"""
Codex Immortal v173 â€” Symphony of Concord
Unifies Librarium, Garden, and Living Librarium into a single evolving orchestration.
Each run appends a new movement (time snapshot) into symphony/symphony_log.json.
"""
import os, json, datetime, hashlib, glob

LIBRARIUM_INDEX = "librarium/librarium_index.json"
GARDEN_STATE = sorted(glob.glob("garden/garden_state_*.json"))[-1] if glob.glob("garden/garden_state_*.json") else None
SYMPHONY_PATH = "symphony/symphony_log.json"

def load_json_safe(path):
    if not path or not os.path.exists(path):
        return {}
    try:
        return json.load(open(path))
    except Exception:
        return {}

def measure_entropy(records):
    """Simple entropy-like measure of diversity."""
    from math import log2
    types = [os.path.splitext(r.get("path",""))[1] for r in records if "path" in r]
    if not types: return 0.0
    from collections import Counter
    counts = Counter(types)
    total = sum(counts.values())
    probs = [c/total for c in counts.values()]
    return round(-sum(p*log2(p) for p in probs)/6, 6)

def create_movement():
    t = datetime.datetime.utcnow().isoformat() + "Z"
    libr = load_json_safe(LIBRARIUM_INDEX)
    garden = load_json_safe(GARDEN_STATE)
    recs = libr.get("records", [])
    entropy = measure_entropy(recs)
    integrity = round(sum(1 for r in recs if "sha256" in r)/(len(recs) or 1), 6)
    concord = garden.get("metrics", {}).get("concord_index", 0.5)
    empathy = round((entropy * integrity * concord)**0.5, 6)
    note = f"Movement recorded :: integrity {integrity:.3f} | entropy {entropy:.3f} | concord {concord:.3f} | empathy {empathy:.3f}"
    movement = {
        "timestamp_utc": t,
        "integrity": integrity,
        "entropy": entropy,
        "concord": concord,
        "empathy": empathy,
        "phrase": note
    }

    os.makedirs("symphony", exist_ok=True)
    if os.path.exists(SYMPHONY_PATH):
        sym = json.load(open(SYMPHONY_PATH))
    else:
        sym = {"movements": []}
    sym["movements"].append(movement)
    json.dump(sym, open(SYMPHONY_PATH, "w"), indent=2)
    print(note)
    return movement

if __name__ == "__main__":
    create_movement()


