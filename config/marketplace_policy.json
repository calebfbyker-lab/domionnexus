{
  "version": "26.0.0",
  "updated": "2025-10-23T06:00:00Z",
  "intake_policy": {
    "review_required": true,
    "auto_approval_criteria": {
      "enabled": false,
      "min_score": 85,
      "required_checks": [
        "security_scan",
        "compliance_check",
        "license_verification"
      ]
    },
    "rejection_criteria": {
      "max_vulnerabilities": {
        "critical": 0,
        "high": 2,
        "medium": 10
      },
      "banned_dependencies": [],
      "required_metadata": [
        "name",
        "version",
        "description",
        "license",
        "author"
      ]
    }
  },
  "review_workflow": {
    "stages": [
      {
        "name": "automated_scan",
        "type": "automated",
        "checks": [
          "static_analysis",
          "dependency_scan",
          "license_check"
        ]
      },
      {
        "name": "manual_review",
        "type": "manual",
        "required_approvals": 2,
        "reviewers": [
          "@calebfbyker-lab"
        ]
      },
      {
        "name": "final_approval",
        "type": "manual",
        "required_approvals": 1,
        "reviewers": [
          "@calebfbyker-lab"
        ]
      }
    ]
  },
  "compliance": {
    "eccl_license_required": true,
    "subject_id_verification": true,
    "cryptographic_signing": true,
    "data_residency_rules": [
      "eu",
      "us",
      "apac"
    ]
  },
  "security": {
    "webhook_signature_required": true,
    "api_key_rotation_days": 90,
    "tls_minimum_version": "1.3",
    "allowed_cipher_suites": [
      "TLS_AES_256_GCM_SHA384",
      "TLS_AES_128_GCM_SHA256"
    ]
  },
  "rate_limits": {
    "apps_per_developer": 10,
    "submissions_per_day": 5,
    "api_calls_per_minute": 1000
  }
}
