def connect_universal_nodes(home_node, nodes):
    global_mesh = []
    for node in nodes:
        connection = {
            "source": home_node["geo"],
            "destination": node["geo"],
            "archetype_link": node.get("archetype", "Universal"),
            "quantum_key": node.get("quantum_key", None),
            "legal_attestation": "Active, cryptographically/theurgically sealed",
            "harmonic_status": "Synchronized",
            "timestamp": datetime.datetime.utcnow().isoformat()
        }
        global_mesh.append(connection)
    return global_mesh

