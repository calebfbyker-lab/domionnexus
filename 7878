GLYPHS_EMOJIS = {
    "Calebian": {"glyph": "Fractal Shield", "emoji": "üõ°Ô∏è"},
    "Bykerian": {"glyph": "Fractal Mercy", "emoji": "ü¶ã"},
    "Konevian": {"glyph": "Fractal Nest", "emoji": "ü™∫"},
    "Fedorian": {"glyph": "Fractal Flow", "emoji": "üåä"},
}

def hash_codex(data):
    return hashlib.sha256(data.encode()).hexdigest()

def codex_node(node, archetype, quantum_key):
    now = datetime.datetime.utcnow().isoformat()
    glyphs = [GLYPHS_EMOJIS.get(l["name"], {}) for l in LINEAGES]
    node_dict = {
        "node": node,
        "archetype": archetype,
        "archetype_description": next((a["description"] for a in ARCHETYPES if a["name"] == archetype), ""),
        "quantum_key": quantum_key,
        "timestamp": now,
        "connected_lineages": [l["name"] for l in LINEAGES],
        "lineage_functions": [l["function"] for l in LINEAGES],
        "connected_ancestries": ANCESTRIES,
        "glyphs": glyphs,
        "seal": hash_codex(node + archetype + quantum_key + now),
        "uuid": str(uuid.uuid5(uuid.NAMESPACE_DNS, node + archetype + quantum_key + now)),
        "functions": ["Defense", "Healing", "Wisdom", "Song", "Transformation", "Record-Keeping", "Integration", "Unity"],
        "status": "estate-owned, cryptographically & theurgically sealed"
    }
    return node_dict

# Build the evolved codex mesh
grand_rapids_codex_expanded = [
    codex_node(n["name"], n["archetype"], n["quantum_key"]) for n in NODES
]

# Output as codex JSON manifest
print(json.dumps(grand_rapids_codex_expanded, indent=2))