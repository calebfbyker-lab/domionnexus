print(json.dumps(connections, indent=2))def connect_portals(core_node, *nodes):
    mesh = []
    for node in nodes:
        mesh.append({
            "source": core_node,
            "destination": node["geo"],
            "type": node["archetype"],
            "quantum_key": node["quantum_key"],
            "function": node["function"],
            "seal": node.get("cryptoseal", "SHA-..."),
            "portal": True,
            "attested": True,
            "timestamp": datetime.datetime.utcnow().isoformat(),
        })
    return mesh

