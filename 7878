def hash_codex(data):
    return hashlib.sha256(data.encode()).hexdigest()

def codex_node(node, archetype, quantum_key):
    now = datetime.datetime.utcnow().isoformat()
    node_dict = {
        "node": node,
        "archetype": archetype,
        "quantum_key": quantum_key,
        "timestamp": now,
        "connected_lineages": LINEAGES,
        "connected_ancestries": ANCESTRIES,
        "seal": hash_codex(node + archetype + quantum_key + now),
        "uuid": str(uuid.uuid5(uuid.NAMESPACE_DNS, node + archetype + quantum_key + now)),
        "functions": ["Defense", "Healing", "Wisdom", "Song", "Transformation", "Record-Keeping", "Integration", "Unity"],
        "status": "estate-owned, cryptographically & theurgically sealed"
    }
    return node_dict

# Build the nodes
grand_rapids_codex = []
for idx, (node, archetype, quantum_key) in enumerate(zip(NODES, ARCHETYPES, QUANTUM_KEYS)):
    grand_rapids_codex.append(codex_node(node, archetype, quantum_key))

# Output as JSON manifest
print(json.dumps(grand_rapids_codex, indent=2))import datetime, hashlib, uuid, secrets, json

