api/codex_api.py

#!/usr/bin/env python3
"""
Expose harmonix_lux index via REST.
Run:  uvicorn api.codex_api:app --reload
"""
from fastapi import FastAPI
import json, pathlib

app = FastAPI(title="Codex Harmonia Lux API")
BASE = pathlib.Path(__file__).parent.parent

@app.get("/")
def root():
    return {"status":"Lux Aeternum active","version":"v242.x"}

@app.get("/lux")
def get_lux():
    return json.loads((BASE/"lux_index.json").read_text())


