#### **harmonix.py**
```python
#!/usr/bin/env python3
"""
Codex Harmonia v242
Synchronizes Codex manifests and creates a harmonic index (AI √ó NI √ó TI).
"""
import hashlib, json, datetime, pathlib

BASE = pathlib.Path(__file__).parent
manifest, hashes = {}, {}

def sha256_file(path: pathlib.Path):
    h = hashlib.sha256()
    with open(path, "rb") as f:
        for chunk in iter(lambda: f.read(65536), b""):
            h.update(chunk)
    return h.hexdigest()

# Walk the project and hash every file except manifest.json
for p in BASE.rglob("*"):
    if p.is_file() and p.name != "manifest.json":
        hashes[str(p.relative_to(BASE))] = sha256_file(p)

manifest["version"] = "v242"
manifest["generated"] = datetime.datetime.utcnow().isoformat() + "Z"
manifest["files"] = hashes
(BASE / "manifest.json").write_text(json.dumps(manifest, indent=2), encoding="utf-8")

harmonix = {
    "version": "v242",
    "description": "Codex Harmonia ‚Äî unified orchestration layer",
    "components": len(hashes),
    "generated": manifest["generated"],
    "seal": "calebfedorbykerkonev10271998"
}
(BASE / "harmonix.json").write_text(json.dumps(harmonix, indent=2), encoding="utf-8")

print(f"‚úÖ Codex Harmonia (v242) built successfully ‚Äî {len(hashes)} files indexed.")


