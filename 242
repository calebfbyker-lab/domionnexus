api/codex_api.py

#!/usr/bin/env python3
"""
Simple FastAPI endpoint exposing harmonix.json
Run with:  uvicorn api.codex_api:app --reload
"""
from fastapi import FastAPI
import json, pathlib

app = FastAPI(title="Codex Harmonia API")
BASE = pathlib.Path(__file__).parent.parent

@app.get("/")
def root():
    return {"status": "Codex Harmonia API active", "version": "v242"}

@app.get("/harmonix")
def get_harmonix():
    data = json.loads((BASE / "harmonix.json").read_text())
    return data


