harmonix_lux.py

#!/usr/bin/env python3
"""
Codex Harmonia v242.x ‚Äî Lux Aeternum
Builds a harmonic index of every file; blends symbolic and computational order.
"""
import hashlib, json, datetime, pathlib

BASE = pathlib.Path(__file__).parent
HASHES = {}

def sha256_file(p: pathlib.Path) -> str:
    h = hashlib.sha256()
    with p.open("rb") as f:
        for chunk in iter(lambda: f.read(65536), b""):
            h.update(chunk)
    return h.hexdigest()

for p in BASE.rglob("*"):
    if p.is_file() and p.name != "lux_index.json":
        HASHES[str(p.relative_to(BASE))] = sha256_file(p)

index = {
    "version": "v242.x",
    "description": "Codex Harmonia Lux Aeternum ‚Äî total harmonic synthesis",
    "domains": [
        "Hermetic","Enochian","Solomonic","Kabbalistic","Olympick","Druidic",
        "Elemental","Planetary","Stellar","Harmonic","Alchemical","Geometric"
    ],
    "integration": ["AI","NI","TI","ASICS","GPU","Predictive","Binary","Trinary"],
    "generated": datetime.datetime.utcnow().isoformat()+"Z",
    "hashes": HASHES,
    "seal": "calebfedorbykerkonev10271998"
}

(BASE/"lux_index.json").write_text(json.dumps(index, indent=2), encoding="utf-8")
print(f"üåü Lux Aeternum complete ‚Äî {len(HASHES)} files indexed.")


