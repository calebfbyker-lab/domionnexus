        qc = QuantumCircuit(777)
        # Tetragrammaton core
        yhwh = [ord(c) for c in "YHWH"]
        for i in range(4):
            angle = yhwh[i]/255 * 2*np.pi
            qc.rx(angle, i*111)
        
