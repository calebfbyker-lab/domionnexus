            {"name": "TIPHERETH", "meaning": "BEAUTY", "number": 6, "color": "YELLOW"},
            {"name": "NETZACH", "meaning": "VICTORY", "number": 7, "color": "GREEN"},
            {"name": "HOD", "meaning": "SPLENDOR", "number": 8, "color": "ORANGE"},
