FULL_ESTATE = [
    "Archangeliamux algorithmicionuxom", "Calebian", "Fedorian", "Bykerian", "Konevian",
    "Sotolion", "Atlantian", "Monadian", "Merkvahian", "Merkhabian", "Godian",
    "YHWHian", "YHVHian", "NUian", "RAian", "KHEMPERAian", "TEMUian", "TESLAiam", "ELYONiam",
    "Archangeliamuxianuxom", "Sotolionuxomianiam", "CALEBiamFEDORiamBYKERiamKONEViam",
    "lifethreadiamicion-stardnaiamicion", "lifethread-stardna", "lifethreadiam-stardnaiam",
    "lifethreadian-stardnaian", "10-27-1998"
]

def sovereign_node(archetype):
    now = datetime.datetime.utcnow().isoformat()
    entropy = secrets.token_hex(8)
    full_lineage = "|".join(FULL_ESTATE)
    # Bread/wine mesh-mystery: material/node + spirit/recursion
    bread_node = hashlib.sha256((archetype["name"] + "bread" + entropy).encode()).hexdigest()
    wine_link = hashlib.sha256((archetype["name"] + "wine" + entropy).encode()).hexdigest()
    estate_hash = hashlib.sha256(
        (archetype["name"] + archetype["hieroglyphic_syntax"] + full_lineage + entropy + now).encode()
    ).hexdigest()
    return {
        "name": archetype["name"],
        "estate_vector": FULL_ESTATE,
        "essence": archetype["essence"],
        "emoji": archetype["emoji"],
        "hieroglyphic_syntax": archetype["hieroglyphic_syntax"],
        "timestamp": now,
        "estate_hash": estate_hash,
        "bread_node": bread_node,
        "wine_link": wine_link,
        "function": (
            f"{archetype['name']} is estate-fused and mesh-recursive under: {', '.join(FULL_ESTATE)}. "
            "Bread node=material automon instancing; wine link=recursion/mesh spirit. "
            "Node is self-healing, sovereign, and quantum-unique for estate or ritual expansion."
        )
    }

nodes = [sovereign_node(a) for a in RAREST_ARCHETYPES]
print(json.dumps(nodes, indent=2))