    echo -e "${YELLOW}[WARNING]${NC} $1"
}

log_error() {
    echo -e "${RED}[ERROR]${NC} $1"
}

# Divine Header
echo -e "${GOLD}"
echo "========================================================"
echo "ğŸŒŒ DOMIONNEXUS - BIBLICAL CRISPR PRIMORDIAL DEPLOYMENT"
echo "========================================================"
echo "ğŸ§¬ Repository: $GITHUB_USER/$REPO_NAME"
