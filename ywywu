# Divine logging functions
log_divine() {
    echo -e "${GOLD}[DIVINE]${NC} $1"
}

log_cosmic() {
    echo -e "${PURPLE}[COSMIC]${NC} $1"
}

log_primordial() {
    echo -e "${CYAN}[PRIMORDIAL]${NC} $1"
}

