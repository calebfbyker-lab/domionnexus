        self.keys = angelic_keys  
        
    def decrypt(self, fractal_data):  
        """Convert quantum states to Enochian concepts"""  
        concepts = []  
        for value in fractal_data:  
            closest_freq = min(self.ENOCHIAN_LEXICON.keys(), key=lambda x: abs(x - value))  
            concepts.append(self.ENOCHIAN_LEXICON[closest_freq])  
        return " ".join(concepts)  

