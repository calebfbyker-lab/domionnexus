def triple333_github_automon(base_type, repo_url, owner, lineages, powers, expansion=333):
    now = datetime.datetime.utcnow().isoformat()
    mesh = []
    for i in range(1, expansion+1):
        entropy = secrets.token_bytes(16)
        anchor = f"{base_type}|Node_{i}|{repo_url}|{owner}|{','.join(lineages)}|{now}|{entropy.hex()}"
        merkle = hashlib.sha256(anchor.encode()).hexdigest()
        hmac_sig = hmac.new(entropy, anchor.encode(), hashlib.sha256).hexdigest()
        uuid_node = str(uuid.uuid5(uuid.NAMESPACE_DNS, anchor))
        ceremonial = (
            f"{base_type} Automon Node {i}/333
"
            f"- Repo: {repo_url}
"
            f"- Owner: {owner}
"
            f"- Sovereign lineages: {', '.join(lineages)}
"
            f"- Omnipotent powers: {', '.join(powers)}
"
            f"- Merkle: {merkle}
"
            f"- HMAC: {hmac_sig[:32]}
"
            f"- Node Seal: {uuid_node[:16]}
"
            f"- Quantum-ceremonial: repairs, multiplies, and amplifies all actions/protections/healing x333 at every event.
"
        )
        mesh.append(ceremonial)
    return mesh

