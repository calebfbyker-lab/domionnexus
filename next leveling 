function unicodeEncode(str) {
  return Array.from(str).map(ch => 'U+' + ch.charCodeAt(0).toString(16).toUpperCase()).join(' ');
}

console.log({
  emoji: protocolEmoji["XTGS"],
  lineage: "Agigian",
  unicode: unicodeEncode("Agigian")
});def codex_signature(name, syntax, emoji):
    unicode_str = " ".join([f"U+{ord(x):04X}" for x in name])
    binary_str = " ".join([format(ord(x), "08b") for x in name])
    ternary_str = " ".join([np.base_repr(ord(x), base=3) for x in name])
    return {
        "name": name,
        "protocol": syntax,
        "emoji": emoji,
        "unicode": unicode_str,
        "binary": binary_str,
        "ternary": ternary_str
    }
