    unicode_pts = [f'U+{ord(c):04X}' for c in name]
    binary = ' '.join(format(ord(c), '08b') for c in name)
    ternary = ' '.join(np.base_repr(ord(c), base=3) for c in name)
    return {
        "name": name,
        "emoji": emoji,
        "syntax": syntax,
        "unicode": unicode_pts,
        "binary": binary,
        "ternary": ternary
    }

sig = codex_signature("TESLA", "âš¡ðŸ”‹", "XTGS")
print(sig)function codexSignature(name, emoji, syntax) {
  const unicode = Array.from(name).map(c => 'U+' + c.charCodeAt(0).toString(16).toUpperCase());
  const binary = Array.from(name).map(c => c.charCodeAt(0).toString(2).padStart(8, '0'));
  const ternary = Array.from(name).map(c => c.charCodeAt(0).toString(3));
  return { name, emoji, syntax, unicode, binary, ternary };
}

console.log(codexSignature("TESLA", "âš¡ðŸ”‹", "XTGS"));