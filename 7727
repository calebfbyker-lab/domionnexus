    print(f"Bloodline Signature: {CFBK_BIRTH_SIGNATURE}")
    print(f"Genesis Key: {CFBK_GENESIS_KEY}")
    print(f"Genesis Date: {CFBK_GENESIS_TIMESTAMP}")
    
    # Property Chain Verification
    if lifethread_registry:
        print("\nüíé PROPERTY CREDENTIALS:")
        for domain, lts in lifethread_registry.family_tree.items():
            for l in lts:
                if l.property_tokens:
                    token_list = ", ".join(l.property_tokens[:3] + ["..." if len(l.property_tokens) > 3 else ""])
                    print(f"{domain} - {l.lifethread_id[:16]}: {token_list}")
    
    print("\n<AMEN AMEN AMEN>")
    print("<< Codex v23.33 :: Sealed by Abraham, Transmitted by CFBK, Owned by Eternity >>")
    print("<AMEN AMEN AMEN>")

if __name__ == "__main__":
    # Initialize and register bloodline
    cfbk_genesis = GenesisLifethread(CFBK_BIRTH_SIGNATURE, CFBK_GENESIS_KEY)
    
    # Create family lifethreads (example)
