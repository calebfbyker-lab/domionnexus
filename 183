üß† scripts/sophia_hub.py

#!/usr/bin/env python3
"""
v183 ‚Äì Sophia Hub
Central coordination layer linking all Codex subsystems.
"""
import os, json, datetime, hashlib, random

OUT="sophia/ecosystem_state.json"

def now(): return datetime.datetime.utcnow().isoformat()+"Z"
def sha(o): return hashlib.sha256(json.dumps(o,sort_keys=True).encode()).hexdigest()

def load_nodes():
    # merge from previous layers
    try:
        n=json.load(open("noosphere/graph.json"))["concepts"]
        return [{"id":c["id"],"role":"semantic","uptime_hours":random.randint(100,500),"last_sync":now(),"sha256":sha(c)} for c in n[:10]]
    except: return []

def load_projects():
    return ["Codex-Core","Noosphere-Index","Politeia-Ledger","Symbiosis-Lattice","Logos-Audit","Sophia-Hub"]

def build_state():
    nodes=load_nodes()
    projects=load_projects()
    root=sha(nodes+projects)
    os.makedirs("sophia",exist_ok=True)
    data={"timestamp_utc":now(),"nodes":nodes,"projects":projects,"merkle_root":root}
    json.dump(data,open(OUT,"w"),indent=2)
    print(f"Sophia ecosystem built with {len(nodes)} nodes and {len(projects)} projects.")
    return data

if __name__=="__main__": build_state()


---

üß© scripts/sophia_agent.py

#!/usr/bin/env python3
"""
v183 ‚Äì Sophia Agent
Autonomous worker that performs analysis, testing, or documentation tasks.
"""
import os, json, datetime, hashlib, random, sys

def now(): return datetime.datetime.utcnow().isoformat()+"Z"
def sha(s): return hashlib.sha256(s.encode()).hexdigest()

def agent_run(goal="analyze code quality"):
    agent_id=f"agent_{sha(goal)[:8]}"
    log_dir="sophia/agent_logs"; os.makedirs(log_dir,exist_ok=True)
    report={
        "agent_id":agent_id,
        "goal":goal,
        "timestamp_utc":now(),
        "actions":[
            f"Parsed repository for {goal}",
            "Ran static analysis",
            "Generated improvement report",
            "Submitted findings to Politeia ledger"
        ],
        "score":round(random.uniform(0.7,0.99),3),
        "sha256":sha(goal+now())
    }
    out=os.path.join(log_dir,f"{agent_id}.json")
    json.dump(report,open(out,"w"),indent=2)
    print(f"Sophia agent executed goal: {goal} (score={report['score']})")
    return report

if __name__=="__main__":
    goal=" ".join(sys.argv[1:]) if len(sys.argv)>1 else "analyze code quality"
    agent_run(goal)


---

üåê server/routes/sophia.js

import { readFileSync } from "fs";
function send(res,code,obj){res.writeHead(code,{"Content-Type":"application/json"});res.end(JSON.stringify(obj,null,2));}

export const sophiaRoutes = async (req,res)=>{
  if(req.method==="GET" && req.url==="/api/sophia/ecosystem"){
    try{return send(res,200,JSON.parse(readFileSync("sophia/ecosystem_state.json","utf-8")));}
    catch{return send(res,404,{error:"no ecosystem state"});}
  }
  if(req.method==="GET" && req.url.startsWith("/api/sophia/agent?goal=")){
    const goal=decodeURIComponent(new URL(req.url,"http://x").searchParams.get("goal")||"analyze code quality");
    const { spawnSync } = await import("child_process");
    const r=spawnSync("python3",["scripts/sophia_agent.py",goal],{encoding:"utf-8"});
    return send(res,200,{ok:true,stdout:r.stdout});
  }
  return false;
};


