import { readFileSync } from "fs";
function send(res,c,o){res.writeHead(c,{"Content-Type":"application/json"});res.end(JSON.stringify(o,null,2));}

export const sophiaxRoutes = async (req,res)=>{
  if(req.method==="GET" && req.url==="/api/sophiax/fabric"){
    try{return send(res,200,JSON.parse(readFileSync("sophiax/fabric_state.json","utf-8")));}
    catch{return send(res,404,{error:"no fabric state"});}
  }
  if(req.method==="GET" && req.url==="/api/sophiax/agentnet"){
    try{return send(res,200,JSON.parse(readFileSync("sophiax/agentnet_state.json","utf-8")));}
    catch{return send(res,404,{error:"no agentnet state"});}
  }
  return false;
};


