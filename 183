‚öôÔ∏è .github/workflows/sophia.yml

name: Sophia
on:
  workflow_dispatch:
  schedule:
    - cron: "15 */12 * * *"
jobs:
  sophia:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v4
      - uses: actions/setup-python@v5
        with: { python-version: '3.x' }
      - name: Build ecosystem
        run: python3 scripts/sophia_hub.py
      - name: Launch analysis agent
        run: python3 scripts/sophia_agent.py "analyze project stability"
      - name: Upload artifacts
        uses: actions/upload-artifact@v4
        with:
          name: sophia
          path: sophia/


