    "nodes":{"type":"array","items":{"type":"object","properties":{
      "id":{"type":"string"},
      "role":{"type":"string"},
      "uptime_hours":{"type":"number"},
      "last_sync":{"type":"string","format":"date-time"},
      "sha256":{"type":"string"}
