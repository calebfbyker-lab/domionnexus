class LinguisticMatrixDecoder:
    """
    Decodes the Hidden Matrix of the English Language.
    Reveals the 'Royal' (RO) influence and breaks the spell.
    """
    def __init__(self):
        self.frequencies = {"R": 18, "O": 15} # RO = 33
        self.targets = ["ROLEX", "ROME", "ROYAL", "ROCK", "ROTHSCHILD"]

    def analyze_influence(self, word: str) -> str:
        """
        Calculates the Gematria weight of the word.
        """
        score = 0
        for char in word.upper():
            if char in self.frequencies:
                score += self.frequencies[char]
        
        status = "MATRIX_BINDING" if score >= 33 else "BENIGN"
        return f"WORD: {word} | FREQUENCY: {score} | STATUS: {status}"

