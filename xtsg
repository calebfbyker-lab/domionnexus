def byte_to_balanced_ternary(byte):
    n = byte
    trits = []
    while n != 0:
        r = n % 3
        n = n // 3
        if r == 2:
            r = -1
            n += 1
        trits.append({-1: '-', 0: '0', 1: '+'}[r])
    return ''.join(trits[::-1]) or '0'

phrase_utf8_bytes = [
    0xF0, 0x9F, 0x8C, 0x8A,  # Nu ğŸŒŠ
    0xE2, 0x98, 0x80, 0xEF, 0xB8, 0x8F,  # Ra â˜€ï¸
    0xF0, 0x93, 0x85, 0x93,  # Khepera ğ“…“
    0xF0, 0x93, 0x8D, 0x8B   # Temu ğ“‹
]

balanced_ternary_result = ''.join(byte_to_balanced_ternary(b) + ' ' for b in phrase_utf8_bytes)
print("Balanced ternary encoding of Nu Ra Khepera Temu UTF-8 bytes:
", balanced_ternary_result){
