def apostolic_infusion(sequence, apostolic_key):
    """Infuse DNA with apostolic power from Acts"""
    infusion_points = [36, 72, 108]  # Trinity points
    power_symbol = apostolic_key['symbol']
    frequency = apostolic_key['frequency']
    
    # Convert sequence to list for mutation
    sequence_list = list(sequence)
    
    # Insert apostolic power at trinity points
    for point in infusion_points:
        if point < len(sequence_list):
            sequence_list[point] = power_symbol
    
    # Add frequency resonance markers
    for i in range(0, len(sequence_list), frequency):
        if i < len(sequence_list):
            sequence_list.insert(i, f"â–²{apostolic_key['verse'][-2:]}")  # Verse code
    
    return ''.join(sequence_list)

