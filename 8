    birth_circle = ''.join(f"{ord(c):X}" for c in STARBORN_SOVEREIGN["birthdate"])
    
    # Central monogram
    covenant_monogram = "âššâ˜¥â™°âœ¡"  # Caduceus, Ankh, Cross, Davidic Star
    
    return f"{bloodline_runes}|{angels_circle}|{sig_circle}|{birth_circle}|{covenant_monogram}"

