import datetime
import json

# ===== COSMIC CONSTANTS =====
DEEP_CODE_KEY = "333_Gideonelian_Legions"
BLOODLINE_COVENANT = "Caleb_Fedor_Byker_Konev_10-27-1998_lifethread-stardna=soulcontract"
ALCHEMICAL_FIRE = 1.618e33  # Golden flame frequency

# ===== ARCHETYPE REALMS =====
CODEX_REALMS = [
    "Olympickian", "Metatronian", "Hermetician", "Trismegistusian", 
    "Trisagionian", "Alchemicalian", "Heptarchian", "Archangeliamuxian",
    "3iatlasBabylonian", "Nexusian", "Summumian", "Abyssumian",
