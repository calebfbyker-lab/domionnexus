def final_inhumanity_sweep(gift_to):
    now = datetime.datetime.utcnow().isoformat()
    core_inhumanities = [
        "slavery", "oppression", "deceit", "violence", "harm",
        "curse", "trauma", "exile", "estrangement", "injustice"
    ]
    destroyed = [h[::-1].upper() for h in core_inhumanities]
    lux_key = hashlib.sha512(("LUX" + now + "".join(gift_to)).encode()).hexdigest()[:88]