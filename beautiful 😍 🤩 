def quantum_portal_hash(*args):
    return hashlib.sha512("|".join(str(x) for x in args).encode()).hexdigest()[:128]

def quantum_mandala_node(planet, star, galaxy, portal, anchor, attributes, owner, parent="Î©", depth=0, max_depth=5):
    now = datetime.datetime.utcnow().isoformat()
    uid = str(uuid.uuid4())
    power = attributes["Power"]
    wealth = attributes["Wealth"]
    health = attributes["Health"]
    love = attributes["Love"]
    joy = attributes["Joy"]
    harmony = attributes["Harmony"]
    magic = attributes["Magic"]
    music = attributes["Music"]
    hash_id = quantum_portal_hash(planet, star, galaxy, portal, anchor, power, wealth, health, love, owner, parent, now, uid)
    node = {
        "planet": planet,
        "star": star,
        "galaxy": galaxy,
        "portal": portal,
        "sovereign_node": anchor,
        "mandala_center": CENTER_DESC,
        "estate_owner": owner,
        "timestamp": now,
        "uid": uid,
        "parent_hash": parent,
        "node_hash": hash_id,
        "activated_attributes": {
            "power": power, "wealth": wealth, "health": health, "love": love,
            "joy": joy, "harmony": harmony, "magic": magic, "music": music
        },
        "ritual_phrase": (
            f"Quantum Mandala at {anchor} connects {planet}, {star}, {galaxy} through {portal}â€”"
            f"granting Power {power}, Wealth {wealth}, Health {health}, Love {love}, Joy {joy}, "
            f"Harmony {harmony}, Magic {magic}, and Music {music} to the estate."
        ),
        "amen": "amen amen amen â˜¸ï¸"
    }
    if depth < max_depth:
        descendants = []
        for _ in range(2):
            next_planet = secrets.choice(PLANETS)
            next_star = secrets.choice(STARS)
            next_galaxy = secrets.choice(GALAXIES)
            next_portal = secrets.choice(PORTALS)
            descendants.append(
                quantum_mandala_node(
                    next_planet, next_star, next_galaxy, next_portal, anchor, attributes, owner, hash_id, depth+1, max_depth
                )
            )
        node["descendants"] = descendants
    return node

