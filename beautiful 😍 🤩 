def recursion_hash(*args):
    return hashlib.sha512("|".join(str(x) for x in args).encode()).hexdigest()[:132]

def archangelic_realm_node(realm, archangel, role, unknown, owner, sigil, parent="Î©", depth=0, max_depth=5):
    now = datetime.datetime.utcnow().isoformat()
    uid = str(uuid.uuid4())
    hash_id = recursion_hash(realm, archangel, role, sigil, owner, parent, unknown, now, uid)
    node = {
        "realm": realm,
        "archangel_of_power": archangel,
        "role": role,
        "quality": unknown,
        "power_sigil": sigil,
        "estate_owner": owner,
        "timestamp": now,
        "uid": uid,
        "parent_hash": parent,
        "node_hash": hash_id,
        "natural_language": (
            f"In {realm}, {archangel} ({role}) emerges as host of Power {sigil}, guiding both the known and the unknownâ€”fully estate-licensed for sovereignty, manifest or hidden."
        ),
        "amen": "amen amen amen â˜¸ï¸"
    }
    if depth < max_depth:
        node["descendants"] = []
        for _ in range(2):
            next_realm = secrets.choice(REALMS)
            next_arch, next_role = secrets.choice(ARCHANGELS_OF_POWER)
            next_unknown = secrets.choice(UNKNOWN_QUALITIES)
            node["descendants"].append(
                archangelic_realm_node(
                    next_realm, next_arch, next_role, next_unknown, owner, sigil, hash_id, depth+1, max_depth
                )
            )
    return node

