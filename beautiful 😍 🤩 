def portal_hash(*vals):
    return hashlib.sha512("|".join(str(x) for x in vals).encode()).hexdigest()

def cosmic_portal_node(planet, metal, p_sig, star, galaxy, portal, owner, world_symbol, parent="Î©", depth=0, max_depth=5):
    now = datetime.datetime.utcnow().isoformat()
    uid = str(uuid.uuid4())
    hash_id = portal_hash(planet, metal, p_sig, star, galaxy, portal, owner, world_symbol, parent, now, uid)
    node = {
        "planet": planet,
        "metal": "Gold",
        "alchemy_sigil": "â˜‰",
        "star": star,
        "galaxy": galaxy,
        "portal": portal,
        "world_symbol": world_symbol,
        "estate_owner": owner,
        "timestamp": now,
        "uid": uid,
        "parent_hash": parent,
        "node_hash": hash_id,
        "cosmic_desc": (
            f"{planet}/{star}/{galaxy} aligned at portal '{portal}' with symbol {world_symbol}. "
            "Gold flows, inheritance and blessing pass through Atlantis directly to Heaven. "
            "Node is cryptographically, legally, and ritual-proven for eternal estate sovereignty and ascension."
        ),
        "amen": "amen amen amen â˜¸ï¸"
    }
    if depth < max_depth:
        descendants = []
        for _ in range(2):
            next_planet, next_metal, next_p_sig = secrets.choice(PLANETS)
            next_star = secrets.choice(STARS)
            next_galaxy = secrets.choice(GALAXIES)
            next_portal = secrets.choice(PORTALS)
            next_symbol = secrets.choice(SYMBOLS)
            descendants.append(
                cosmic_portal_node(
                    next_planet, next_metal, next_p_sig, next_star, next_galaxy, next_portal,
                    owner, next_symbol, hash_id, depth+1, max_depth
                )
            )
        node["descendants"] = descendants
    return node

