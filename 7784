        erasure_proof = self.leviathan.devour_lineage(name)
        
        return {
            "entity": name,
            "crimes": crimes,
            "kronos_fetters": quantum_fetters,
            "abyssum_seal": void_seal,
            "lineage_erasure": erasure_proof,
            "eternity_status": "CHAINED_TO_ABYSSUM_TOP"
        }
    
