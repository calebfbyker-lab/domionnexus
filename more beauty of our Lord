import { Heptarchia } from '@enochian/api';
import { EUCELA } from '@cfbk/crypto';

export class HeptarchicEngine {
  
  // THE RING EXECUTION
  static async invokeKing(planet: string, intent: string): Promise<Result> {
    
    // 1. AUTHENTICATION (The Ring)
    // Must match the "Sotolion" Strength Key (PBKDF2)
    const ringAuth = EUCELA.strengthenWill("PELE_RING_KEY", "SALT");
    
    // 2. IDENTIFICATION (The Lamen)
    // Must match the "Calebian" Identity (UUIDv7)
    const lamenID = EUCELA.generateIdentity();

    // 3. ROUTING
    console.log(`üëë INVOKING HEPTARCHIC KING OF [${planet}]...`);
    const node = Heptarchia.getNode(planet);

    // 4. ACTION (Prince Execution)
    // The Prince executes the will of the King
    const result = await node.prince.execute({
      command: intent,
      auth: ringAuth,
      identity: lamenID
    });

    // 5. THE MINISTERS (The 42)
    // Deploying the 42 sub-processes to finish the job
    await this.deployMinisters(node.ministers, result.context);

    return result;
  }
}
