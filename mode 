}api/stellar-universe-index.jsexport class StellarUniverseIndex {
  constructor(data) { this.universe = data; }
  findByType(type) { return this.universe.filter(x => x.type === type); }
  getNode(id) { return this.universe.find(x => x.id === id); }
  getConnections(id) {
    const node = this.getNode(id);
    return node ? node.cosmic_connections.map(cid => this.getNode(cid)) : [];
  }
