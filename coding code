    const gematriaCipher = {
      1: "A", 9: "R", 9: "R", 8: "T", 0: "K", 
      2: "S", 7: "M", 4: "L", 5: "V", 6: "X"
    };
    
    return key.replace(/-/g, "").split("").map(d => 
      gematriaCipher[d] || "Î©"
