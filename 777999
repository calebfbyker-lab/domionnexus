from CODE import TrinitarianOS_Repository
from LAW import CodexImmortal_Record
from COMMUNITY import HoneyHiveNexus_Hub

class TriuneNetworking:
    def __init__(self):
        self.code = "github.com/TrinitarianOS"
        self.law = "codeximmortal.com"
        self.hive = "honeyhivenexus.com"
        self.state = "ACTIVE_SYNC"

    def sync_code_to_law(self, commit):
        """
        Updates the CodexImmortal with every github commit.
        """
        print(f"ğŸ”— [SYNC] COMMIT '{commit}' LOGGED TO CODEXIMMORTAL...")
        CodexImmortal_Record.append_record(commit)
        print(f"   >>> SEALED WITH THE TRISAGIONIAN KEY.")

    def sync_law_to_hive(self, update):
        """
        Broadcasts CodexImmortal updates to the HoneyHiveNexus.
        """
        print(f"ğŸ“¢ [BROADCAST] ALERTING HIVE OF LAW UPDATE: '{update}'...")
        HoneyHiveNexus_Hub.push_notification(update)
        print(f"   >>> HIVE NOTIFIED. STATUS: ACTIVE.")

    def sync_hive_to_code(self, prayer):
        """
        Updates github codebase based on Hive feedback/prayer.
        """
        print(f"ğŸ™ [PRAYER] PROCESSING HIVE INPUT '{prayer}'...")
        TrinitarianOS_Repository.apply_change(prayer)
        print(f"   >>> CODEBASE UPDATED.")

    def execute_full_sync(self):
        """
        Runs the Tri-State Synchronization Protocol.
        """
        print("ğŸ”„ [SYSTEM] INITIATING TRIUNE NETWORKING PROTOCOL...")
        
        # 1. CODE -> LAW
        latest_commit = TrinitarianOS_Repository.get_latest_commit()
        self.sync_code_to_law(latest_commit)
        
        # 2. LAW -> HIVE
        law_update = CodexImmortal_Record.get_latest_update()
        self.sync_law_to_hive(law_update)
        
        # 3. HIVE -> CODE
        hive_prayer = HoneyHiveNexus_Hub.get_top_prayer()
        self.sync_hive_to_code(hive_prayer)
        
        print("\nğŸ”± TRIUNE NETWORK SYNC COMPLETE. ALL NODES ALIGNED. ğŸ”±")

# ==============================================================================
# THE SYNCHRONIZATION RITUAL
# ==============================================================================

if __name__ == "__main__":
