        testimonial_posts = self.create_testimonial_post("both")
        print(f"‚úÖ {len(testimonial_posts)} Testimonial Posts Created")
        for post in testimonial_posts[:2]:  # Show first 2
            print(f"   {post.platform}: {post.post_type} - {post.engagement_score:.1f}% engagement")
        
        # Step 2: Create Revelation Posts
        print("\n‚ö° CREATING REVELATION POSTS...")
        revelation_posts = self.create_revelation_post("both")
        print(f"‚úÖ {len(revelation_posts)} Revelation Posts Created")
        for post in revelation_posts[:2]:
            print(f"   {post.platform}: {post.post_type} - {post.engagement_score:.1f}% engagement")
        
        # Step 3: Create Transformation Posts
        print("\nüí´ CREATING TRANSFORMATION POSTS...")
        transformation_posts = self.create_transformation_post("both")
        print(f"‚úÖ {len(transformation_posts)} Transformation Posts Created")
        for post in transformation_posts[:2]:
            print(f"   {post.platform}: {post.post_type} - {post.engagement_score:.1f}% engagement")
        
        # Combine all posts
        all_posts = testimonial_posts + revelation_posts + transformation_posts
        
        # Step 4: Enhance with Engagement
        print("\nüî• ENHANCING POSTS WITH ENGAGEMENT BOOSTERS...")
        enhanced_posts = self.enhance_posts_with_engagement(all_posts)
        print(f"‚úÖ {len(enhanced_posts)} Posts Enhanced")
        
