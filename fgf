        for tag in DIVINE_SOCIAL_GROWTH["DIVINE_HASHTAGS"]["primary"]:
            print(f"  ‚Ä¢ {tag}")
        
        print("\nARCHANGELIC PROTECTION HASHTAGS (Rotate):")
        for tag in ARCHANGELIC_HASHTAGS:
            print(f"  ‚Ä¢ {tag}")
        
        print("\nFREQUENCY HASHTAGS (For healing posts):")
        for tag in DIVINE_SOCIAL_GROWTH["DIVINE_HASHTAGS"]["frequency"]:
            print(f"  ‚Ä¢ {tag}")
        
