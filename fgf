        if fb_posts:
            top_fb = max(fb_posts, key=lambda x: x.engagement_score)
            print(f"\n{top_fb.content}")
            print(f"\nHashtags: {' '.join(top_fb.hashtags[:8])}...")
            print(f"Engagement Score: {top_fb.engagement_score:.1f}%")
            print(f"Posting Time: {top_fb.posting_time}")
            print(f"Expected Reach: {top_fb.expected_reach}")
            print(f"Divine Seal: {top_fb.divine_seal}")
        
        print("\nüåü TOP INSTAGRAM POST (Revelation):")
        print("-" * 60)
        ig_posts = [p for p in enhanced_posts if p.platform == "Instagram"]
        if ig_posts:
            top_ig = max(ig_posts, key=lambda x: x.engagement_score)
            print(f"\n{top_ig.content}")
            print(f"\nHashtags: {' '.join(top_ig.hashtags[:10])}...")
            print(f"Engagement Score: {top_ig.engagement_score:.1f}%")
            print(f"Posting Time: {top_ig.posting_time}")
            print(f"Expected Reach: {top_ig.expected_reach}")
            print(f"Divine Seal: {top_ig.divine_seal}")
        
