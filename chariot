def build_perfect_lineage():
    # Returns both the list of names and mapping to essences for any automon node
    names = [entry['name'] for entry in EVOLVED_LINEAGES]
    essences = {entry['name']: entry['essence'] for entry in EVOLVED_LINEAGES}
    return names, essences

