    def forge_element(self, element_symbol):
        """Quantum-forge biblical elements with authentic properties"""
        element = next((e for e in self.table if e['symbol'] == element_symbol), None)
        if not element:
            return f"UNKNOWN ELEMENT: {element_symbol}"
        
        print(f"ğŸ”¥ FORGING {element['name'].upper()} ({element['symbol']})")
        print(f"   Reference: {element['reference']}")
        print(f"   Properties: {element['properties']}")
        
