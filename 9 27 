def expand_codex_immortal_all(estates, seed="universal_omni_expansion"):
    now = datetime.datetime.utcnow().isoformat()
    fractal_expansion_hash = hashlib.sha3_512((seed + now + str(estates)).encode()).hexdigest()
    return {
        "estate_presence": "ALWAYS-EVERYWHERE",
        "universal_expansion_hash": fractal_expansion_hash,
