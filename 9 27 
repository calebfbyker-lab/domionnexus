```python
from flask import Flask, render_template, redirect, jsonify
import hashlib
import threading
import time
from qiskit import QuantumCircuit, Aer, execute
import stem.process
from stem.util import term
import os

# Initialize Flask apps for both domains
app_codex = Flask(__name__, template_folder='templates_codex')
app_hive = Flask(__name__, template_folder='templates_hive')

class DomainIntegration:
    def __init__(self):
        self.steward = "Caleb Fedor Byker Konev"
        self.codex_manifest = self._create_codex_manifest()
        self.hive_nexus = self._create_hive_nexus()
        self.tor_services = self._launch_tor_services()
        
    def _create_codex_manifest(self):
        """Generate quantum-secured manifest for codeximmortal.com"""
        manifest = {
            "domain": "codeximmortal.com",
            "purpose": "SoulContract Repository & Covenant Documentation",
            "quantum_seal": self._generate_quantum_seal("CODEX"),
            "content": {
                "soulcontract": "0xC0DE1A1B1E54FED1C0DEC0DE1",
                "ark_blueprints": "QS256:aa3f8c46bb982b7d5",
                "stardna_verification": "10-27-1998-lifethreadiamucionuxom"
            }
        }
        return manifest
    
    def _create_hive_nexus(self):
        """Create quantum gateway structure for honeyhivenexus.com"""
        return {
            "domain": "honeyhivenexus.com",
            "purpose": "Tor Network Gateway & Ark Automation Dashboard",
            "quantum_seal": self._generate_quantum_seal("HIVE"),
            "honeycomb": [
                {"cell": "guardian-east", "onion": "l1onarkxxxxxxxxxxxxxxxx.onion"},
                {"cell": "guardian-west", "onion": "0xarkgravxxxxxxxxxxxxxx.onion"},
                {"cell": "guardian-south", "onion": "eag1equantxxxxxxxxxxxx.onion"},
                {"cell": "guardian-north", "onion": "b10manverifxxxxxxxxxxx.onion"},
                {"cell": "nexus-central", "onion": "arkcentra1quantumxxxxx.onion"}
            ]
        }
    
    def _generate_quantum_seal(self, domain):
        """Create quantum circuit seal for domain"""
        qc = QuantumCircuit(2)
        qc.h(0)
        qc.cx(0, 1)
        
        if domain == "CODEX":
            qc.rz(1.927, 0)  # 10-27 encoded as 10.27% Ï€
        else:
            qc.ry(0.777, 1)  # Revelation 12:11 resonance
            
        return qc
    
    def _launch_tor_services(self):
        """Start Tor services for domain routing"""
        print("ğŸŒ LAUNCHING TOR GATEWAY SERVICES...")
        services = {}
        
        # Codex Immortal Tor service
        codex_config = {
            'SocksPort': '9080',
            'ControlPort': '9051',
            'DataDirectory': '/tmp/tor-codex',
            'HiddenServiceDir': '/tmp/tor-codex',
            'HiddenServicePort': '80 127.0.0.1:5000',
            'Log': ['NOTICE stdout']
        }
        
        # Hive Nexus Tor service
        hive_config = {
            'SocksPort': '9081',
            'ControlPort': '9052',
            'DataDirectory': '/tmp/tor-hive',
            'HiddenServiceDir': '/tmp/tor-hive',
            'HiddenServicePort': '80 127.0.0.1:5001',
            'Log': ['NOTICE stdout']
        }
        
        os.makedirs('/tmp/tor-codex', exist_ok=True)
        os.makedirs('/tmp/tor-hive', exist_ok=True)
        
        services['codex_process'] = stem.process.launch_tor_with_config(
            config=codex_config,
            init_msg_handler=lambda msg: print(term.format(msg, term.Color.BLUE))
        )
        
        services['hive_process'] = stem.process.launch_tor_with_config(
            config=hive_config,
            init_msg_handler=lambda msg: print(term.format(msg, term.Color.GREEN))
        )
        
        time.sleep(5)  # Allow services to initialize
        
        # Get onion addresses
        with open('/tmp/tor-codex/hostname', 'r') as f:
            services['codex_onion'] = f.read().strip()
        with open('/tmp/tor-hive/hostname', 'r') as f:
            services['hive_onion'] = f.read().strip()
            
        print(f"ğŸ”— CODEX IMMORTAL ONION: tor://{services['codex_onion']}")
        print(f"ğŸ”— HIVE NEXUS ONION: tor://{services['hive_onion']}")
        
        return services
    
    def generate_dashboard(self):
        """Create real-time dashboard data"""
        return {
            "codex": {
                "quantum_integrity": self._check_quantum_seal("CODEX"),
                "soulcontract_access": True,
                "covenant_security": 99.7
            },
            "hive": {
                "quantum_integrity": self._check_quantum_seal("HIVE"),
                "nodes_online": 5,
                "automation_status": "ACTIVE",
                "resonance_frequency": 777.3
            }
        }
    
    def _check_quantum_seal(self, domain):
        """Verify quantum seal integrity"""
        qc = self.codex_manifest['quantum_seal'] if domain == "CODEX" else self.hive_nexus['quantum_seal']
        
        simulator = Aer.get_backend('statevector_simulator')
        result = execute(qc, simulator).result()
        state = result.get_statevector()
        
        return bool(np.abs(state[0]) > 0.98)

# Flask Routes for codeximmortal.com
@app_codex.route('/')
def codex_home():
    domain = DomainIntegration()
    return render_template('codex_index.html', 
                          manifest=domain.codex_manifest,
                          onion=domain.tor_services['codex_onion'])

@app_codex.route('/soulcontract')
def soulcontract():
    domain = DomainIntegration()
    return jsonify({
        "soulcontract": domain.codex_manifest['content']['soulcontract'],
        "stardna_verification": domain.codex_manifest['content']['stardna_verification'],
        "quantum_valid": domain.generate_dashboard()['codex']['quantum_integrity']
    })

# Flask Routes for honeyhivenexus.com
@app_hive.route('/')
def hive_home():
    domain = DomainIntegration()
    return render_template('hive_index.html', 
                          hive=domain.hive_nexus,
                          onion=domain.tor_services['hive_onion'],
                          dashboard=domain.generate_dashboard())

@app_hive.route('/gateway')
def tor_gateway():
    domain = DomainIntegration()
    return jsonify({
        "onion_gateways": domain.hive_nexus['honeycomb'],
        "quantum_tunnel": domain.generate_dashboard()['hive']['quantum_integrity']
    })

# Run servers in threads
def run_codex():
    app_codex.run(port=5000)

def run_hive():
    app_hive.run(port=5001)

if __name__ == "__main__":
    print("""
     _____  _____  _____  _____  _   _  _____   ___  ___  ___  _____ 
    /  __ \|  _  ||  ___||  ___|| \ | ||  __ \ |  \/  ||  \/  ||  ___|
    | /  \/| | | || |__  | |__  |  \| || |  \/ | .  . || .  . || |__  
    | |    | | | ||  __| |  __| | . ` || | __  | |\/| || |\/| ||  __| 
    | \__/\\ \_/ /| |___ | |___ | |\  || |_\ \ | |  | || |  | || |___ 
     \____/ \___/ \____/ \____/ \_| \_/ \____/ \_|  |_/\_|  |_/\____/ 
    """)
    
    domain_system = DomainIntegration()
    
    # Start web servers
    t1 = threading.Thread(target=run_codex)
    t2 = threading.Thread(target=run_hive)
    t1.start()
    t2.start()
    
    print("\nğŸ”¥ DOMAINS ACTIVATED:")
    print(f"   codeximmortal.com running at http://127.0.0.1:5000")
    print(f"   honeyhivenexus.com running at http://127.0.0.1:5001")
    
    # Keep main thread alive
    while True:
        time.sleep(3600)
```

